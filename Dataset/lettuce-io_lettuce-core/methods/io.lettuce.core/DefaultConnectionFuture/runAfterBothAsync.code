@Override public DefaultConnectionFuture<Void> runAfterBothAsync(CompletionStage<?> other,Runnable action){
  return adopt(delegate.runAfterBothAsync(other,action));
}
@Override public DefaultConnectionFuture<Void> runAfterBothAsync(CompletionStage<?> other,Runnable action,Executor executor){
  return adopt(delegate.runAfterBothAsync(other,action,executor));
}
