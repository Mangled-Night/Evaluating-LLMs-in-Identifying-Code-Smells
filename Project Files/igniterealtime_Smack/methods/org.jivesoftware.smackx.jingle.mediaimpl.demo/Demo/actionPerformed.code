public void actionPerformed(ActionEvent e){
  if (outgoing != null)   return;
  try {
    outgoing=jm.createOutgoingJingleSession(jid.getText());
    outgoing.startOutgoing();
  }
 catch (  XMPPException e1) {
    e1.printStackTrace();
  }
}
public void actionPerformed(ActionEvent e){
  if (outgoing != null)   try {
    outgoing.terminate();
  }
 catch (  XMPPException e1) {
    e1.printStackTrace();
  }
 finally {
    outgoing=null;
  }
  if (incoming != null)   try {
    incoming.terminate();
  }
 catch (  XMPPException e1) {
    e1.printStackTrace();
  }
 finally {
    incoming=null;
  }
}
