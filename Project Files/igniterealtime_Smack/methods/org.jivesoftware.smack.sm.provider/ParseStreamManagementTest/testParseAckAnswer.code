@Test public void testParseAckAnswer() throws Exception {
  long handledPackets=42 + 42;
  String ackStanza=XMLBuilder.create("a").a("xmlns","urn:xmpp:sm:3").a("h",String.valueOf(handledPackets)).asString(outputProperties);
  StreamManagement.AckAnswer acknowledgementPacket=ParseStreamManagement.ackAnswer(PacketParserUtils.getParserFor(ackStanza));
  assertNotNull(acknowledgementPacket);
  assertEquals(handledPackets,acknowledgementPacket.getHandledCount());
}
