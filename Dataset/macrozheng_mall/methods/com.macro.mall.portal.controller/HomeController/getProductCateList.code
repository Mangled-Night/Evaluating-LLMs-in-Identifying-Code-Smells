@ApiOperation("获取首页商品分类") @RequestMapping(value="/productCateList/{parentId}",method=RequestMethod.GET) @ResponseBody public CommonResult<List<PmsProductCategory>> getProductCateList(@PathVariable Long parentId){
  List<PmsProductCategory> productCategoryList=homeService.getProductCateList(parentId);
  return CommonResult.success(productCategoryList);
}
