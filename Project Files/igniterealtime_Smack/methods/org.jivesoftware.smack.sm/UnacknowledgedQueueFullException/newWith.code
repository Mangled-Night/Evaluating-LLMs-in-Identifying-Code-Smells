public static UnacknowledgedQueueFullException newWith(int overflowElementNum,List<Element> elements,BlockingQueue<Stanza> unacknowledgedStanzas){
  final int unacknowledgesStanzasQueueSize=unacknowledgedStanzas.size();
  List<Stanza> localUnacknowledgesStanzas=new ArrayList<>(unacknowledgesStanzasQueueSize);
  localUnacknowledgesStanzas.addAll(unacknowledgedStanzas);
  int droppedElements=elements.size() - overflowElementNum - 1;
  String message="The queue size " + unacknowledgesStanzasQueueSize + " is not able to fit another "+ droppedElements+ " potential stanzas type top-level stream-elements.";
  return new UnacknowledgedQueueFullException(message,overflowElementNum,droppedElements,elements,localUnacknowledgesStanzas);
}
