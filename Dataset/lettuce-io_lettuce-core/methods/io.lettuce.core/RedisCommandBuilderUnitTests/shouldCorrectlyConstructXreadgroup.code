@Test void shouldCorrectlyConstructXreadgroup(){
  Command<String,String,?> command=sut.xreadgroup(Consumer.from("a","b"),new XReadArgs(),XReadArgs.StreamOffset.latest("stream"));
  assertThat(Unpooled.wrappedBuffer(command.getArgs().getFirstEncodedKey()).toString(StandardCharsets.UTF_8)).isEqualTo("stream");
}
