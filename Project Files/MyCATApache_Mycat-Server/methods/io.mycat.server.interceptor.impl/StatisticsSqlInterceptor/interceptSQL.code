/** 
 * interceptSQL , type :insert,delete,update,select exectime:xxx ms log content : select:select 1 from table,exectime:100ms,shared:1 etc
 */
@Override public String interceptSQL(String sql,int sqlType){
  LOGGER.debug("sql interceptSQL:");
  final int sqltype=sqlType;
  final String sqls=DefaultSqlInterceptor.processEscape(sql);
  MycatServer.getInstance().getBusinessExecutor().execute(new StatisticsSqlRunner(sqltype,sqls));
  return sql;
}
