@Override public void set(ByteBuffer bytes){
  if (key == null) {
    key=(bytes == null) ? null : codec.decodeKey(bytes);
    return;
  }
  V value=(bytes == null) ? null : codec.decodeValue(bytes);
  output.put(key,value);
  key=null;
}
@Override @SuppressWarnings("unchecked") public void set(long integer){
  if (key == null) {
    key=(K)Long.valueOf(integer);
    return;
  }
  V value=(V)Long.valueOf(integer);
  output.put(key,value);
  key=null;
}
