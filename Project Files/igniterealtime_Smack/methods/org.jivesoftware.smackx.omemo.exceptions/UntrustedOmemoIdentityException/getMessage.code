@Override public String getMessage(){
  if (trustedKey != null) {
    return "Untrusted OMEMO Identity encountered:\n" + "Fingerprint of trusted key:\n" + trustedKey.blocksOf8Chars() + "\n"+ "Fingerprint of untrusted key:\n"+ untrustedKey.blocksOf8Chars();
  }
 else {
    return "Untrusted OMEMO Identity encountered:\n" + "Fingerprint of untrusted key:\n" + untrustedKey.blocksOf8Chars();
  }
}
