public <K,V>void build(CommandArgs<K,V> args){
  if (withdistance) {
    args.add("WITHDIST");
  }
  if (withhash) {
    args.add("WITHHASH");
  }
  if (withcoordinates) {
    args.add("WITHCOORD");
  }
  if (sort != null && sort != Sort.none) {
    args.add(sort.name());
  }
  if (count != null) {
    args.add(CommandKeyword.COUNT).add(count);
    if (any) {
      args.add("ANY");
    }
  }
}
