private static void testSingleNodeNormalSQL(Connection theCon) throws SQLException {
  theCon.setAutoCommit(true);
  System.out.println("testSingleNodeNormalSQL begin");
  String[] sqls={"select * from company limit 100","select name,count(id),max(id),min(id) from company group by name order by name desc limit 100 ","update company set name=name where id =1","delete from company where id =-1 ","delete from company where id =1 ","insert into company(id,name) values(1,'hp')"};
  Statement stmt=theCon.createStatement();
  for (  String sql : sqls) {
    System.out.println("execute " + sql);
    stmt.execute(sql);
  }
  theCon.setAutoCommit(false);
  for (  String sql : sqls) {
    stmt.execute(sql);
  }
  theCon.commit();
  System.out.println("testSingleNodeNormalSQL end");
}
