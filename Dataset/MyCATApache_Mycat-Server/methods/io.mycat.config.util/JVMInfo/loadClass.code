public Class<?> loadClass(String name){
  try {
    Class<?> clazz=loaderCache.get(name);
    if (clazz == null) {
      clazz=Class.forName(name,false,getClass().getClassLoader());
      loaderCache.put(name,clazz);
    }
    return clazz;
  }
 catch (  ClassNotFoundException e) {
    return null;
  }
}
