@ApiOperation("添加品牌关注") @RequestMapping(value="/add",method=RequestMethod.POST) @ResponseBody public CommonResult add(@RequestBody MemberBrandAttention memberBrandAttention){
  int count=memberAttentionService.add(memberBrandAttention);
  if (count > 0) {
    return CommonResult.success(count);
  }
 else {
    return CommonResult.failed();
  }
}
