@Test public void test(){
  ArthasEnvironment arthasEnvironment=new ArthasEnvironment();
  Assertions.assertThat(arthasEnvironment.resolvePlaceholders("hello, ${java.version}")).isEqualTo("hello, " + System.getProperty("java.version"));
  Assertions.assertThat(arthasEnvironment.resolvePlaceholders("hello, ${xxxxxxxxxxxxxxx}")).isEqualTo("hello, ${xxxxxxxxxxxxxxx}");
  System.setProperty("xxxxxxxxxxxxxxx","vvv");
  Assertions.assertThat(arthasEnvironment.resolvePlaceholders("hello, ${xxxxxxxxxxxxxxx}")).isEqualTo("hello, vvv");
  System.clearProperty("xxxxxxxxxxxxxxx");
}
