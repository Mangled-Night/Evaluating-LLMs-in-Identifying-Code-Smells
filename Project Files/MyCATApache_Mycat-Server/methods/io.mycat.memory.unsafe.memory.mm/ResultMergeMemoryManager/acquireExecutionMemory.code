@Override protected synchronized long acquireExecutionMemory(long numBytes,long taskAttemptId,MemoryMode memoryMode) throws InterruptedException {
switch (memoryMode) {
case ON_HEAP:
    return onHeapExecutionMemoryPool.acquireMemory(numBytes,taskAttemptId);
case OFF_HEAP:
  return offHeapExecutionMemoryPool.acquireMemory(numBytes,taskAttemptId);
}
return 0L;
}
