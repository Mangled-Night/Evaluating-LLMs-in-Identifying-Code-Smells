private static class TestMessageListener implements ChatMessageListener {
  private Chat msgChat;
  private int counter=0;
  @Override public void processMessage(  Chat chat,  Message message){
    msgChat=chat;
    counter++;
  }
  @SuppressWarnings("unused") public Chat getChat(){
    return msgChat;
  }
  public int getNumMessages(){
    return counter;
  }
}
