@Override public <U>DefaultConnectionFuture<U> handleAsync(BiFunction<? super T,Throwable,? extends U> fn){
  return adopt(delegate.handleAsync(fn));
}
@Override public <U>DefaultConnectionFuture<U> handleAsync(BiFunction<? super T,Throwable,? extends U> fn,Executor executor){
  return adopt(delegate.handleAsync(fn,executor));
}
