@Test public void testSplitMethodInfo() throws Throwable {
  Assertions.assertThat(StringUtils.splitMethodInfo("a|b")).containsExactly("a","b");
  Assertions.assertThat(StringUtils.splitMethodInfo("xxxxxxxxxx|fffffffffff")).containsExactly("xxxxxxxxxx","fffffffffff");
  Assertions.assertThat(StringUtils.splitMethodInfo("print|(ILjava/util/List;)V")).containsExactly("print","(ILjava/util/List;)V");
}
