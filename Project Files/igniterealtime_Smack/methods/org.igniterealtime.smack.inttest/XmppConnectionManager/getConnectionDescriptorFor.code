@SuppressWarnings("unchecked") public <C extends AbstractXMPPConnection>XmppConnectionDescriptor<C,? extends ConnectionConfiguration,? extends ConnectionConfiguration.Builder<?,?>> getConnectionDescriptorFor(Class<C> connectionClass){
  return (XmppConnectionDescriptor<C,? extends ConnectionConfiguration,? extends ConnectionConfiguration.Builder<?,?>>)connectionDescriptors.getFirst(connectionClass);
}
