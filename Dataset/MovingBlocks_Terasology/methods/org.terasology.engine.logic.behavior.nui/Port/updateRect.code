public abstract void updateRect();
@Override public void updateRect(){
  this.rect=new Rectanglef(index() + 0.4f,4.05f).setSize(0.6f,0.9f);
}
@Override public void updateRect(){
  this.rect=new Rectanglef(index(),4.05f).setSize(0.4f,0.9f);
}
@Override public void updateRect(){
  rect=new Rectanglef(4.5f,0.05f).setSize(1f,1f);
}
