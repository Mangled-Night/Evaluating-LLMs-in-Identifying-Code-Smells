@Override public void storeSession(SignalProtocolAddress signalProtocolAddress,SessionRecord sessionRecord){
  OmemoDevice device;
  try {
    device=asOmemoDevice(signalProtocolAddress);
  }
 catch (  XmppStringprepException e) {
    throw new AssertionError(e);
  }
  try {
    omemoStore.storeRawSession(getOurDevice(),device,sessionRecord);
  }
 catch (  IOException e) {
    throw new IllegalStateException(e);
  }
}
