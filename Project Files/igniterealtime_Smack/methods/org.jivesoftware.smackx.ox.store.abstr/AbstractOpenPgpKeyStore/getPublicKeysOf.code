@Override public PGPPublicKeyRingCollection getPublicKeysOf(BareJid owner) throws IOException, PGPException {
  PGPPublicKeyRingCollection keys=publicKeyRingCollections.get(owner);
  if (keys == null) {
    keys=readPublicKeysOf(owner);
    if (keys != null) {
      publicKeyRingCollections.put(owner,keys);
    }
  }
  return keys;
}
