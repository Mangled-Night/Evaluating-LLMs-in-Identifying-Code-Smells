@ApiOperation("分页获取新品推荐商品") @RequestMapping(value="/newProductList",method=RequestMethod.GET) @ResponseBody public CommonResult<List<PmsProduct>> newProductList(@RequestParam(value="pageNum",defaultValue="1") Integer pageNum,@RequestParam(value="pageSize",defaultValue="6") Integer pageSize){
  List<PmsProduct> productList=homeService.newProductList(pageNum,pageSize);
  return CommonResult.success(productList);
}
