@Test public void testAllUnbounded(){
  initConstraint(null,null,false,false);
  assertTrue(constraint.isSatisfiedBy(1000));
  assertTrue(constraint.isSatisfiedBy(50000));
  assertTrue(constraint.isSatisfiedBy(50));
  assertTrue(constraint.isSatisfiedBy(-1000));
