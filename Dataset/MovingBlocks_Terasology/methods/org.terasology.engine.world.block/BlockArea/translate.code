@Override public BlockArea translate(int dx,int dy,BlockArea dest){
  dest.minX=minX + dx;
  dest.minY=minY + dy;
  dest.maxX=maxX + dx;
  dest.maxY=maxY + dy;
  return dest;
}
public BlockArea translate(int dx,int dy){
  return translate(dx,dy,this);
}
public BlockArea translate(Vector2ic dv){
  return translate(dv.x(),dv.y(),this);
}
