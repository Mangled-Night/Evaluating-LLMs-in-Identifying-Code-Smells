/** 
 * Bytes to Serializable.
 */
public static Object bytes2Object(final byte[] bytes){
  if (bytes == null)   return null;
  ObjectInputStream ois=null;
  try {
    ois=new ObjectInputStream(new ByteArrayInputStream(bytes));
    return ois.readObject();
  }
 catch (  Exception e) {
    e.printStackTrace();
    return null;
  }
 finally {
    try {
      if (ois != null) {
        ois.close();
      }
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
  }
}
