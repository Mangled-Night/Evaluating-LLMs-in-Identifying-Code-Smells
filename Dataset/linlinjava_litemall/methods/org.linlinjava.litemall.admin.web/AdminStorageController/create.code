@RequiresPermissions("admin:storage:create") @RequiresPermissionsDesc(menu={"系统管理","对象存储"},button="上传") @PostMapping("/create") public Object create(@RequestParam("file") MultipartFile file) throws IOException {
  String originalFilename=file.getOriginalFilename();
  LitemallStorage litemallStorage=storageService.store(file.getInputStream(),file.getSize(),file.getContentType(),originalFilename);
  return ResponseUtil.ok(litemallStorage);
}
