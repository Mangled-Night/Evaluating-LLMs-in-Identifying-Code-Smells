private static SelectJob createQueryJob(Connection con){
  SelectJob job=null;
  if (sqlTemplateItems != null) {
    job=new UserTableSelectJob(con,sqlTemplateItems,executeTimes,finshiedCount,failedCount);
  }
 else {
    job=new TravelRecordSelectJob(con,minId,maxId,executeTimes,finshiedCount,failedCount);
  }
  return job;
}
