private boolean check(){
  int r=0;
  for (  int v : post()) {
    if (post(v) != r) {
      StdOut.println("post(v) and post() inconsistent");
      return false;
    }
    r++;
  }
  r=0;
  for (  int v : pre()) {
    if (pre(v) != r) {
      StdOut.println("pre(v) and pre() inconsistent");
      return false;
    }
    r++;
  }
  return true;
}
