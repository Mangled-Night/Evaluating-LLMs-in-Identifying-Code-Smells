/** 
 * 64-bit integer output, may be null.
 * @param < K > Key type.
 * @param < V > Value type.
 * @author Will Glozer
 * @author Mark Paluch
 */
public class IntegerOutput<K,V> extends CommandOutput<K,V,Long> {
  public IntegerOutput(  RedisCodec<K,V> codec){
    super(codec,null);
  }
  @Override public void set(  long integer){
    output=integer;
  }
  @Override public void set(  ByteBuffer bytes){
    if (bytes == null) {
      output=null;
    }
 else {
      output=Long.parseLong(StandardCharsets.UTF_8.decode(bytes).toString());
    }
  }
}
