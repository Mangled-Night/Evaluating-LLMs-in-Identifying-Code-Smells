/** 
 * 后端连接ID生成器
 * @author mycat
 */
private static class ConnectIdGenerator {
  private static final long MAX_VALUE=Long.MAX_VALUE;
  private AtomicLong connectId=new AtomicLong(0);
  private long getId(){
    return connectId.incrementAndGet();
  }
}
