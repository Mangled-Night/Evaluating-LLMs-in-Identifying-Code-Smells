@ParameterizedTest @EnumSource(SmackTestUtil.XmlPullParserKind.class) public void testAttributesElementWithOneAttribute(SmackTestUtil.XmlPullParserKind parserKind) throws XmlPullParserException, IOException {
  String elementWithOneAttribute="<element attribute-one='attribute-one-value'/>";
  XmlPullParser parser=SmackTestUtil.getParserFor(elementWithOneAttribute,parserKind);
  assertAttributeHolds(parser,0,"attribute-one","","");
  assertThrows(NullPointerException.class,() -> assertAttributeHolds(parser,1,"attribute-one","",""));
}
