private void setError(PostgreSQLDetector detector){
  if (this.errorCount.incrementAndGet() < maxRetryCount) {
    if (detector != null && !detector.isQuit()) {
      heartbeat();
    }
  }
 else {
    if (detector != null) {
      detector.quit();
    }
    this.status=ERROR_STATUS;
    this.errorCount.set(0);
    ;
  }
}
