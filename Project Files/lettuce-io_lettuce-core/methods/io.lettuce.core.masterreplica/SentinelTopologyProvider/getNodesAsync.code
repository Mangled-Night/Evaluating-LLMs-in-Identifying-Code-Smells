@Override public CompletableFuture<List<RedisNodeDescription>> getNodesAsync(){
  logger.debug("lookup topology for masterId {}",masterId);
  Mono<StatefulRedisSentinelConnection<String,String>> connect=Mono.fromFuture(redisClient.connectSentinelAsync(StringCodec.UTF8,sentinelUri));
  return connect.flatMap(this::getNodes).toFuture();
}
