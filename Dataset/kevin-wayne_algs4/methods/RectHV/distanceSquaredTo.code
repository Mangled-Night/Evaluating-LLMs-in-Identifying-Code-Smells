/** 
 * Returns the square of the Euclidean distance between this rectangle and the point  {@code p}.
 * @param p the point
 * @return the square of the Euclidean distance between the point {@code p} andthe closest point on this rectangle; 0 if the point is contained in this rectangle
 */
public double distanceSquaredTo(Point2D p){
  double dx=0.0, dy=0.0;
  if (p.x() < xmin)   dx=p.x() - xmin;
 else   if (p.x() > xmax)   dx=p.x() - xmax;
  if (p.y() < ymin)   dy=p.y() - ymin;
 else   if (p.y() > ymax)   dy=p.y() - ymax;
  return dx * dx + dy * dy;
}
