/** 
 * 将Map对应的col字段集，返回row中对应的index数组
 * @param columns
 * @param toIndexMap
 * @return
 */
protected static int[] toColumnIndex(String[] columns,Map<String,ColMeta> toIndexMap){
  int[] result=new int[columns.length];
  ColMeta curColMeta;
  for (int i=0; i < columns.length; i++) {
    curColMeta=toIndexMap.get(StringUtil.removeBackquote(columns[i]).toUpperCase());
    if (curColMeta == null) {
      throw new IllegalArgumentException("all columns in group by clause should be in the selected column list.!" + columns[i]);
    }
    result[i]=curColMeta.colIndex;
  }
  return result;
}
