/** 
 * Leaves the current game and returns to main menu
 * @return String containing final message
 */
@Command(shortDescription="Leaves the current game and returns to main menu",requiredPermission=PermissionManager.NO_PERMISSION) public String leave(){
  if (networkSystem.getMode() != NetworkMode.NONE) {
    gameEngine.changeState(new StateMainMenu());
    return "Leaving..";
  }
 else {
    return "Not connected";
  }
}
