public Macro getMacroByTitle(String title){
  Collection<Macro> col=Collections.unmodifiableList(macros);
  Iterator<Macro> iter=col.iterator();
  while (iter.hasNext()) {
    Macro macro=iter.next();
    if (macro.getTitle().equalsIgnoreCase(title)) {
      return macro;
    }
  }
  return null;
}
