@Override public String[] getParameterNames(Method method){
  if (method.getParameterCount() == 0) {
    return new String[0];
  }
  return doGetParameterNames(method.getParameterAnnotations());
}
@Override public String[] getParameterNames(Constructor<?> ctor){
  if (ctor.getParameterCount() == 0) {
    return new String[0];
  }
  return doGetParameterNames(ctor.getParameterAnnotations());
}
