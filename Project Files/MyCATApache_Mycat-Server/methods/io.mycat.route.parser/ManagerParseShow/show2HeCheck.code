static int show2HeCheck(String stmt,int offset){
  if (stmt.length() > ++offset) {
switch (stmt.charAt(offset)) {
case 'L':
case 'l':
      return show2HelCheck(stmt,offset);
case 'A':
case 'a':
    return ManagerParseHeartbeat.show2HeaCheck(stmt,offset);
default :
  return OTHER;
}
}
return OTHER;
}
