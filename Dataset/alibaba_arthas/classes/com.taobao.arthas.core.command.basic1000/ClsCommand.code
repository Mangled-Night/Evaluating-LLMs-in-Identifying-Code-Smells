@Name("cls") @Summary("Clear the screen") public class ClsCommand extends AnnotatedCommand {
  @Override public void process(  CommandProcess process){
    if (!process.session().isTty()) {
      process.end(-1,"Command 'cls' is only support tty session.");
      return;
    }
    process.write(RenderUtil.cls()).write("\n");
    process.end();
  }
}
