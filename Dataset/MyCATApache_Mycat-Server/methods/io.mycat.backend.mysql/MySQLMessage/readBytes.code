public byte[] readBytes(){
  if (position >= length) {
    return EMPTY_BYTES;
  }
  byte[] ab=new byte[length - position];
  System.arraycopy(data,position,ab,0,ab.length);
  position=length;
  return ab;
}
public byte[] readBytes(int length){
  byte[] ab=new byte[length];
  System.arraycopy(data,position,ab,0,length);
  position+=length;
  return ab;
}
