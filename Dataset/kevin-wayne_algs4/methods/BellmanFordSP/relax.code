private void relax(EdgeWeightedDigraph G,int v){
  for (  DirectedEdge e : G.adj(v)) {
    int w=e.to();
    if (distTo[w] > distTo[v] + e.weight() + EPSILON) {
      distTo[w]=distTo[v] + e.weight();
      edgeTo[w]=e;
      if (!onQueue[w]) {
        queue.enqueue(w);
        onQueue[w]=true;
      }
    }
    if (++cost % G.V() == 0) {
      findNegativeCycle();
      if (hasNegativeCycle())       return;
    }
  }
}
