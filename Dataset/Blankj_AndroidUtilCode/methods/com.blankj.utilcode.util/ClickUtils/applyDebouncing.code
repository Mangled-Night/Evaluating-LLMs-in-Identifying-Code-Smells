private static void applyDebouncing(final View[] views,final boolean isGlobal,@IntRange(from=0) long duration,final View.OnClickListener listener){
  if (views == null || views.length == 0 || listener == null)   return;
  for (  View view : views) {
    if (view == null)     continue;
    view.setOnClickListener(new OnDebouncingClickListener(isGlobal,duration){
      @Override public void onDebouncingClick(      View v){
        listener.onClick(v);
      }
    }
);
  }
}
