/** 
 * Returns a solution to the linear system of equations <em>Ax</em> = <em>b</em>.
 * @return a solution <em>x</em> to the linear system of equations<em>Ax</em> = <em>b</em>;  {@code null} if no such solution
 */
public double[] primal(){
  double[] x=new double[n];
  for (int i=Math.min(n - 1,m - 1); i >= 0; i--) {
    double sum=0.0;
    for (int j=i + 1; j < n; j++) {
      sum+=a[i][j] * x[j];
    }
    if (Math.abs(a[i][i]) > EPSILON)     x[i]=(a[i][n] - sum) / a[i][i];
 else     if (Math.abs(a[i][n] - sum) > EPSILON)     return null;
  }
  for (int i=n; i < m; i++) {
    double sum=0.0;
    for (int j=0; j < n; j++) {
      sum+=a[i][j] * x[j];
    }
    if (Math.abs(a[i][n] - sum) > EPSILON)     return null;
  }
  return x;
}
