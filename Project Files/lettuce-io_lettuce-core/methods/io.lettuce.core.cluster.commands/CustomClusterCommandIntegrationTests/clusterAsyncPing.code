@Test void clusterAsyncPing(){
  RedisCommand<String,String,String> command=new Command<>(CustomCommandIntegrationTests.MyCommands.PING,new StatusOutput<>(StringCodec.UTF8),null);
  AsyncCommand<String,String,String> async=new AsyncCommand<>(command);
  connection.dispatch(async);
  assertThat(TestFutures.getOrTimeout((RedisFuture)async)).isEqualTo("PONG");
}
