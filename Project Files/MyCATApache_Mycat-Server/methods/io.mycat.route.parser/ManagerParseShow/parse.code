public static int parse(String stmt,int offset){
  int i=offset;
  for (; i < stmt.length(); i++) {
switch (stmt.charAt(i)) {
case ' ':
      continue;
case '/':
case '#':
    i=ParseUtil.comment(stmt,i);
  continue;
case '@':
return show2Check(stmt,i);
case 'C':
case 'c':
return showCCheck(stmt,i);
case 'd':
case 'D':
return show2DCheck(stmt,i);
case 'V':
case 'v':
return showVCheck(stmt,i);
default :
return OTHER;
}
}
return OTHER;
}
