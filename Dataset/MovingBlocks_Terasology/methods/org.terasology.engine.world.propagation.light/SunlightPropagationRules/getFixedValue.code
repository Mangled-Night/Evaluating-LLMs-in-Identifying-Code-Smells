/** 
 * If the light is above the sunlight regeneration threshold it is maintained, otherwise it is zero <p> {@inheritDoc}
 */
@Override public byte getFixedValue(Block block,Vector3ic pos){
  byte lightVal=(byte)(regenWorldView.getValueAt(pos) - Chunks.SUNLIGHT_REGEN_THRESHOLD);
  return (lightVal > 0) ? lightVal : 0;
}
