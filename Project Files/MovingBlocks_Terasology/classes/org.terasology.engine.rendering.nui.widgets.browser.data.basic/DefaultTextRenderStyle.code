private static final class DefaultTextRenderStyle implements TextRenderStyle {
  private Font font;
  private Color color;
  private DefaultTextRenderStyle(  Font font,  Color color){
    this.font=font;
    this.color=color;
  }
  @Override public Font getFont(  boolean isHyperlink){
    return font;
  }
  @Override public Color getColor(  boolean isHyperlink){
    return color;
  }
}
