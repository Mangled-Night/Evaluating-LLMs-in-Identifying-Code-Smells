public static void main(String[] args){
  for (int i=0; i < 20; i++) {
    int y=9;
    int count=3;
    long slot=i % y;
    int slotSize=y / count;
    Long index=slot / slotSize;
    if (slotSize * count != y && index > count - 1) {
      index=index - 1;
    }
    System.out.println(slot + "   " + index);
  }
}
