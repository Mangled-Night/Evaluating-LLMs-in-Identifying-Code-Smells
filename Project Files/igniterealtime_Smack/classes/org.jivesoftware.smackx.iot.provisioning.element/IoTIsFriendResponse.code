public class IoTIsFriendResponse extends IQ {
  public static final String ELEMENT="isFriendResponse";
  public static final String NAMESPACE=Constants.IOT_PROVISIONING_NAMESPACE;
  private final BareJid jid;
  private final boolean result;
  public IoTIsFriendResponse(  BareJid jid,  boolean result){
    super(ELEMENT,NAMESPACE);
    this.jid=jid;
    this.result=result;
  }
  public BareJid getJid(){
    return jid;
  }
  public boolean getIsFriendResult(){
    return result;
  }
  @Override protected IQChildElementXmlStringBuilder getIQChildElementBuilder(  IQChildElementXmlStringBuilder xml){
    xml.attribute("jid",jid);
    xml.attribute("result",result);
    xml.setEmptyElement();
    return xml;
  }
}
