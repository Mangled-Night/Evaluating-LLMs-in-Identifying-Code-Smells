private static int characterSetC(String stmt,int offset){
  if (stmt.length() > offset + 1) {
    char c1=stmt.charAt(++offset);
switch (c1) {
case 'o':
case 'O':
      return characterSetConnection(stmt,offset);
case 'l':
case 'L':
    return characterSetClient(stmt,offset);
default :
  return OTHER;
}
}
return OTHER;
}
