@ReceiveEvent(components=BlockRegionComponent.class) public void onBlockRegionActivated(OnActivatedComponent event,EntityRef entity){
  BlockRegionComponent regionComp=entity.getComponent(BlockRegionComponent.class);
  blockRegions.put(entity,regionComp.region);
  for (  Vector3ic pos : regionComp.region) {
    blockRegionLookup.put(new Vector3i(pos),entity);
  }
}
