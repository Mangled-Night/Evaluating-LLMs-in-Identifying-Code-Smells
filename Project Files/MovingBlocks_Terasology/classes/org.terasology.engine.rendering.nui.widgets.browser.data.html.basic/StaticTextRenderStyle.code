private static final class StaticTextRenderStyle implements TextRenderStyle {
  private Font font;
  private Color color;
  private StaticTextRenderStyle(  Font font,  Color color){
    this.font=font;
    this.color=color;
  }
  @Override public Font getFont(  boolean hyperlink){
    return font;
  }
  @Override public Color getColor(  boolean hyperlink){
    return color;
  }
}
