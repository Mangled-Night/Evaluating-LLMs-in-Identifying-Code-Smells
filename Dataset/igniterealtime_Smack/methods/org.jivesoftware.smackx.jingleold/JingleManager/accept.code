@Override public boolean accept(Stanza pin){
  if (pin instanceof IQ) {
    IQ iq=(IQ)pin;
    if (iq.getType().equals(IQ.Type.set)) {
      if (iq instanceof Jingle) {
        Jingle jin=(Jingle)pin;
        if (jin.getAction().equals(JingleActionEnum.SESSION_INITIATE)) {
          return true;
        }
      }
    }
  }
  return false;
}
