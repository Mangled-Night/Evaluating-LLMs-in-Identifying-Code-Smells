@Override public OfflineMessageRequest parse(XmlPullParser parser,int initialDepth,IqData iqData,XmlEnvironment xmlEnvironment) throws XmlPullParserException, IOException {
  OfflineMessageRequest request=new OfflineMessageRequest();
  boolean done=false;
  while (!done) {
    XmlPullParser.Event eventType=parser.next();
    if (eventType == XmlPullParser.Event.START_ELEMENT) {
      if (parser.getName().equals("item")) {
        request.addItem(parseItem(parser));
      }
 else       if (parser.getName().equals("purge")) {
        request.setPurge(true);
      }
 else       if (parser.getName().equals("fetch")) {
        request.setFetch(true);
      }
    }
 else     if (eventType == XmlPullParser.Event.END_ELEMENT) {
      if (parser.getName().equals("offline")) {
        done=true;
      }
    }
  }
  return request;
}
