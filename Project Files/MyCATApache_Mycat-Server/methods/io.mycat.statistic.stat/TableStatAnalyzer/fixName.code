/** 
 * 去掉库名、去掉``
 * @param tableName
 * @return
 */
private String fixName(String tableName){
  if (tableName != null) {
    tableName=tableName.replace("`","");
    int dotIdx=tableName.indexOf(".");
    if (dotIdx > 0) {
      tableName=tableName.substring(1 + dotIdx).trim();
    }
  }
  return tableName;
}
