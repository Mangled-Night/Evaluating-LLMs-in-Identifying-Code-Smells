public class CloseHandler implements Handler<Void> {
  private ShellImpl shell;
  public CloseHandler(  ShellImpl shell){
    this.shell=shell;
  }
  @Override public void handle(  Void event){
    shell.jobController().close(shell.closedFutureHandler());
  }
}
