@Override public void onBlockChanged(Vector3ic pos,Block newBlock,Block originalBlock){
  Vector3i chunkPos=Chunks.toChunkPos(pos,new Vector3i());
  if (relevantChunks.contains(chunkPos)) {
    queuedOutgoingBlockChanges.add(NetData.BlockChangeMessage.newBuilder().setPos(NetMessageUtil.convert(pos)).setNewBlock(newBlock.getId()).build());
  }
}
