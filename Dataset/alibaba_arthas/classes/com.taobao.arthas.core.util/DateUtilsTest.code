public class DateUtilsTest {
  @Test public void testGetCurrentDateWithCorrectFormat(){
    SimpleDateFormat dateFormat=new SimpleDateFormat("yyyy-MM-dd HH:mm:ss");
    Date date=new Date();
    Assert.assertEquals(DateUtils.formatDate(date),dateFormat.format(date));
  }
  @Test public void testGetCurrentDateWithInCorrectFormat(){
    SimpleDateFormat dateFormat=new SimpleDateFormat("yyyy-MM-dd HH:mm");
    Date date=new Date();
    Assert.assertNotEquals(DateUtils.formatDate(date),dateFormat.format(date));
  }
}
