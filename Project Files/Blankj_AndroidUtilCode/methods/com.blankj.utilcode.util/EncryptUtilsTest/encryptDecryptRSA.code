@Test public void encryptDecryptRSA() throws Exception {
  int keySize=1024;
  Pair<String,String> publicPrivateKey=genKeyPair(keySize);
  String publicKey=publicPrivateKey.first;
  String privateKey=publicPrivateKey.second;
  String dataRSA="BlankjBlankjBlankjBlankjBlankjBlankjBlankjBlankjBlankjBlankjBlankjBlankjBlankjBlankjBlankjBlankjBlankjBlankjBlankjBlankjBlankjBl";
  System.out.println("publicKeyBase64:" + publicKey);
  System.out.println("privateKeyBase64:" + privateKey);
  System.out.println(EncryptUtils.encryptRSA2HexString(dataRSA.getBytes(),UtilsBridge.base64Decode(publicKey.getBytes()),keySize,"RSA/None/PKCS1Padding"));
  assertArrayEquals(EncryptUtils.decryptRSA(EncryptUtils.encryptRSA(dataRSA.getBytes(),UtilsBridge.base64Decode(publicKey.getBytes()),keySize,"RSA/None/PKCS1Padding"),UtilsBridge.base64Decode(privateKey.getBytes()),keySize,"RSA/None/PKCS1Padding"),dataRSA.getBytes());
}
