public static <T>void sort(@Nullable T[] array,Comparator<? super T> c){
  if (array == null || array.length < 2)   return;
  Arrays.sort(array,c);
}
public static void sort(@Nullable byte[] array){
  if (array == null || array.length < 2)   return;
  Arrays.sort(array);
}
