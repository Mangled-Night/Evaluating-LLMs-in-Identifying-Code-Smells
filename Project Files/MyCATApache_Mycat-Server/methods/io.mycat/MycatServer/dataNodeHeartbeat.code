private Runnable dataNodeHeartbeat(){
  return new Runnable(){
    @Override public void run(){
      timerExecutor.execute(new Runnable(){
        @Override public void run(){
          Map<String,PhysicalDBPool> nodes=config.getDataHosts();
          for (          PhysicalDBPool node : nodes.values()) {
            node.doHeartbeat();
          }
        }
      }
);
    }
  }
;
}
