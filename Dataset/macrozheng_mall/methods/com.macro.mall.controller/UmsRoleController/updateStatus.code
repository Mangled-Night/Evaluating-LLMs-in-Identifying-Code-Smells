@ApiOperation("修改角色状态") @RequestMapping(value="/updateStatus/{id}",method=RequestMethod.POST) @ResponseBody public CommonResult updateStatus(@PathVariable Long id,@RequestParam(value="status") Integer status){
  UmsRole umsRole=new UmsRole();
  umsRole.setStatus(status);
  int count=roleService.update(id,umsRole);
  if (count > 0) {
    return CommonResult.success(count);
  }
  return CommonResult.failed();
}
