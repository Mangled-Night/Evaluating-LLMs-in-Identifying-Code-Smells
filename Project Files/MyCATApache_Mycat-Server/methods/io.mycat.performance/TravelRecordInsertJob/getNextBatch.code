private List<Map<String,String>> getNextBatch(){
  if (finsihed >= endId) {
    return Collections.emptyList();
  }
  long end=(finsihed + batchSize) < this.endId ? (finsihed + batchSize) : endId;
  if (end + batchSize > this.endId) {
    end=this.endId;
  }
  List<Map<String,String>> list=new ArrayList<Map<String,String>>(Integer.valueOf((end - finsihed) + ""));
  for (long i=finsihed; i <= end; i++) {
    Map<String,String> m=new HashMap<String,String>();
    m.put("id",i + "");
    m.put("user_id","user " + i);
    m.put("traveldate",getRandomDay(i));
    m.put("fee",i % 10000 + "");
    m.put("days",i % 10000 + "");
    list.add(m);
  }
  finsihed+=list.size();
  return list;
}
