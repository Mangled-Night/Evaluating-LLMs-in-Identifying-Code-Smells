@RequiresPermissions("admin:config:express:updateConfigs") @RequiresPermissionsDesc(menu={"配置管理","运费配置"},button="编辑") @PostMapping("/express") public Object updateExpress(@RequestBody String body){
  Map<String,String> data=JacksonUtil.toMap(body);
  systemConfigService.updateConfig(data);
  SystemConfig.updateConfigs(data);
  return ResponseUtil.ok();
}
