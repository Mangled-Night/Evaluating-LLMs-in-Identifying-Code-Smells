@Override public String draw(){
  final StringBuilder ladderSB=new StringBuilder();
  int deep=0;
  for (  String item : items) {
    if (deep == 0) {
      ladderSB.append(item).append("\n");
    }
 else {
      ladderSB.append(StringUtils.repeat(STEP_CHAR,deep * INDENT_STEP)).append(LADDER_CHAR).append(item).append("\n");
    }
    deep++;
  }
  return ladderSB.toString();
}
