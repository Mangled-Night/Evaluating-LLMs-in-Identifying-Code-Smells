/** 
 * Return the used omemoStore backend. If there is no store backend set yet, set the default one (typically a file-based one).
 * @return omemoStore backend
 */
public OmemoStore<T_IdKeyPair,T_IdKey,T_PreKey,T_SigPreKey,T_Sess,T_Addr,T_ECPub,T_Bundle,T_Ciph> getOmemoStoreBackend(){
  if (omemoStore == null) {
    omemoStore=createDefaultOmemoStoreBackend();
  }
  return omemoStore;
}
