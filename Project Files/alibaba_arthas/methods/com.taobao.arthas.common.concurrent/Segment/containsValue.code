boolean containsValue(Object value){
  if (count != 0) {
    for (    HashEntry<K,V> e : table) {
      for (; e != null; e=e.next) {
        Object opaque=e.valueRef;
        V v;
        if (opaque == null) {
          v=readValueUnderLock(e);
        }
 else {
          v=e.dereferenceValue(opaque);
        }
        if (value.equals(v)) {
          return true;
        }
      }
    }
  }
  return false;
}
