@Override public void run(){
  try {
    output.write(FIRST_BYTES);
    output.flush();
    while (true) {
      int got=input.read(readBuffer);
      output.write(writeBuffer,0,got);
    }
  }
 catch (  IOException e) {
    e.printStackTrace();
    if (socket != null) {
      try {
        socket.close();
      }
 catch (      IOException e1) {
        e1.printStackTrace();
      }
    }
  }
}
