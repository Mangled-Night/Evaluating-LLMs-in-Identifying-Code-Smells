@RequiresPermissions("admin:config:mall:updateConfigs") @RequiresPermissionsDesc(menu={"配置管理","商场配置"},button="编辑") @PostMapping("/mall") public Object updateMall(@RequestBody String body){
  Map<String,String> data=JacksonUtil.toMap(body);
  systemConfigService.updateConfig(data);
  SystemConfig.updateConfigs(data);
  return ResponseUtil.ok();
}
