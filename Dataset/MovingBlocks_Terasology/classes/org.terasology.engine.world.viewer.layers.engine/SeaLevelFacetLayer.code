/** 
 * Provides information about the sea level.
 */
@Renders(value=SeaLevelFacet.class,order=-1) public class SeaLevelFacetLayer extends AbstractFacetLayer {
  @Override public void render(  BufferedImage img,  Region region){
  }
  @Override public String getWorldText(  Region region,  int wx,  int wy){
    return "SeaLevel: " + region.getFacet(SeaLevelFacet.class).getSeaLevel();
  }
}
