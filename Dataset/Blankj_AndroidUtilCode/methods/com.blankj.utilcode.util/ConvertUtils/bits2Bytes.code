/** 
 * Bits to bytes.
 * @param bits The bits.
 * @return bytes
 */
public static byte[] bits2Bytes(String bits){
  int lenMod=bits.length() % 8;
  int byteLen=bits.length() / 8;
  if (lenMod != 0) {
    for (int i=lenMod; i < 8; i++) {
      bits="0" + bits;
    }
    byteLen++;
  }
  byte[] bytes=new byte[byteLen];
  for (int i=0; i < byteLen; ++i) {
    for (int j=0; j < 8; ++j) {
      bytes[i]<<=1;
      bytes[i]|=bits.charAt(i * 8 + j) - '0';
    }
  }
  return bytes;
}
