private void prim(EdgeWeightedGraph G,int s){
  scan(G,s);
  while (!pq.isEmpty()) {
    Edge e=pq.delMin();
    int v=e.either(), w=e.other(v);
    assert marked[v] || marked[w];
    if (marked[v] && marked[w])     continue;
    mst.enqueue(e);
    weight+=e.weight();
    if (!marked[v])     scan(G,v);
    if (!marked[w])     scan(G,w);
  }
}
