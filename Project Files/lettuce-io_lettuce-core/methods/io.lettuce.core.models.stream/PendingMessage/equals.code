@Override public boolean equals(Object o){
  if (this == o)   return true;
  if (!(o instanceof PendingMessage))   return false;
  PendingMessage that=(PendingMessage)o;
  if (msSinceLastDelivery != that.msSinceLastDelivery)   return false;
  if (redeliveryCount != that.redeliveryCount)   return false;
  if (id != null ? !id.equals(that.id) : that.id != null)   return false;
  return consumer != null ? consumer.equals(that.consumer) : that.consumer == null;
}
