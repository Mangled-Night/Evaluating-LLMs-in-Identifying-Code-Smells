@Override public void onDownloadProgress(long totalTransferredBytes,long totalBytes,int completedFiles,int nFiles){
  int globalPercentage=(int)(totalTransferredBytes * 100f / totalBytes);
  String message=String.format("Downloaded modules: %d of %d\nDownloaded bytes: %d of %d\nGlobal progress: %d%%",completedFiles,nFiles,totalTransferredBytes,totalBytes,globalPercentage);
  popup.setMessage(POPUP_TITLE,message);
}
