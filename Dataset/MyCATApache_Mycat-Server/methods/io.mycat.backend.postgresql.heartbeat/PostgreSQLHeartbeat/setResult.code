public void setResult(int result,PostgreSQLDetector detector,Object attr){
  this.isChecking.set(false);
switch (result) {
case OK_STATUS:
    setOk(detector);
  break;
case ERROR_STATUS:
setError(detector);
break;
case TIMEOUT_STATUS:
setTimeout(detector);
break;
}
if (this.status != OK_STATUS) {
switchSourceIfNeed("heartbeat error");
}
}
