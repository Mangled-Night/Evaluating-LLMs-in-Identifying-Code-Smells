@Test void masterSlaveSslWithReconnect(){
  RedisCommands<String,String> connection=MasterSlave.connect(redisClient,StringCodec.UTF8,MASTER_SLAVE_URIS_NO_VERIFY).sync();
  connection.quit();
  Delay.delay(Duration.ofMillis(200));
  assertThat(connection.ping()).isEqualTo("PONG");
  connection.getStatefulConnection().close();
}
