@Test void isChangedShouldReturnTrueBecauseSlaveIsGone(){
  RedisMasterReplicaNode upstream=new RedisMasterReplicaNode("host",1234,RedisURI.create("host",111),RedisInstance.Role.UPSTREAM);
  RedisMasterReplicaNode replica=new RedisMasterReplicaNode("host",234,RedisURI.create("host",234),RedisInstance.Role.UPSTREAM);
  RedisMasterReplicaNode newupstream=new RedisMasterReplicaNode("host",1234,RedisURI.create("host",111),RedisInstance.Role.UPSTREAM);
  assertThat(ReplicaUtils.isChanged(Arrays.asList(upstream,replica),Arrays.asList(newupstream))).isTrue();
}
