@RequiresPermissions("admin:brand:delete") @RequiresPermissionsDesc(menu={"商场管理","品牌管理"},button="删除") @PostMapping("/delete") public Object delete(@RequestBody LitemallBrand brand){
  Integer id=brand.getId();
  if (id == null) {
    return ResponseUtil.badArgument();
  }
  brandService.deleteById(id);
  return ResponseUtil.ok();
}
