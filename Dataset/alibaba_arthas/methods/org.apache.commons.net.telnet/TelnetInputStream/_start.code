void _start(){
  if (__thread == null) {
    return;
  }
  int priority;
  __isClosed=false;
  priority=Thread.currentThread().getPriority() + 1;
  if (priority > Thread.MAX_PRIORITY) {
    priority=Thread.MAX_PRIORITY;
  }
  __thread.setPriority(priority);
  __thread.setDaemon(true);
  __thread.start();
  __threaded=true;
}
