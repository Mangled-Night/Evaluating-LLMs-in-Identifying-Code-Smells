@RequiresPermissions("admin:notice:delete") @RequiresPermissionsDesc(menu={"推广管理","通知管理"},button="删除") @PostMapping("/delete") public Object delete(@RequestBody LitemallNotice notice){
  noticeAdminService.deleteByNoticeId(notice.getId());
  noticeService.deleteById(notice.getId());
  return ResponseUtil.ok();
}
