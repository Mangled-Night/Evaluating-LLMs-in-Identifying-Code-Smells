/** 
 * 修改菜单层级
 */
private void updateLevel(UmsMenu umsMenu){
  if (umsMenu.getParentId() == 0) {
    umsMenu.setLevel(0);
  }
 else {
    UmsMenu parentMenu=menuMapper.selectByPrimaryKey(umsMenu.getParentId());
    if (parentMenu != null) {
      umsMenu.setLevel(parentMenu.getLevel() + 1);
    }
 else {
      umsMenu.setLevel(0);
    }
  }
}
