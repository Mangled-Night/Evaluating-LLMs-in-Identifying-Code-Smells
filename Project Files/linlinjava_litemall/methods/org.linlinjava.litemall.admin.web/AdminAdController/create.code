@RequiresPermissions("admin:ad:create") @RequiresPermissionsDesc(menu={"推广管理","广告管理"},button="添加") @PostMapping("/create") public Object create(@RequestBody LitemallAd ad){
  Object error=validate(ad);
  if (error != null) {
    return error;
  }
  adService.add(ad);
  return ResponseUtil.ok(ad);
}
