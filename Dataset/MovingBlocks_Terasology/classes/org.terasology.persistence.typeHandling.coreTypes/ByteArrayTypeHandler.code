public class ByteArrayTypeHandler extends org.terasology.persistence.typeHandling.TypeHandler<byte[]> {
  @Override public PersistedData serializeNonNull(  byte[] value,  PersistedDataSerializer serializer){
    return serializer.serialize(value);
  }
  @Override public Optional<byte[]> deserialize(  PersistedData data){
    if (data.isBytes()) {
      return Optional.of(data.getAsBytes());
    }
 else {
      return Optional.empty();
    }
  }
}
