private static Supplier<List<GLFWVidMode>> createAvailableResolutionSupplier(){
  return Suppliers.memoize(() -> GLFW.glfwGetVideoModes(GLFW.glfwGetPrimaryMonitor()).stream().sorted(Comparator.comparing(GLFWVidMode::width).thenComparing(GLFWVidMode::width).thenComparing(GLFWVidMode::refreshRate)).collect(Collectors.toList()));
}
