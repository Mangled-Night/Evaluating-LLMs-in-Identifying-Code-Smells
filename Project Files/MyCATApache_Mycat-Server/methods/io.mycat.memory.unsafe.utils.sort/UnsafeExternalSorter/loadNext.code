@Override public void loadNext() throws IOException {
synchronized (this) {
    loaded=true;
    if (nextUpstream != null) {
      if (lastPage != null) {
        freePage(lastPage);
        lastPage=null;
      }
      upstream=nextUpstream;
      nextUpstream=null;
    }
    numRecords--;
    upstream.loadNext();
  }
}
@Override public void loadNext() throws IOException {
  while (!current.hasNext() && !iterators.isEmpty()) {
    current=iterators.remove();
  }
  current.loadNext();
}
