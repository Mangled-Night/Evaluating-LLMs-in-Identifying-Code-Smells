public static class BlockEventChecker extends BaseComponentSystem {
  public boolean addedReceived;
  public boolean activateReceived;
  public boolean deactivateReceived;
  public boolean removedReceived;
  @ReceiveEvent(components=BlockComponent.class) public void onAdded(  OnAddedComponent event,  EntityRef entity){
    addedReceived=true;
  }
  @ReceiveEvent(components=BlockComponent.class) public void onActivated(  OnActivatedComponent event,  EntityRef entity){
    activateReceived=true;
  }
  @ReceiveEvent(components=BlockComponent.class) public void onDeactivated(  BeforeDeactivateComponent event,  EntityRef entity){
    deactivateReceived=true;
  }
  @ReceiveEvent(components=BlockComponent.class) public void onRemoved(  BeforeRemoveComponent event,  EntityRef entity){
    removedReceived=true;
  }
}
