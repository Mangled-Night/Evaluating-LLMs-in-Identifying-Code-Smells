private static int committed(String stmt,int offset){
  if (stmt.length() > offset + 8) {
    char c1=stmt.charAt(++offset);
    char c2=stmt.charAt(++offset);
    char c3=stmt.charAt(++offset);
    char c4=stmt.charAt(++offset);
    char c5=stmt.charAt(++offset);
    char c6=stmt.charAt(++offset);
    char c7=stmt.charAt(++offset);
    char c8=stmt.charAt(++offset);
    if ((c1 == 'O' || c1 == 'o') && (c2 == 'M' || c2 == 'm') && (c3 == 'M' || c3 == 'm')&& (c4 == 'I' || c4 == 'i')&& (c5 == 'T' || c5 == 't')&& (c6 == 'T' || c6 == 't')&& (c7 == 'E' || c7 == 'e')&& (c8 == 'D' || c8 == 'd')&& (stmt.length() == ++offset || ParseUtil.isEOF(stmt.charAt(offset)))) {
      return TX_READ_COMMITTED;
    }
  }
  return OTHER;
}
