@ApiOperation("getForEntity url") @RequestMapping(value="/get/{id}",method=RequestMethod.GET) @ResponseBody public Object getForEntity(@PathVariable Long id){
  String url=HOST_MALL_ADMIN + "/brand/{id}";
  ResponseEntity<CommonResult> responseEntity=restTemplate.getForEntity(url,CommonResult.class,id);
  return responseEntity.getBody();
}
