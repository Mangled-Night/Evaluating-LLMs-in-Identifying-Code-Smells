@Command(shortDescription="Lists all permission the specified player has",helpText="Lists all permission the specified player has",runOnServer=true,requiredPermission=PermissionManager.USER_MANAGEMENT_PERMISSION) public String listPermissions(@CommandParam(value="player",suggester=UsernameSuggester.class) String player){
  for (  EntityRef client : entityManager.getEntitiesWith(ClientComponent.class)) {
    ClientComponent clientComponent=client.getComponent(ClientComponent.class);
    if (clientHasName(clientComponent,player)) {
      EntityRef clientInfo=clientComponent.clientInfo;
      PermissionSetComponent permissionSetComp=clientInfo.getComponent(PermissionSetComponent.class);
      return Objects.toString(permissionSetComp.permissions);
    }
  }
  return "Player not found";
}
