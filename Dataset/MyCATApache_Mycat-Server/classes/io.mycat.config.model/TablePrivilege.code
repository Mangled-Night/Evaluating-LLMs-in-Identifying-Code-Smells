/** 
 * 表级权限
 */
public static class TablePrivilege {
  private String name;
  private int[] dml=new int[]{0,0,0,0};
  public String getName(){
    return name;
  }
  public void setName(  String name){
    this.name=name;
  }
  public int[] getDml(){
    return dml;
  }
  public void setDml(  int[] dml){
    this.dml=dml;
  }
}
