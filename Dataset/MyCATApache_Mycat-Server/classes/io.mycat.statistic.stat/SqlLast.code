/** 
 * 记录SQL
 */
public static class SqlLast implements Comparable<SqlLast> {
  private String sql;
  private long executeTime;
  private long startTime;
  private long endTime;
  private String host;
  public SqlLast(  String sql,  long executeTime,  long startTime,  long endTime,  String host){
    super();
    this.sql=sql;
    this.executeTime=executeTime;
    this.startTime=startTime;
    this.endTime=endTime;
    this.host=host;
  }
  public String getSql(){
    return sql;
  }
  public long getStartTime(){
    return startTime;
  }
  public long getExecuteTime(){
    return executeTime;
  }
  public long getEndTime(){
    return endTime;
  }
  @Override public int compareTo(  SqlLast o){
    long st1=o == null ? 0 : o.getStartTime();
    return (int)(st1 - this.getStartTime());
  }
  @Override public boolean equals(  Object obj){
    if (obj instanceof SqlLast) {
      return this.compareTo((SqlLast)obj) == 0;
    }
 else {
      return super.equals(obj);
    }
  }
  public String getHost(){
    return host;
  }
}
