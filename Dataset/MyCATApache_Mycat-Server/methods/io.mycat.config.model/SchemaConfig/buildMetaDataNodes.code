/** 
 * 取得含有不同Meta信息的数据节点,比如表和表结构。
 */
private Set<String> buildMetaDataNodes(){
  Set<String> set=new HashSet<String>();
  if (!isEmpty(dataNode)) {
    set.add(dataNode);
  }
  if (!noSharding) {
    for (    TableConfig tc : tables.values()) {
      set.add(tc.getDataNodes().get(0));
    }
  }
  return set;
}
