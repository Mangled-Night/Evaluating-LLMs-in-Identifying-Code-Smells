@ApiOperation("添加后台资源") @RequestMapping(value="/create",method=RequestMethod.POST) @ResponseBody public CommonResult create(@RequestBody UmsResource umsResource){
  int count=resourceService.create(umsResource);
  dynamicSecurityMetadataSource.clearDataSource();
  if (count > 0) {
    return CommonResult.success(count);
  }
 else {
    return CommonResult.failed();
  }
}
