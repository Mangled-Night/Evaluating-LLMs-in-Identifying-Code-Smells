public void dispose(){
  if (vao != 0) {
    GL30.glDeleteVertexArrays(vao);
  }
  if (vbo != 0) {
    GL30.glDeleteBuffers(vbo);
  }
  if (ebo != 0) {
    GL30.glDeleteBuffers(ebo);
  }
  vao=0;
  vbo=0;
  ebo=0;
}
