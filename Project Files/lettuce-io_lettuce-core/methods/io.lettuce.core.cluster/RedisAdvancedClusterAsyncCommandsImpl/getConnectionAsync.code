private CompletableFuture<RedisClusterAsyncCommands<K,V>> getConnectionAsync(String nodeId){
  return getConnectionProvider().<K,V>getConnectionAsync(ConnectionIntent.WRITE,nodeId).thenApply(StatefulRedisConnection::async);
}
private CompletableFuture<RedisClusterAsyncCommands<K,V>> getConnectionAsync(String host,int port){
  return getConnectionProvider().<K,V>getConnectionAsync(ConnectionIntent.WRITE,host,port).thenApply(StatefulRedisConnection::async);
}
