@RequiresPermissions("admin:category:delete") @RequiresPermissionsDesc(menu={"商场管理","类目管理"},button="删除") @PostMapping("/delete") public Object delete(@RequestBody LitemallCategory category){
  Integer id=category.getId();
  if (id == null) {
    return ResponseUtil.badArgument();
  }
  categoryService.deleteById(id);
  return ResponseUtil.ok();
}
