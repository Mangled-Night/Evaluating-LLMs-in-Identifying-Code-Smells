@Override public boolean updateMesh(){
  if (vertexElements == null || disposed) {
    return false;
  }
  dispose();
  disposed=false;
  for (  ChunkMesh.RenderType type : ChunkMesh.RenderType.values()) {
    generateVBO(type);
  }
  triangleCount=(vertexCount[0] + vertexCount[1] + vertexCount[2]+ vertexCount[3]) / 3;
  return true;
}
