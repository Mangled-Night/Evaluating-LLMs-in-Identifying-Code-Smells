/** 
 * check whether a component is in an Array<br/>
 * @param e   component
 * @param s   array
 * @param < E > component type
 * @return <br/>(1,1,2,3)        == true (1,2,3,4)        == false (null,1,null,2)  == true (1,null)         == false
 */
public static <E>boolean isIn(E e,E... s){
  if (null != s) {
    for (    E es : s) {
      if (isEquals(e,es)) {
        return true;
      }
    }
  }
  return false;
}
