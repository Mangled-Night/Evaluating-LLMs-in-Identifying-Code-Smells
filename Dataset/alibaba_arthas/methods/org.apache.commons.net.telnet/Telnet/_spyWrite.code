/** 
 * Sends a written char on the spy stream.
 * @param ch - character written to the session
 */
void _spyWrite(int ch){
  if (!(_stateIsDo(TelnetOption.ECHO) && _requestedDo(TelnetOption.ECHO))) {
    OutputStream spy=spyStream;
    if (spy != null) {
      try {
        spy.write(ch);
        spy.flush();
      }
 catch (      IOException e) {
        spyStream=null;
      }
    }
  }
}
