/** 
 * find the second text token's text
 */
public static String findSecondTokenText(List<CliToken> tokens){
  if (tokens == null || tokens.isEmpty()) {
    return null;
  }
  boolean first=true;
  for (  CliToken token : tokens) {
    if (token != null && token.isText()) {
      if (first) {
        first=false;
      }
 else {
        return token.value();
      }
    }
  }
  return null;
}
