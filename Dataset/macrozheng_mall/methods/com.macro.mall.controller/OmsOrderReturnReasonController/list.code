@ApiOperation("分页查询退货原因") @RequestMapping(value="/list",method=RequestMethod.GET) @ResponseBody public CommonResult<CommonPage<OmsOrderReturnReason>> list(@RequestParam(value="pageSize",defaultValue="5") Integer pageSize,@RequestParam(value="pageNum",defaultValue="1") Integer pageNum){
  List<OmsOrderReturnReason> reasonList=orderReturnReasonService.list(pageSize,pageNum);
  return CommonResult.success(CommonPage.restPage(reasonList));
}
