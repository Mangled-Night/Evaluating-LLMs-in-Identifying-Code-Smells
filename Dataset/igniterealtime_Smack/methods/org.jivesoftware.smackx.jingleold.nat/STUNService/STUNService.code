/** 
 * Basic constructor, with the hostname and port
 * @param hostname The hostname
 * @param port     The port
 */
public STUNService(String hostname,int port){
  super();
  this.hostname=hostname;
  this.port=port;
}
/** 
 * Default constructor, without name and port.
 */
public STUNService(){
  this(null,-1);
}
