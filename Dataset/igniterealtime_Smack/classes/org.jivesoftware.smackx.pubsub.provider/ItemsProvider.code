/** 
 * Parses the <b>items</b> element out of the message event stanza from the server as specified in the <a href="http://xmpp.org/extensions/xep-0060.html#schemas-event">items schema</a>.
 * @author Robin Collier
 */
public class ItemsProvider extends EmbeddedExtensionProvider<ItemsExtension> {
  @Override protected ItemsExtension createReturnExtension(  String currentElement,  String currentNamespace,  Map<String,String> attributeMap,  List<? extends XmlElement> content){
    return new ItemsExtension(ItemsExtension.ItemsElementType.items,attributeMap.get("node"),content);
  }
}
