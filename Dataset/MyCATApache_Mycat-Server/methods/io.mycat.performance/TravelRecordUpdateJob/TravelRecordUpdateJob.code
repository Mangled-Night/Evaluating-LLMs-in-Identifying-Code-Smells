public TravelRecordUpdateJob(SimpleConPool conPool,long totalRecords,int batchSize,long startId,AtomicLong finshiedCount,AtomicLong failedCount){
  super();
  this.conPool=conPool;
  this.endId=startId + totalRecords - 1;
  this.batchSize=batchSize;
  this.finsihed=startId;
  this.finshiedCount=finshiedCount;
  this.failedCount=failedCount;
}
