public static List<Vector3ic> requiredChunks(Vector3ic pos){
  List<Vector3ic> positions=Lists.newArrayListWithCapacity(3 * 3 * 3);
  for (int x=-1; x <= 1; x++) {
    for (int y=-1; y <= 1; y++) {
      for (int z=-1; z <= 1; z++) {
        positions.add(new Vector3i(pos).add(x,y,z));
      }
    }
  }
  return positions;
}
