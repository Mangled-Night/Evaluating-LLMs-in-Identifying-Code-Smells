@Override public Duration createDelay(long attempt){
  long delay;
  if (attempt <= 0) {
    delay=0;
  }
 else   if (powersOf == 2) {
    delay=calculatePowerOfTwo(attempt);
  }
 else {
    delay=calculateAlternatePower(attempt);
  }
  return applyBounds(Duration.ofNanos(targetTimeUnit.toNanos(delay)));
}
