@ApiOperation("备注订单") @RequestMapping(value="/update/note",method=RequestMethod.POST) @ResponseBody public CommonResult updateNote(@RequestParam("id") Long id,@RequestParam("note") String note,@RequestParam("status") Integer status){
  int count=orderService.updateNote(id,note,status);
  if (count > 0) {
    return CommonResult.success(count);
  }
  return CommonResult.failed();
}
