/** 
 * Trigger event that sets the direction a player is facing.
 */
@OwnerEvent public class SetDirectionEvent implements Event {
  private float yaw;
  private float pitch;
  protected SetDirectionEvent(){
  }
  public SetDirectionEvent(  float yaw,  float pitch){
    this.yaw=yaw;
    this.pitch=pitch;
  }
  public float getYaw(){
    return yaw;
  }
  public float getPitch(){
    return pitch;
  }
}
