@Command(shortDescription="Kick user by ID",runOnServer=true,requiredPermission=PermissionManager.USER_MANAGEMENT_PERMISSION) public String kickUserByID(@CommandParam("userId") int userId){
  for (  EntityRef clientEntity : entityManager.getEntitiesWith(ClientComponent.class)) {
    EntityRef clientInfo=clientEntity.getComponent(ClientComponent.class).clientInfo;
    NetworkComponent nc=clientInfo.getComponent(NetworkComponent.class);
    if (userId == nc.getNetworkId()) {
      return kick(clientEntity);
    }
  }
  throw new IllegalArgumentException("No such user with ID " + userId);
}
