public boolean parseArgs(String[] args){
  if (args.length < 5) {
    System.out.println("input param,format: [jdbcurl] [user] [password]  [threadpoolsize]  recordcount ");
    return false;
  }
  url=args[0];
  user=args[1];
  password=args[2];
  threadCount=Integer.parseInt(args[3]);
  rangeItems=new String[]{"0-" + Integer.parseInt(args[4])};
  return true;
}
