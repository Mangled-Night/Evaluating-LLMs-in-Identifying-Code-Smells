/** 
 * Functions for deleting a key
 */
private Node<Key> erase(int i){
  Node<Key> reference=nodes[i];
  Node<Key> x=head;
  Node<Key> previous=null;
  while (x != reference) {
    previous=x;
    x=x.sibling;
  }
  previous.sibling=x.sibling;
  if (x == head)   head=head.sibling;
  nodes[i]=null;
  return x;
}
