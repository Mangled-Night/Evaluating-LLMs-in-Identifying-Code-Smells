@Override public <K,V>void build(CommandArgs<K,V> args){
  if (command.getSubCommand() == null) {
    args.add("+" + command.getCommand().name());
  }
 else {
    args.add("+" + command.getCommand().name() + "|"+ command.getSubCommand().name());
  }
}
@Override public <K,V>void build(CommandArgs<K,V> args){
  if (command.getSubCommand() == null) {
    args.add("-" + command.getCommand().name());
  }
 else {
    args.add("-" + command.getCommand().name() + "|"+ command.getSubCommand().name());
  }
}
