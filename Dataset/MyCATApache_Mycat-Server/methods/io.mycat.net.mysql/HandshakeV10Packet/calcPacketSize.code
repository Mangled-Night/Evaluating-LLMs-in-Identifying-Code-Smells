@Override public int calcPacketSize(){
  int size=1;
  size+=(serverVersion.length + 1);
  size+=4;
  size+=seed.length;
  size+=1;
  size+=2;
  size+=1;
  size+=2;
  size+=2;
  size+=1;
  size+=10;
  if ((serverCapabilities & Capabilities.CLIENT_SECURE_CONNECTION) != 0) {
    if (restOfScrambleBuff.length <= 13) {
      size+=13;
    }
 else {
      size+=restOfScrambleBuff.length;
    }
  }
  if ((serverCapabilities & Capabilities.CLIENT_PLUGIN_AUTH) != 0) {
    size+=(authPluginName.length + 1);
  }
  return size;
}
