@ApiOperation("添加商品收藏") @RequestMapping(value="/add",method=RequestMethod.POST) @ResponseBody public CommonResult add(@RequestBody MemberProductCollection productCollection){
  int count=memberCollectionService.add(productCollection);
  if (count > 0) {
    return CommonResult.success(count);
  }
 else {
    return CommonResult.failed();
  }
}
