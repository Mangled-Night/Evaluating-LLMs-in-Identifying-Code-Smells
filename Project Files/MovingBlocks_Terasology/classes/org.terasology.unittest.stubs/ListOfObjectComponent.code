public class ListOfObjectComponent implements Component<ListOfObjectComponent> {
  public String shortName;
  public List<SubElement> elements=new ArrayList<>();
  @Override public void copyFrom(  ListOfObjectComponent other){
    this.shortName=other.shortName;
    this.elements=other.elements.stream().map(SubElement::new).collect(Collectors.toList());
  }
@MappedContainer public static class SubElement {
    public String id;
    public String type;
    public SubElement(){
    }
    public SubElement(    SubElement other){
      this();
      this.id=other.id;
      this.type=other.type;
    }
  }
}
