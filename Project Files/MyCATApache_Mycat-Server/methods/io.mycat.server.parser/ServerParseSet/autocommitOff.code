private static int autocommitOff(String stmt,int offset){
  if (stmt.length() > ++offset) {
switch (stmt.charAt(offset)) {
case 'F':
case 'f':
      if (stmt.length() == ++offset || ParseUtil.isEOF(stmt.charAt(offset))) {
        return AUTOCOMMIT_OFF;
      }
 else {
        return OTHER;
      }
default :
    return OTHER;
}
}
return OTHER;
}
