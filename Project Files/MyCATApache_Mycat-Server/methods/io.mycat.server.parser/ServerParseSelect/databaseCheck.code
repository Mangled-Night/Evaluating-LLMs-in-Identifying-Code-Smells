/** 
 * SELECT DATABASE()
 */
static int databaseCheck(String stmt,int offset){
  int length=offset + _DATABASE.length;
  if (stmt.length() >= length && ParseUtil.compare(stmt,offset,_DATABASE)) {
    if (stmt.length() > length && stmt.charAt(length) != ' ') {
      return OTHER;
    }
 else {
      return DATABASE;
    }
  }
  return OTHER;
}
