/** 
 * 通过别名获取到表的名称.如果获取不到返回alias
 * @param alias
 * @return
 */
private String getTableNameByAlias(String alias){
  if (alias != null) {
    String upperCaseAlias=alias.toUpperCase();
    if (aliasMap.containsKey(upperCaseAlias)) {
      return aliasMap.get(upperCaseAlias);
    }
  }
  return alias;
}
