@ApiOperation("添加首页推荐专题") @RequestMapping(value="/create",method=RequestMethod.POST) @ResponseBody public CommonResult create(@RequestBody List<SmsHomeRecommendSubject> homeRecommendSubjectList){
  int count=recommendSubjectService.create(homeRecommendSubjectList);
  if (count > 0) {
    return CommonResult.success(count);
  }
  return CommonResult.failed();
}
