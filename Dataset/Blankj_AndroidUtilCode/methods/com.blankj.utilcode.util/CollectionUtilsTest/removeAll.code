@Test public void removeAll(){
  ArrayList<Integer> list=CollectionUtils.newArrayList(0,1,2,3);
  Assert.assertEquals(0,CollectionUtils.removeAll(null,null).size());
  Assert.assertTrue(CollectionUtils.isEqualCollection(CollectionUtils.newArrayList(0,1,2,3),CollectionUtils.removeAll(list,null)));
  Assert.assertEquals(0,CollectionUtils.removeAll(null,list).size());
  Assert.assertTrue(CollectionUtils.isEqualCollection(CollectionUtils.newArrayList(2,3),CollectionUtils.removeAll(list,CollectionUtils.newArrayList(0,1,6))));
}
