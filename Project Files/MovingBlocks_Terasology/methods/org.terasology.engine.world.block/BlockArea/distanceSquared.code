@Override public long distanceSquared(Vector2ic p){
  return distanceSquared(p.x(),p.y());
}
@Override public long distanceSquared(int px,int py){
  int cx2=minX() + maxX();
  int cy2=minY() + maxY();
  long dx=Math.max(Math.abs(2 * px - cx2) - getSizeX(),0) / 2;
  long dy=Math.max(Math.abs(2 * py - cy2) - getSizeY(),0) / 2;
  return dx * dx + dy * dy;
}
