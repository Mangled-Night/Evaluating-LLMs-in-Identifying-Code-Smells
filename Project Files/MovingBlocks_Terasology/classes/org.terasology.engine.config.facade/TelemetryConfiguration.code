/** 
 * TelemetryConfiguration is a wrapper for  {@link org.terasology.engine.config.TelemetryConfig}. It provides other modules with necessary telemetry configuration.
 */
@API public interface TelemetryConfiguration {
  boolean isTelemetryEnabled();
  boolean isErrorReportingEnabled();
  int fetchBindingSize();
  Boolean get(  String telemetryField);
  boolean containsField(  String telemetryField);
}
