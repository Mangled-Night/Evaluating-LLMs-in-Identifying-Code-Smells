@Requires(@Facet(Facet2.class)) @Produces(Facet3.class) @Updates({@Facet(Facet1.class),@Facet(Facet4.class)}) public static class FacetUpdater implements FacetProvider {
  @Override public void process(  GeneratingRegion region){
    Facet3 facet=new Facet3(region.getRegion(),region.getBorderForFacet(Facet3.class));
    Facet1 facet1=region.getRegionFacet(Facet1.class);
    Facet4 facet4=region.getRegionFacet(Facet4.class);
    facet1.updated=true;
    facet4.updated=true;
    region.setRegionFacet(Facet3.class,facet);
  }
}
