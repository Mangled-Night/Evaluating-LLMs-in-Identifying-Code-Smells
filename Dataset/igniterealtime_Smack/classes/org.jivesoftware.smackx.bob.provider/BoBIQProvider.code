/** 
 * Bits of Binary IQ provider class.
 * @author Florian Schmaus
 * @see <a href="http://xmpp.org/extensions/xep-0231.html">XEP-0231: Bits of
 *      Binary</a>
 */
public class BoBIQProvider extends IqProvider<BoBIQ> {
  @Override public BoBIQ parse(  XmlPullParser parser,  int initialDepth,  IqData iqData,  XmlEnvironment xmlEnvironment) throws XmlPullParserException, IOException {
    Pair<ContentId,BoBData> parserResult=BoBProviderUtil.parseContentIdAndBobData(parser,initialDepth,xmlEnvironment);
    return new BoBIQ(parserResult.getFirst(),parserResult.getSecond());
  }
}
