public class DescriptionProvider extends FormFieldChildElementProvider<FormField.Description> {
  @Override public FormField.Description parse(  XmlPullParser parser,  int initialDepth,  XmlEnvironment xmlEnvironment) throws XmlPullParserException, IOException {
    String description=parser.nextText();
    return new FormField.Description(description);
  }
  @Override public QName getQName(){
    return FormField.Description.QNAME;
  }
}
