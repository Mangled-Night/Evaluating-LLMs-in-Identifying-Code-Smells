private static int sCheck(String stmt,int offset){
  if (stmt.length() > ++offset) {
switch (stmt.charAt(offset)) {
case 'E':
case 'e':
      return seCheck(stmt,offset);
case 'H':
case 'h':
    return show(stmt,offset);
case 'W':
case 'w':
  return swh(stmt,offset);
case 'T':
case 't':
return stop(stmt,offset);
default :
return OTHER;
}
}
return OTHER;
}
