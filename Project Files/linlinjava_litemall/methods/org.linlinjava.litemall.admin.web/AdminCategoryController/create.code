@RequiresPermissions("admin:category:create") @RequiresPermissionsDesc(menu={"商场管理","类目管理"},button="添加") @PostMapping("/create") public Object create(@RequestBody LitemallCategory category){
  Object error=validate(category);
  if (error != null) {
    return error;
  }
  categoryService.add(category);
  return ResponseUtil.ok(category);
}
