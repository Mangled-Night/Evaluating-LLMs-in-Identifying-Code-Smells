private void dfs(Digraph G,int v){
  marked[v]=true;
  low[v]=pre++;
  int min=low[v];
  stack.push(v);
  for (  int w : G.adj(v)) {
    if (!marked[w])     dfs(G,w);
    if (low[w] < min)     min=low[w];
  }
  if (min < low[v]) {
    low[v]=min;
    return;
  }
  int w;
  do {
    w=stack.pop();
    id[w]=count;
    low[w]=G.V();
  }
 while (w != v);
  count++;
}
