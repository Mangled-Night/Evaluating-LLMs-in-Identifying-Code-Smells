@ApiOperation(value="根据专题名称分页获取商品专题") @RequestMapping(value="/list",method=RequestMethod.GET) @ResponseBody public CommonResult<CommonPage<CmsSubject>> getList(@RequestParam(value="keyword",required=false) String keyword,@RequestParam(value="pageNum",defaultValue="1") Integer pageNum,@RequestParam(value="pageSize",defaultValue="5") Integer pageSize){
  List<CmsSubject> subjectList=subjectService.list(keyword,pageNum,pageSize);
  return CommonResult.success(CommonPage.restPage(subjectList));
}
