/** 
 * Return whether the file exists.
 * @param file The file.
 * @return {@code true}: yes<br> {@code false}: no
 */
public static boolean isFileExists(final File file){
  if (file == null)   return false;
  if (file.exists()) {
    return true;
  }
  return isFileExists(file.getAbsolutePath());
}
