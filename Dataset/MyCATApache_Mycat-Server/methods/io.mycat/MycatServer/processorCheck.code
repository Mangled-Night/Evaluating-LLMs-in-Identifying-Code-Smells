private Runnable processorCheck(){
  return new Runnable(){
    @Override public void run(){
      timerExecutor.execute(new Runnable(){
        @Override public void run(){
          try {
            for (            NIOProcessor p : processors) {
              p.checkBackendCons();
            }
          }
 catch (          Exception e) {
            LOGGER.warn("checkBackendCons caught err:" + e);
          }
        }
      }
);
      timerExecutor.execute(new Runnable(){
        @Override public void run(){
          try {
            for (            NIOProcessor p : processors) {
              p.checkFrontCons();
            }
          }
 catch (          Exception e) {
            LOGGER.warn("checkFrontCons caught err:" + e);
          }
        }
      }
);
    }
  }
;
}
