/** 
 * @author mycat
 */
public class BindValue {
  public boolean isNull;
  public boolean isLongData;
  public boolean isSet;
  public long length;
  public int type;
  public byte scale;
  /** 
 * 数据值 
 */
  public byte byteBinding;
  public short shortBinding;
  public int intBinding;
  public float floatBinding;
  public long longBinding;
  public double doubleBinding;
  public Object value;
  public void reset(){
    this.isNull=false;
    this.isLongData=false;
    this.isSet=false;
    this.length=0;
    this.type=0;
    this.scale=0;
    this.byteBinding=0;
    this.shortBinding=0;
    this.intBinding=0;
    this.floatBinding=0;
    this.longBinding=0L;
    this.doubleBinding=0D;
    this.value=null;
  }
}
