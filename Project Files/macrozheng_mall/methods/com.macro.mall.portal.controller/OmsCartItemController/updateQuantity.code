@ApiOperation("修改购物车中指定商品的数量") @RequestMapping(value="/update/quantity",method=RequestMethod.GET) @ResponseBody public CommonResult updateQuantity(@RequestParam Long id,@RequestParam Integer quantity){
  int count=cartItemService.updateQuantity(id,memberService.getCurrentMember().getId(),quantity);
  if (count > 0) {
    return CommonResult.success(count);
  }
  return CommonResult.failed();
}
