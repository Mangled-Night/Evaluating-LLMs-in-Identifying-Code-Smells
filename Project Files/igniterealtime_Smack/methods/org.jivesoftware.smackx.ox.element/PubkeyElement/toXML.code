@Override public XmlStringBuilder toXML(org.jivesoftware.smack.packet.XmlEnvironment enclosingNamespace){
  XmlStringBuilder xml=new XmlStringBuilder(this).optAttribute(ATTR_DATE,date).rightAngleBracket().append(getDataElement()).closeElement(this);
  return xml;
}
@Override public XmlStringBuilder toXML(org.jivesoftware.smack.packet.XmlEnvironment enclosingNamespace){
  XmlStringBuilder xml=new XmlStringBuilder(this,enclosingNamespace).rightAngleBracket().append(b64Data).closeElement(this);
  return xml;
}
