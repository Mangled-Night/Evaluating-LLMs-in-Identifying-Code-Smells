private Node eraseMin(){
  Node min=head;
  Node previous=null;
  Node current=head;
  while (current.sibling != null) {
    if (greater(min.key,current.sibling.key)) {
      previous=current;
      min=current.sibling;
    }
    current=current.sibling;
  }
  previous.sibling=min.sibling;
  if (min == head)   head=min.sibling;
  return min;
}
