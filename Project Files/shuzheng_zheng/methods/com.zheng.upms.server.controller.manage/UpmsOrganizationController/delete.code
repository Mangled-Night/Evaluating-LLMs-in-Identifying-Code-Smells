@ApiOperation(value="删除组织") @RequiresPermissions("upms:organization:delete") @RequestMapping(value="/delete/{ids}",method=RequestMethod.GET) @ResponseBody public Object delete(@PathVariable("ids") String ids){
  int count=upmsOrganizationService.deleteByPrimaryKeys(ids);
  return new UpmsResult(UpmsResultConstant.SUCCESS,count);
}
