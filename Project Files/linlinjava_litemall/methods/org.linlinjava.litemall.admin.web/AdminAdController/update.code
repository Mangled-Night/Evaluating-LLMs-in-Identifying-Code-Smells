@RequiresPermissions("admin:ad:update") @RequiresPermissionsDesc(menu={"推广管理","广告管理"},button="编辑") @PostMapping("/update") public Object update(@RequestBody LitemallAd ad){
  Object error=validate(ad);
  if (error != null) {
    return error;
  }
  if (adService.updateById(ad) == 0) {
    return ResponseUtil.updatedDataFailed();
  }
  return ResponseUtil.ok(ad);
}
