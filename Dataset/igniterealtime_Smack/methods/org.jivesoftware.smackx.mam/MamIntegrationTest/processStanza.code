@Override public void processStanza(Stanza stanza){
  Message message=(Message)stanza;
  if (message.getBody().equals(messageBody)) {
    messageReceived.signal();
  }
}
@Override public void processStanza(Stanza stanza){
  Message message=(Message)stanza;
  if (message.getBody().equals(lastBody)) {
    allMessagesReceived.signal();
  }
}
