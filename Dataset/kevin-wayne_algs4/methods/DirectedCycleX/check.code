private boolean check(){
  if (hasCycle()) {
    int first=-1, last=-1;
    for (    int v : cycle()) {
      if (first == -1)       first=v;
      last=v;
    }
    if (first != last) {
      System.err.printf("cycle begins with %d and ends with %d\n",first,last);
      return false;
    }
  }
  return true;
}
