@Override <K,V>void build(CommandArgs<K,V> args){
  args.add("OVERFLOW").add(overflowType);
}
abstract <K,V>void build(CommandArgs<K,V> args);
public <K,V>void build(CommandArgs<K,V> args){
  for (  SubCommand command : commands) {
    command.build(args);
  }
}
