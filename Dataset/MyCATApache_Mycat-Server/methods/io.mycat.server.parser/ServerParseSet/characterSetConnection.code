private static int characterSetConnection(String stmt,int offset){
  if (stmt.length() > offset + 8) {
    char c1=stmt.charAt(++offset);
    char c2=stmt.charAt(++offset);
    char c3=stmt.charAt(++offset);
    char c4=stmt.charAt(++offset);
    char c5=stmt.charAt(++offset);
    char c6=stmt.charAt(++offset);
    char c7=stmt.charAt(++offset);
    char c8=stmt.charAt(++offset);
    if ((c1 == 'N' || c1 == 'n') && (c2 == 'N' || c2 == 'n') && (c3 == 'E' || c3 == 'e')&& (c4 == 'C' || c4 == 'c')&& (c5 == 'T' || c5 == 't')&& (c6 == 'I' || c6 == 'i')&& (c7 == 'O' || c7 == 'o')&& (c8 == 'N' || c8 == 'n')) {
      while (stmt.length() > ++offset) {
switch (stmt.charAt(offset)) {
case ' ':
case '\r':
case '\n':
case '\t':
          continue;
case '=':
        while (stmt.length() > ++offset) {
switch (stmt.charAt(offset)) {
case ' ':
case '\r':
case '\n':
case '\t':
            continue;
default :
          return (offset << 8) | CHARACTER_SET_CONNECTION;
      }
    }
  return OTHER;
default :
return OTHER;
}
}
}
}
return OTHER;
}
