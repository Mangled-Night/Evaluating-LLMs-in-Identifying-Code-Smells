@Override protected IQChildElementXmlStringBuilder getIQChildElementBuilder(IQChildElementXmlStringBuilder buf){
  buf.append(" jid=\"").append(userID).append("\">");
  if (reason != null) {
    buf.append("<reason>").append(reason).append("</reason>");
  }
  if (sessionID != null) {
    buf.append(new SessionID(sessionID).toXML());
  }
  if (userID != null) {
    buf.append(new UserID(userID).toXML());
  }
  return buf;
}
