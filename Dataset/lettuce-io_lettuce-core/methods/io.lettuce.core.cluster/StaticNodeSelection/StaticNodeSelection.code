public StaticNodeSelection(ClusterDistributionChannelWriter writer,Predicate<RedisClusterNode> selector,ConnectionIntent connectionIntent,Function<StatefulRedisConnection<K,V>,API> apiExtractor){
  this.writer=writer;
  this.connectionIntent=connectionIntent;
  this.apiExtractor=apiExtractor;
  this.redisClusterNodes=writer.getPartitions().stream().filter(selector).collect(Collectors.toList());
}
