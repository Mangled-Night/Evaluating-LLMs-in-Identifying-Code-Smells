@Override public void initialize() throws SmackException.SmackMessageException, XMPPErrorException, InterruptedException, NoResponseException, NotConnectedException {
  clearCandidates();
  if (!RTPBridge.serviceAvailable(connection)) {
    setInitialized();
    throw new SmackException.SmackMessageException("No RTP Bridge service available");
  }
  setInitialized();
}
