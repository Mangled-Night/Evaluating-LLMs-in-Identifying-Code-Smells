@Override public Asset read(JsonReader in) throws IOException {
  if (in.peek() == JsonToken.NULL) {
    in.nextNull();
    return null;
  }
 else {
    String value=in.nextString();
    Optional<? extends Asset> asset=assetManager.getAsset(value,assetClass);
    if (asset.isPresent()) {
      return asset.get();
    }
  }
  return null;
}
