private void frontendCheck(){
  Iterator<Entry<Long,FrontendConnection>> it=frontends.entrySet().iterator();
  while (it.hasNext()) {
    FrontendConnection c=it.next().getValue();
    if (c == null) {
      it.remove();
      this.frontendsLength.decrementAndGet();
      continue;
    }
    if (c.isClosed()) {
      it.remove();
      this.frontendsLength.decrementAndGet();
    }
 else {
      checkConSendQueue(c);
      c.idleCheck();
    }
  }
}
