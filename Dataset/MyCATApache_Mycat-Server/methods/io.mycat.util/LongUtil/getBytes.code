static void getBytes(long i,int index,byte[] buf){
  long q;
  int r;
  int charPos=index;
  byte sign=0;
  if (i < 0) {
    sign='-';
    i=-i;
  }
  while (i > Integer.MAX_VALUE) {
    q=i / 100;
    r=(int)(i - ((q << 6) + (q << 5) + (q << 2)));
    i=q;
    buf[--charPos]=IntegerUtil.digitOnes[r];
    buf[--charPos]=IntegerUtil.digitTens[r];
  }
  int q2;
  int i2=(int)i;
  while (i2 >= 65536) {
    q2=i2 / 100;
    r=i2 - ((q2 << 6) + (q2 << 5) + (q2 << 2));
    i2=q2;
    buf[--charPos]=IntegerUtil.digitOnes[r];
    buf[--charPos]=IntegerUtil.digitTens[r];
  }
  for (; ; ) {
    q2=(i2 * 52429) >>> (16 + 3);
    r=i2 - ((q2 << 3) + (q2 << 1));
    buf[--charPos]=IntegerUtil.digits[r];
    i2=q2;
    if (i2 == 0) {
      break;
    }
  }
  if (sign != 0) {
    buf[--charPos]=sign;
  }
}
