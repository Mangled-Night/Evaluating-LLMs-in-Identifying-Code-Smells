/** 
 * Pop fragment.
 * @param fm The manager of fragment.
 */
public static void pop(@NonNull final FragmentManager fm){
  pop(fm,true);
}
/** 
 * Pop fragment.
 * @param fm          The manager of fragment.
 * @param isImmediate True to pop immediately, false otherwise.
 */
public static void pop(@NonNull final FragmentManager fm,final boolean isImmediate){
  if (isImmediate) {
    fm.popBackStackImmediate();
  }
 else {
    fm.popBackStack();
  }
}
