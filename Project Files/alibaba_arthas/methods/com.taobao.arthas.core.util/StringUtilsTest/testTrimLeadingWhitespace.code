@Test public void testTrimLeadingWhitespace(){
  Assert.assertEquals("",StringUtils.trimLeadingWhitespace(""));
  Assert.assertEquals("",StringUtils.trimLeadingWhitespace(" "));
  Assert.assertEquals("!",StringUtils.trimLeadingWhitespace("!"));
  Assert.assertEquals("foo  ",StringUtils.trimLeadingWhitespace("  foo  "));
}
