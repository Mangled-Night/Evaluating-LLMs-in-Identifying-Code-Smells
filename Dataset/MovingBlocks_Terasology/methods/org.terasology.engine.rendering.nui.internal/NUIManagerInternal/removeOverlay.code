@Override public void removeOverlay(UIElement overlay){
  removeOverlay(overlay.getUrn());
}
@Override public void removeOverlay(String uri){
  Set<ResourceUrn> assetUri=Assets.resolveAssetUri(uri,UIElement.class);
  if (assetUri.size() == 1) {
    removeOverlay(assetUri.iterator().next());
  }
}
@Override public void removeOverlay(ResourceUrn uri){
  ControlWidget widget=overlays.remove(uri);
  if (widget != null) {
    widget.onClosed();
  }
}
