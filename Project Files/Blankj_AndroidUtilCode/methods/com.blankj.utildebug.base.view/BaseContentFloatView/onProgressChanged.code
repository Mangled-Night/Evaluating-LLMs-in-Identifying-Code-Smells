@Override public void onProgressChanged(int direction,float progress,boolean isTouch){
  if (direction == SwipePanel.TOP) {
    Drawable topDrawable=swipePanel.getTopDrawable();
    if (isTouch) {
      topDrawable.setLevel((int)(progress * 20000));
    }
 else {
      if (progress < 0.5) {
        stopRotate();
      }
    }
  }
}
