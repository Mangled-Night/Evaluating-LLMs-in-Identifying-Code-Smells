@RequiresPermissions("admin:config:wx:updateConfigs") @RequiresPermissionsDesc(menu={"配置管理","小程序配置"},button="编辑") @PostMapping("/wx") public Object updateWx(@RequestBody String body){
  Map<String,String> data=JacksonUtil.toMap(body);
  systemConfigService.updateConfig(data);
  SystemConfig.updateConfigs(data);
  return ResponseUtil.ok();
}
