@ReceiveEvent(components=BlockComponent.class) public void onActivateBlock(OnActivatedComponent event,EntityRef entity){
  BlockComponent block=entity.getComponent(BlockComponent.class);
  EntityRef oldEntity=blockEntityLookup.put(block.getPosition(new Vector3i()),entity);
  if (oldEntity != null && !Objects.equal(oldEntity,entity)) {
    oldEntity.destroy();
  }
}
