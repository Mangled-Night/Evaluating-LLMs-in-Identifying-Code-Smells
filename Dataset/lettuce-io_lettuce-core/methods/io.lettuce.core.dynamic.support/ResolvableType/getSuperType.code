/** 
 * Return a  {@link ResolvableType} representing the direct supertype of this type. If no supertype is available this methodreturns  {@link #NONE}.
 * @see #getInterfaces()
 */
public ResolvableType getSuperType(){
  Class<?> resolved=resolve();
  if (resolved == null || resolved.getGenericSuperclass() == null) {
    return NONE;
  }
  if (this.superType == null) {
    this.superType=forType(TypeWrapper.forGenericSuperclass(resolved),asVariableResolver());
  }
  return this.superType;
}
