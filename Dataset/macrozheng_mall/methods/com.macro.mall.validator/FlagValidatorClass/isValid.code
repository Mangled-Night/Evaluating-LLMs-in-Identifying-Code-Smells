@Override public boolean isValid(Integer value,ConstraintValidatorContext constraintValidatorContext){
  boolean isValid=false;
  if (value == null) {
    return true;
  }
  for (int i=0; i < values.length; i++) {
    if (values[i].equals(String.valueOf(value))) {
      isValid=true;
      break;
    }
  }
  return isValid;
}
