public static void main(String[] args){
  final VolatileObject vo=new VolatileObject();
  new Thread(){
    @Override public void run(){
      System.out.print("set...");
      while (true) {
        vo.object=new Object();
      }
    }
  }
.start();
  new Thread(){
    @Override public void run(){
      System.out.print("get...");
      while (true) {
        Object oo=vo.object;
        oo.toString();
      }
    }
  }
.start();
}
