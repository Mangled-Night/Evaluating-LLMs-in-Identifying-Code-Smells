@Override public XmlStringBuilder toXML(XmlEnvironment xmlEnvironment){
  XmlStringBuilder xml=new XmlStringBuilder(this,xmlEnvironment);
  xml.optAttributeCs("height",height).optAttributeCs("width",width).rightAngleBracket();
  xml.append(uris);
  xml.closeElement(this);
  return xml;
}
@Override public XmlStringBuilder toXML(XmlEnvironment xmlEnvironment){
  XmlStringBuilder xml=new XmlStringBuilder(this,xmlEnvironment);
  xml.attribute("type",type).rightAngleBracket();
  xml.escape(uri.toString());
  xml.closeElement(this);
  return xml;
}
