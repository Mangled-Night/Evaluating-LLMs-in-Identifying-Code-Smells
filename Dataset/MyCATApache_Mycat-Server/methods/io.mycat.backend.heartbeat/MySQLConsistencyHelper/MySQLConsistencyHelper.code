public MySQLConsistencyHelper(MySQLConsistencyChecker heartbeat,SQLJob sqlJob){
  this.heartbeat=heartbeat;
  this.sqlJob=sqlJob;
  this.retryTime.set(RETRY_TIMES);
}
public MySQLConsistencyHelper(MySQLConsistencyChecker heartbeat,SQLJob sqlJob,int retryTime){
  this.heartbeat=heartbeat;
  this.sqlJob=sqlJob;
  if (retryTime > 0 && retryTime < 10)   this.retryTime.set(retryTime);
 else   this.retryTime.set(RETRY_TIMES);
}
