/** 
 * supported: <p> case1: thread| grep xxx [thread|, grep, xxx] -> [thread, |, grep, xxx] case:2 thread | grep xxx [thread, |, grep, xxx] -> [thread, |, grep, xxx] case3: thread |grep xxx [thread, |grep] -> [thread, |, grep, xxx]
 */
@Test public void testSupportedPipeCharWithoutRegex(){
  String[] expectedTextTokenValue=new String[]{"thread","|","grep","xxx"};
  List<CliToken> actualTokens=CliTokenImpl.tokenize("thread| grep xxx");
  assertEquals(expectedTextTokenValue,actualTokens);
  actualTokens=CliTokenImpl.tokenize("thread | grep xxx");
  assertEquals(expectedTextTokenValue,actualTokens);
  actualTokens=CliTokenImpl.tokenize("thread |grep xxx");
  assertEquals(expectedTextTokenValue,actualTokens);
}
