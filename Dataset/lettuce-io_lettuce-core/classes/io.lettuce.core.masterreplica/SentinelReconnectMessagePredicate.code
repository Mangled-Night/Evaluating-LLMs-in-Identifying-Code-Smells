/** 
 * {@link MessagePredicate} to check whether the channel and message contain Sentinel availability changes or a Sentinel wasadded.
 */
private static class SentinelReconnectMessagePredicate implements MessagePredicate {
  @Override public boolean test(  String channel,  String message){
    if (channel.equals("+sentinel")) {
      return true;
    }
    if (channel.equals("-odown") || channel.equals("-sdown")) {
      if (message.startsWith("sentinel ")) {
        return true;
      }
    }
    return false;
  }
}
