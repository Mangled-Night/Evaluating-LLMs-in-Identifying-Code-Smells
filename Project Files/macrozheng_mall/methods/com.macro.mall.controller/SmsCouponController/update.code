@ApiOperation("修改优惠券") @RequestMapping(value="/update/{id}",method=RequestMethod.POST) @ResponseBody public CommonResult update(@PathVariable Long id,@RequestBody SmsCouponParam couponParam){
  int count=couponService.update(id,couponParam);
  if (count > 0) {
    return CommonResult.success(count);
  }
  return CommonResult.failed();
}
