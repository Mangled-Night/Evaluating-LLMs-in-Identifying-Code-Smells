@ConditionalOnProperty(name="spring.arthas.enabled",matchIfMissing=true) public class ArthasEndPointAutoConfiguration {
  @Bean @ConditionalOnMissingBean @ConditionalOnAvailableEndpoint public ArthasEndPoint arthasEndPoint(){
    return new ArthasEndPoint();
  }
}
