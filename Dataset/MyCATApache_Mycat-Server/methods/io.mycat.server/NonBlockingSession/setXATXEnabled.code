public void setXATXEnabled(boolean xaTXEnabled){
  if (xaTXEnabled) {
    LOGGER.info("XA Transaction enabled ,con " + this.getSource());
    if (this.xaTXID == null) {
      xaTXID=genXATXID();
    }
  }
 else {
    LOGGER.info("XA Transaction disabled ,con " + this.getSource());
    this.xaTXID=null;
  }
}
