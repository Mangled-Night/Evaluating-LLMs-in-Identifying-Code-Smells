@Configuration @EnableConfigurationProperties(ExpressProperties.class) public class ExpressAutoConfiguration {
  private final ExpressProperties properties;
  public ExpressAutoConfiguration(  ExpressProperties properties){
    this.properties=properties;
  }
  @Bean public ExpressService expressService(){
    ExpressService expressService=new ExpressService();
    expressService.setProperties(properties);
    return expressService;
  }
}
