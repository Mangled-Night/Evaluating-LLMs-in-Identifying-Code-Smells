private static int firstDifferingBit(String k1,String k2){
  int i=0;
  int c1=safeCharAt(k1,0) & ~1;
  int c2=safeCharAt(k2,0) & ~1;
  if (c1 == c2) {
    i=1;
    while (safeCharAt(k1,i) == safeCharAt(k2,i))     i++;
    c1=safeCharAt(k1,i);
    c2=safeCharAt(k2,i);
  }
  int b=0;
  while (((c1 >>> b) & 1) == ((c2 >>> b) & 1))   b++;
  return i * 16 + b;
}
