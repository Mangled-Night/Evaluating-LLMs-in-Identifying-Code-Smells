public static int indexOf(@Nullable double[] array,double valueToFind,int startIndex){
  if (ArrayUtils.isEmpty(array)) {
    return INDEX_NOT_FOUND;
  }
  if (startIndex < 0) {
    startIndex=0;
  }
  for (int i=startIndex; i < array.length; i++) {
