@Test @EnabledOnCommand("ACL") void testConnectToReplicaWithAcl(){
  connection.close();
  RedisURI replicaUri=RedisURI.Builder.redis(host,TestSettings.port(900 + 6)).withAuthentication("default",passwd).build();
  connection=MasterReplica.connect(client,StringCodec.UTF8,replicaUri);
  RedisCommands<String,String> sync=connection.sync();
  assertThat(sync.ping()).isEqualTo("PONG");
}
