@ApiOperation(value="删除系统") @RequiresPermissions("upms:system:delete") @RequestMapping(value="/delete/{ids}",method=RequestMethod.GET) @ResponseBody public Object delete(@PathVariable("ids") String ids){
  int count=upmsSystemService.deleteByPrimaryKeys(ids);
  return new UpmsResult(UpmsResultConstant.SUCCESS,count);
}
