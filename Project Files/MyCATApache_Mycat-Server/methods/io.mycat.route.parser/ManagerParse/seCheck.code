private static int seCheck(String stmt,int offset){
  if (stmt.length() > ++offset) {
switch (stmt.charAt(offset)) {
case 'L':
case 'l':
      return select(stmt,offset);
case 'T':
case 't':
    if (stmt.length() > ++offset) {
      char c=stmt.charAt(offset);
      if (c == ' ' || c == '\r' || c == '\n' || c == '\t' || c == '/' || c == '#') {
        return SET;
      }
    }
  return OTHER;
default :
return OTHER;
}
}
return OTHER;
}
