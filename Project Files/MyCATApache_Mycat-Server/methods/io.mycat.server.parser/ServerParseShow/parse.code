public static int parse(String stmt,int offset){
  int i=offset;
  for (; i < stmt.length(); i++) {
switch (stmt.charAt(i)) {
case ' ':
      continue;
case 'F':
case 'f':
    return fullTableCheck(stmt,i);
case '/':
case '#':
  i=ParseUtil.comment(stmt,i);
continue;
case 'M':
case 'm':
return mycatCheck(stmt,i);
case 'D':
case 'd':
return dataCheck(stmt,i);
case 'T':
case 't':
return tableCheck(stmt,i);
default :
return OTHER;
}
}
return OTHER;
}
