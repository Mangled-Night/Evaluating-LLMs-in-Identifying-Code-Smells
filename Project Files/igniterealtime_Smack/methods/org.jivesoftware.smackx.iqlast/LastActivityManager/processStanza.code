@Override public void processStanza(Stanza packet){
  Presence presence=(Presence)packet;
  Presence.Mode mode=presence.getMode();
  if (mode == null)   return;
switch (mode) {
case available:
case chat:
    resetIdleTime();
  break;
default :
break;
}
}
@Override public void processStanza(Stanza packet){
  Message message=(Message)packet;
  if (message.getType() == Message.Type.error)   return;
  resetIdleTime();
}
