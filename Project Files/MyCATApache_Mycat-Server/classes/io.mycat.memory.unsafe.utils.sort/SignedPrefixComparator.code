public static final class SignedPrefixComparator extends RadixSortSupport {
  @Override public boolean sortDescending(){
    return false;
  }
  @Override public boolean sortSigned(){
    return true;
  }
  @Override public int compare(  long a,  long b){
    return (a < b) ? -1 : (a > b) ? 1 : 0;
  }
}
