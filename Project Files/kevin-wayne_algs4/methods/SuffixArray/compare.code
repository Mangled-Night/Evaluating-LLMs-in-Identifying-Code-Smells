private static int compare(String query,Suffix suffix){
  int n=Math.min(query.length(),suffix.length());
  for (int i=0; i < n; i++) {
    if (query.charAt(i) < suffix.charAt(i))     return -1;
    if (query.charAt(i) > suffix.charAt(i))     return +1;
  }
  return query.length() - suffix.length();
}
