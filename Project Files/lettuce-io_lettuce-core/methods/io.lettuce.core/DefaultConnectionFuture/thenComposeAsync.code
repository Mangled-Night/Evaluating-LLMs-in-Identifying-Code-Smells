@Override public <U>DefaultConnectionFuture<U> thenComposeAsync(Function<? super T,? extends CompletionStage<U>> fn){
  return adopt(delegate.thenComposeAsync(fn));
}
@Override public <U>DefaultConnectionFuture<U> thenComposeAsync(Function<? super T,? extends CompletionStage<U>> fn,Executor executor){
  return adopt(delegate.thenComposeAsync(fn,executor));
}
