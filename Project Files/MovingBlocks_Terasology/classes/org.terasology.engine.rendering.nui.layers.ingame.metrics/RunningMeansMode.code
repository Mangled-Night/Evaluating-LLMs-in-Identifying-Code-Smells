final class RunningMeansMode extends TimeMetricsMode {
  RunningMeansMode(){
    super("\n- Means -",10);
  }
  @Override protected TObjectDoubleMap<String> gatherMetrics(){
    return PerformanceMonitor.getRunningMean();
  }
  @Override public boolean isAvailable(){
    return true;
  }
  @Override public boolean isPerformanceManagerMode(){
    return true;
  }
}
