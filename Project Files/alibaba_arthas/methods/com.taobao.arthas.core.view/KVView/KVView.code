public KVView(){
  this.tableView=new TableView(new TableView.ColumnDefine[]{new TableView.ColumnDefine(TableView.Align.RIGHT),new TableView.ColumnDefine(TableView.Align.RIGHT),new TableView.ColumnDefine(TableView.Align.LEFT)}).hasBorder(false).padding(0);
}
public KVView(TableView.ColumnDefine keyColumnDefine,TableView.ColumnDefine valueColumnDefine){
  this.tableView=new TableView(new TableView.ColumnDefine[]{keyColumnDefine,new TableView.ColumnDefine(TableView.Align.RIGHT),valueColumnDefine}).hasBorder(false).padding(0);
}
