public boolean visit(SQLExprTableSource x){
  tables.add(fixName(x.toString()));
  return super.visit(x);
}
public boolean visit(SQLExprTableSource x){
  if (tables.contains(x.getAlias())) {
    tables.remove(x.getAlias());
    tables.add(fixName(x.toString()));
  }
  return super.visit(x);
}
