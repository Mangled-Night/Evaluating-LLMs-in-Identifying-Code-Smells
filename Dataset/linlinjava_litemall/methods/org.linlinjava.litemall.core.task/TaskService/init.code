@PostConstruct private void init(){
  taskService=this;
  Executors.newSingleThreadExecutor().execute(new Runnable(){
    @Override public void run(){
      while (true) {
        try {
          Task task=delayQueue.take();
          task.run();
        }
 catch (        Exception e) {
          e.printStackTrace();
        }
      }
    }
  }
);
}
