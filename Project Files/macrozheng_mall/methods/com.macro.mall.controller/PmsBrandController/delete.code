@ApiOperation(value="删除品牌") @RequestMapping(value="/delete/{id}",method=RequestMethod.GET) @ResponseBody public CommonResult delete(@PathVariable("id") Long id){
  int count=brandService.deleteBrand(id);
  if (count == 1) {
    return CommonResult.success(null);
  }
 else {
    return CommonResult.failed();
  }
}
