public class HeadlessMesh extends Mesh {
  protected MeshData data;
  protected AABBf aabb=new AABBf();
  public HeadlessMesh(  ResourceUrn urn,  AssetType<?,MeshData> assetType,  MeshData meshData){
    super(urn,assetType);
    reload(meshData);
  }
  @Override protected void doReload(  MeshData meshData){
    this.data=meshData;
    getBound(aabb);
  }
  @Override public AABBfc getAABB(){
    return aabb;
  }
  @Override public VertexAttributeBinding<Vector3fc,Vector3f> vertices(){
    return data.positions();
  }
  @Override public int elementCount(){
    return data.positions().getResource().elements();
  }
  @Override public void render(){
  }
}
