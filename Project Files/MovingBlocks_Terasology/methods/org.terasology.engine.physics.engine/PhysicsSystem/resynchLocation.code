@NetFilterEvent(netFilter=RegisterMode.REMOTE_CLIENT) @ReceiveEvent(components={RigidBodyComponent.class,LocationComponent.class}) public void resynchLocation(LocationResynchEvent event,EntityRef entity){
  logger.debug("Received location resynch event");
  RigidBody body=physics.getRigidBody(entity);
  body.setTransform(event.getPosition(),event.getRotation());
}
