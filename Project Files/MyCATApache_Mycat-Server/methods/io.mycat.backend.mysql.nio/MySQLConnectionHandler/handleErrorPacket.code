/** 
 * ERROR数据包处理
 */
private void handleErrorPacket(byte[] data){
  ResponseHandler respHand=responseHandler;
  if (respHand != null) {
    handleLogNodeInfo("handleErrorPacket");
    respHand.errorResponse(data,source);
  }
 else {
    LOGGER.error("receive ErrorPacket but no handler");
    closeNoHandler();
  }
}
