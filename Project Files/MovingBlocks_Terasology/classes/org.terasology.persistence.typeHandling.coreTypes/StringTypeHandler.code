public class StringTypeHandler extends TypeHandler<String> {
  @Override public PersistedData serializeNonNull(  String value,  PersistedDataSerializer serializer){
    return serializer.serialize(value);
  }
  @Override public Optional<String> deserialize(  PersistedData data){
    if (data.isString()) {
      return Optional.ofNullable(data.getAsString());
    }
    return Optional.empty();
  }
}
