@Override public void checkConsistency(FormField.Builder<?,?> formField){
  checkListRangeConsistency(formField);
  if (formField.getType() != null) {
switch (formField.getType()) {
case hidden:
case jid_multi:
case jid_single:
      throw new ValidationConsistencyException(String.format("Field type '%1$s' is not consistent with validation method '%2$s'.",formField.getType(),BasicValidateElement.METHOD));
default :
    break;
}
}
}
