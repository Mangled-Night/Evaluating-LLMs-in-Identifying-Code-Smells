public abstract Port getTargetPort();
@Override public InputPort getTargetPort(){
  RenderableNode child=node.withModel().getChild(index());
  if (child != null) {
    return child.getInputPort();
  }
  return null;
}
@Override public InputPort getTargetPort(){
  return null;
}
@Override public OutputPort getTargetPort(){
  return outputPort;
}
