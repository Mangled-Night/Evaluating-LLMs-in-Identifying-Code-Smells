private void dfs(Graph G,int v){
  marked[v]=true;
  for (  int w : G.adj(v)) {
    if (cycle != null)     return;
    if (!marked[w]) {
      edgeTo[w]=v;
      color[w]=!color[v];
      dfs(G,w);
    }
 else     if (color[w] == color[v]) {
      isBipartite=false;
      cycle=new Stack<Integer>();
      cycle.push(w);
      for (int x=v; x != w; x=edgeTo[x]) {
        cycle.push(x);
      }
      cycle.push(w);
    }
  }
}
