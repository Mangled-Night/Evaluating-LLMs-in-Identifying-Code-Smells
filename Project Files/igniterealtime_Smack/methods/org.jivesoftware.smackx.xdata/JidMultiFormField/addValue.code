public Builder addValue(Jid jid){
  Value value=new Value(jid);
  ensureValuesAreInitialized();
  values.add(jid);
  rawValues.add(value);
  return getThis();
}
public Builder addValue(Value value) throws XmppStringprepException {
  Jid jid=JidCreate.from(value.getValue());
  ensureValuesAreInitialized();
  values.add(jid);
  rawValues.add(value);
  return this;
}
