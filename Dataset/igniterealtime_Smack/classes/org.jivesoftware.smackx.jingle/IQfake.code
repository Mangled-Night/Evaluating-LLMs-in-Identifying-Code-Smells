/** 
 * Simple class for testing an IQ...
 * @author Alvaro Saurin
 */
private class IQfake extends IQ {
  private String s;
  public IQfake(  final String s){
    super();
    this.s=s;
  }
  public String getChildElementXML(){
    StringBuilder buf=new StringBuilder();
    buf.append(s);
    return buf.toString();
  }
}
