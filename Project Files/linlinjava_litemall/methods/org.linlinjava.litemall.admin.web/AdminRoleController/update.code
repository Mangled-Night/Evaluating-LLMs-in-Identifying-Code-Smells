@RequiresPermissions("admin:role:update") @RequiresPermissionsDesc(menu={"系统管理","角色管理"},button="角色编辑") @PostMapping("/update") public Object update(@RequestBody LitemallRole role){
  Object error=validate(role);
  if (error != null) {
    return error;
  }
  roleService.updateById(role);
  return ResponseUtil.ok();
}
