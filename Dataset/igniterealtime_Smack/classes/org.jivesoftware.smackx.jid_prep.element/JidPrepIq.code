public class JidPrepIq extends IQ {
  public static final String ELEMENT="jid";
  public static final String NAMESPACE="urn:xmpp:jidprep:0";
  private final String jid;
  public JidPrepIq(  String jid){
    super(ELEMENT,NAMESPACE);
    this.jid=jid;
  }
  public String getJid(){
    return jid;
  }
  @Override protected IQChildElementXmlStringBuilder getIQChildElementBuilder(  IQChildElementXmlStringBuilder xml){
    xml.rightAngleBracket();
    xml.escape(jid);
    return xml;
  }
}
