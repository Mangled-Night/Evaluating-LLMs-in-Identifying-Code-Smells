public static final void writeLong(OutputStream out,long l) throws IOException {
  byte[] b=new byte[8];
  b[0]=(byte)(l & 0xff);
  b[1]=(byte)(l >>> 8);
  b[2]=(byte)(l >>> 16);
  b[3]=(byte)(l >>> 24);
  b[4]=(byte)(l >>> 32);
  b[5]=(byte)(l >>> 40);
  b[6]=(byte)(l >>> 48);
  b[7]=(byte)(l >>> 56);
  out.write(b);
}
