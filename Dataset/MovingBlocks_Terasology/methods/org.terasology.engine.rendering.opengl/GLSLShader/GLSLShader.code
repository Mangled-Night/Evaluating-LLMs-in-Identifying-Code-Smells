public GLSLShader(ResourceUrn urn,AssetType<?,ShaderData> assetType,ShaderData data,DisposalAction disposalAction,LwjglGraphicsProcessing graphicsProcessing){
  super(urn,assetType,disposalAction);
  this.disposalAction=disposalAction;
  this.graphicsProcessing=graphicsProcessing;
  graphicsProcessing.asynchToDisplayThread(() -> {
    reload(data);
  }
);
}
