/** 
 * Anything that acts like a chunk for rendering purposes
 */
@API public interface RenderableChunk {
  Vector3f getRenderPosition();
  AABBfc getAABB();
  void setMesh(  ChunkMesh newMesh);
  void setAnimated(  boolean animated);
  boolean isAnimated();
  boolean hasMesh();
  ChunkMesh getMesh();
  void disposeMesh();
  boolean isReady();
}
