@Override public Flux<StreamMessage<K,V>> xread(XReadArgs.StreamOffset<K>... streams){
  return createDissolvingFlux(() -> commandBuilder.xread(null,streams));
}
@Override public Flux<StreamMessage<K,V>> xread(XReadArgs args,XReadArgs.StreamOffset<K>... streams){
  return createDissolvingFlux(() -> commandBuilder.xread(args,streams));
}
