private static <Key extends Comparable<Key>>long brute(Key[] a,int lo,int hi){
  long inversions=0;
  for (int i=lo; i <= hi; i++)   for (int j=i + 1; j <= hi; j++)   if (less(a[j],a[i]))   inversions++;
  return inversions;
}
private static long brute(int[] a,int lo,int hi){
  long inversions=0;
  for (int i=lo; i <= hi; i++)   for (int j=i + 1; j <= hi; j++)   if (a[j] < a[i])   inversions++;
  return inversions;
}
