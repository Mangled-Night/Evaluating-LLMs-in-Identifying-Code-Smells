private static void insertion(String[] a,int lo,int hi,int d){
  for (int i=lo; i <= hi; i++)   for (int j=i; j > lo && less(a[j],a[j - 1],d); j--)   exch(a,j,j - 1);
}
private static void insertion(int[] a,int lo,int hi,int d){
  for (int i=lo; i <= hi; i++)   for (int j=i; j > lo && less(a[j],a[j - 1],d); j--)   exch(a,j,j - 1);
}
