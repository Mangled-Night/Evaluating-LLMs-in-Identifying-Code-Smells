/** 
 * Parses a MessageEvent stanza (extension sub-packet).
 * @param parser the XML parser, positioned at the starting element of the extension.
 * @return a PacketExtension.
 * @throws IOException if an I/O error occurred.
 * @throws XmlPullParserException if an error in the XML parser occurred.
 */
@Override public MessageEvent parse(XmlPullParser parser,int initialDepth,XmlEnvironment xmlEnvironment) throws XmlPullParserException, IOException {
  MessageEvent messageEvent=new MessageEvent();
  boolean done=false;
  while (!done) {
    XmlPullParser.Event eventType=parser.next();
    if (eventType == XmlPullParser.Event.START_ELEMENT) {
      if (parser.getName().equals("id"))       messageEvent.setStanzaId(parser.nextText());
      if (parser.getName().equals(MessageEvent.COMPOSING))       messageEvent.setComposing(true);
      if (parser.getName().equals(MessageEvent.DELIVERED))       messageEvent.setDelivered(true);
      if (parser.getName().equals(MessageEvent.DISPLAYED))       messageEvent.setDisplayed(true);
      if (parser.getName().equals(MessageEvent.OFFLINE))       messageEvent.setOffline(true);
    }
 else     if (eventType == XmlPullParser.Event.END_ELEMENT) {
      if (parser.getName().equals("x")) {
        done=true;
      }
    }
  }
  return messageEvent;
}
