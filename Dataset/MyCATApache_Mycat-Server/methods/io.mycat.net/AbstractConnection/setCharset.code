public boolean setCharset(String charset){
  if (charset != null) {
    charset=charset.replace("'","");
  }
  int ci=CharsetUtil.getIndex(charset);
  if (ci > 0) {
    this.charset=charset.equalsIgnoreCase("utf8mb4") ? "utf8" : charset;
    this.charsetIndex=ci;
    return true;
  }
 else {
    return false;
  }
}
