private class ReverseArrayIterator implements Iterator<Item> {
  private int i;
  public ReverseArrayIterator(){
    i=n - 1;
  }
  public boolean hasNext(){
    return i >= 0;
  }
  public void remove(){
    throw new UnsupportedOperationException();
  }
  public Item next(){
    if (!hasNext())     throw new NoSuchElementException();
    return a[i--];
  }
}
