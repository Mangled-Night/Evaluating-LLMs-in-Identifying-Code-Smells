@Override public Vector2ic next(){
  if (index >= maxArea) {
    throw new NoSuchElementException("radius has been reached");
  }
switch (leg) {
case 0:
    ++x;
  if (x == radius) {
    ++leg;
  }
break;
case 1:
++y;
if (y == radius) {
++leg;
}
break;
case 2:
--x;
if (-x == radius) {
++leg;
}
break;
case 3:
--y;
if (-y == radius) {
leg=0;
++radius;
}
break;
}
index++;
int finalX=center.x() + x * scale;
int finalY=center.y() + (clockwise ? y : -y) * scale;
pos.set(finalX,finalY);
return pos;
}
