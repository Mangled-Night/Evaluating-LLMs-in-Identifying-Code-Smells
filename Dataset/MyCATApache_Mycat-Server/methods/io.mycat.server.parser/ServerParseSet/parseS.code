private static int parseS(String stmt,int offset){
  if (stmt.length() > offset + 1) {
    char c1=stmt.charAt(offset + 1);
    if (c1 == 'E' || c1 == 'e') {
      return session(stmt,offset);
    }
    if (c1 == 'Q' || c1 == 'q') {
      return sqlSelectLimit(stmt,offset);
    }
  }
  return OTHER;
}
