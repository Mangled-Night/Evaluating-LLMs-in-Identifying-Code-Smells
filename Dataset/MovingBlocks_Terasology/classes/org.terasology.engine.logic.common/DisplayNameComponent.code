@AddToBlockBasedItem public class DisplayNameComponent implements Component<DisplayNameComponent> {
  @Replicate public String name="";
  @Replicate public String description="";
  @Override public String toString(){
    return String.format("DisplayName(name = '%s', description = '%s')",name,description);
  }
  @Override public void copyFrom(  DisplayNameComponent other){
    this.name=other.name;
    this.description=other.description;
  }
}
