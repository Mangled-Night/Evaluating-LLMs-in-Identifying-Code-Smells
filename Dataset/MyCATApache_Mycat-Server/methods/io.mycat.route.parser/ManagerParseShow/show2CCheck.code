static int show2CCheck(String stmt,int offset){
  if (stmt.length() > ++offset) {
switch (stmt.charAt(offset)) {
case 'O':
case 'o':
      return show2CoCheck(stmt,offset);
case 'A':
case 'a':
    return show2CACheck(stmt,offset);
case 'h':
case 'H':
  return show2CHCheck(stmt,offset);
default :
return OTHER;
}
}
return OTHER;
}
