/** 
 * Flight recorder event variant of  {@link SentinelTopologyRefreshEvent}.
 * @author Mark Paluch
 * @since 6.1
 */
@Category({"Lettuce","Master/Replica Events"}) @Label("Sentinel Topology Refresh Trigger") @StackTrace(false) class JfrSentinelTopologyRefreshEvent extends Event {
  private final String source;
  private final String message;
  @Timespan(value=MILLISECONDS) private final long delay;
  public JfrSentinelTopologyRefreshEvent(  SentinelTopologyRefreshEvent event){
    this.source=event.getSource();
    this.message=event.getMessage();
    this.delay=event.getDelayMs();
  }
}
