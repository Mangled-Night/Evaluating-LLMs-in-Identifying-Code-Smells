/** 
 * Rearranges the array of extended ASCII strings in ascending order. This is an unstable sorting algorithm.
 * @param a the array to be sorted
 */
public static void sort(String[] a){
  int n=a.length;
  sort(a,0,n - 1,0);
}
private static void sort(String[] a,int lo,int hi,int d){
  if (hi <= lo + CUTOFF) {
    insertion(a,lo,hi,d);
    return;
  }
  int[] heads=new int[R + 2];
  int[] tails=new int[R + 1];
  for (int i=lo; i <= hi; i++) {
    int c=charAt(a[i],d);
    heads[c + 2]++;
  }
  heads[0]=lo;
  for (int r=0; r < R + 1; r++) {
    heads[r + 1]+=heads[r];
    tails[r]=heads[r + 1];
  }
  for (int r=0; r < R + 1; r++) {
    while (heads[r] < tails[r]) {
      int c=charAt(a[heads[r]],d);
      while (c + 1 != r) {
        exch(a,heads[r],heads[c + 1]++);
        c=charAt(a[heads[r]],d);
      }
      heads[r]++;
    }
  }
  for (int r=0; r < R; r++)   sort(a,tails[r],tails[r + 1] - 1,d + 1);
}
