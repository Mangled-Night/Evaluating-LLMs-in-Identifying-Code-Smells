private int calculateStart(int id){
  int count=0;
  for (int i=1; i < depthMap.length; i++) {
    if (depthMap[i] < depthMap[id]) {
      continue;
    }
 else     if (depthMap[i] == depthMap[id]) {
      if (i == id) {
        break;
      }
 else {
        count+=runLength(i);
      }
    }
 else {
      break;
    }
  }
  return count;
}
