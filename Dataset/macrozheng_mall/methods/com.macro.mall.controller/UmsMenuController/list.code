@ApiOperation("分页查询后台菜单") @RequestMapping(value="/list/{parentId}",method=RequestMethod.GET) @ResponseBody public CommonResult<CommonPage<UmsMenu>> list(@PathVariable Long parentId,@RequestParam(value="pageSize",defaultValue="5") Integer pageSize,@RequestParam(value="pageNum",defaultValue="1") Integer pageNum){
  List<UmsMenu> menuList=menuService.list(parentId,pageSize,pageNum);
  return CommonResult.success(CommonPage.restPage(menuList));
}
