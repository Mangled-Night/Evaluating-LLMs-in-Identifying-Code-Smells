private void handleRealAmount(List<OmsOrderItem> orderItemList){
  for (  OmsOrderItem orderItem : orderItemList) {
    BigDecimal realAmount=orderItem.getProductPrice().subtract(orderItem.getPromotionAmount()).subtract(orderItem.getCouponAmount()).subtract(orderItem.getIntegrationAmount());
    orderItem.setRealAmount(realAmount);
  }
}
