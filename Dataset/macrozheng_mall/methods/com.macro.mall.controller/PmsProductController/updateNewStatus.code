@ApiOperation("批量设为新品") @RequestMapping(value="/update/newStatus",method=RequestMethod.POST) @ResponseBody public CommonResult updateNewStatus(@RequestParam("ids") List<Long> ids,@RequestParam("newStatus") Integer newStatus){
  int count=productService.updateNewStatus(ids,newStatus);
  if (count > 0) {
    return CommonResult.success(count);
  }
 else {
    return CommonResult.failed();
  }
}
