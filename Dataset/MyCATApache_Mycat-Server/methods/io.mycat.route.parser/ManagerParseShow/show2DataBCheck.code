static int show2DataBCheck(String stmt,int offset){
  if (stmt.length() > offset + "ASE".length()) {
    char c1=stmt.charAt(++offset);
    char c2=stmt.charAt(++offset);
    char c3=stmt.charAt(++offset);
    if ((c1 == 'A' || c1 == 'a') && (c2 == 'S' || c2 == 's') && (c3 == 'E' || c3 == 'e')) {
      return DATABASE;
    }
  }
  return OTHER;
}
