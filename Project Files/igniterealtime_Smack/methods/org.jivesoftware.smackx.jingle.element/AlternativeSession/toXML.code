@Override public XmlStringBuilder toXML(org.jivesoftware.smack.packet.XmlEnvironment enclosingNamespace){
  XmlStringBuilder xml=new XmlStringBuilder(this,enclosingNamespace);
  xml.rightAngleBracket();
  xml.openElement(reason.asString);
  xml.openElement(SID);
  xml.append(sessionId);
  xml.closeElement(SID);
  xml.closeElement(reason.asString);
  xml.closeElement(this);
  return xml;
}
