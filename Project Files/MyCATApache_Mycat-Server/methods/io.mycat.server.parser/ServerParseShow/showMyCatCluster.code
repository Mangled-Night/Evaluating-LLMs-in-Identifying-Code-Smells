static int showMyCatCluster(String stmt,int offset){
  if (stmt.length() > offset + "luster".length()) {
    char c1=stmt.charAt(++offset);
    char c2=stmt.charAt(++offset);
    char c3=stmt.charAt(++offset);
    char c4=stmt.charAt(++offset);
    char c5=stmt.charAt(++offset);
    char c6=stmt.charAt(++offset);
    if ((c1 == 'L' || c1 == 'l') && (c2 == 'U' || c2 == 'u') && (c3 == 'S' || c3 == 's')&& (c4 == 'T' || c4 == 't')&& (c5 == 'E' || c5 == 'e')&& (c6 == 'R' || c6 == 'r')&& (stmt.length() == ++offset || ParseUtil.isEOF(stmt.charAt(offset)))) {
      return MYCAT_CLUSTER;
    }
  }
  return OTHER;
}
