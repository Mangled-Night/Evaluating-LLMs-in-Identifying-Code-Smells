/** 
 * Parses one of several elements used in PubSub that contain a form of some kind as a child element.  The elements and namespaces supported is defined in  {@link FormNodeType}.
 * @author Robin Collier
 */
public class FormNodeProvider extends EmbeddedExtensionProvider<FormNode> {
  @Override protected FormNode createReturnExtension(  String currentElement,  String currentNamespace,  Map<String,String> attributeMap,  List<? extends XmlElement> content){
    DataForm dataForm=null;
    if (!content.isEmpty()) {
      dataForm=(DataForm)content.get(0);
    }
    return new FormNode(FormNodeType.valueOfFromElementName(currentElement,currentNamespace),attributeMap.get("node"),dataForm);
  }
}
