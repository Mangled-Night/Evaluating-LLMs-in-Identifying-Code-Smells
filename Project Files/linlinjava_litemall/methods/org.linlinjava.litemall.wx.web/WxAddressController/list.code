/** 
 * 用户收货地址列表
 * @param userId 用户ID
 * @return 收货地址列表
 */
@GetMapping("list") public Object list(@LoginUser Integer userId){
  if (userId == null) {
    return ResponseUtil.unlogin();
  }
  List<LitemallAddress> addressList=addressService.queryByUid(userId);
  return ResponseUtil.okList(addressList);
}
