public int getRows(){
  if (!isDDLComplete()) {
    return 0;
  }
  if (commandResponse != null) {
    String[] s=commandResponse.split(" +");
    if (s.length == 0) {
      return 0;
    }
    try {
      return Integer.valueOf(s[s.length - 1].trim());
    }
 catch (    Exception e) {
      throw new RuntimeException(e);
    }
  }
  return 0;
}
