private void _execute(BackendConnection conn){
  if (session.closed()) {
    endRunning();
    session.clearResources(true);
    return;
  }
  conn.setResponseHandler(this);
  try {
    conn.execute(node,session.getSource(),session.getSource().isAutocommit());
  }
 catch (  Exception e1) {
    executeException(conn,e1);
    return;
  }
}
