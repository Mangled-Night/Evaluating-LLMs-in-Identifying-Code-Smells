public static int getInt(final JSONObject jsonObject,final String key){
  return getInt(jsonObject,key,-1);
}
public static int getInt(final JSONObject jsonObject,final String key,final int defaultValue){
  return getValueByType(jsonObject,key,defaultValue,TYPE_INT);
}
public static int getInt(final String json,final String key){
  return getInt(json,key,-1);
}
public static int getInt(final String json,final String key,final int defaultValue){
  return getValueByType(json,key,defaultValue,TYPE_INT);
}
