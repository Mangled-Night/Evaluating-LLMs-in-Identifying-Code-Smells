private InputEvent createNewBindEvent(InputEvent originalEvent){
  try {
    return originalEvent.getClass().getConstructor().newInstance();
  }
 catch (  NoSuchMethodException|IllegalAccessException|InvocationTargetException|InstantiationException exception) {
    logger.error("ERROR!!! Event not Identified: " + originalEvent.toString(),exception);
  }
  return null;
}
