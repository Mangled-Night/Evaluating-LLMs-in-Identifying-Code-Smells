    char c8=stmt.charAt(++offset);
    char c9=stmt.charAt(++offset);
    char c10=stmt.charAt(++offset);
    if ((c1 == 'C' || c1 == 'c') && (c2 == 'O' || c2 == 'o') && (c3 == 'N' || c3 == 'n')&& (c4 == 'N' || c4 == 'n')&& (c5 == 'E' || c5 == 'e')&& (c6 == 'C' || c6 == 'c')&& (c7 == 'T' || c7 == 't')&& (c8 == 'I' || c8 == 'i')&& (c9 == 'O' || c9 == 'o')&& (c10 == 'N' || c10 == 'n')) {
