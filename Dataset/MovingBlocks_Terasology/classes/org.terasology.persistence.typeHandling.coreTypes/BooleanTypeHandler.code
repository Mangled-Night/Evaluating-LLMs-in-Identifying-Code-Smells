public class BooleanTypeHandler extends TypeHandler<Boolean> {
  @Override public PersistedData serializeNonNull(  Boolean value,  PersistedDataSerializer serializer){
    return serializer.serialize(value);
  }
  @Override public Optional<Boolean> deserialize(  PersistedData data){
    if (data.isBoolean()) {
      return Optional.of(data.getAsBoolean());
    }
    return Optional.empty();
  }
}
