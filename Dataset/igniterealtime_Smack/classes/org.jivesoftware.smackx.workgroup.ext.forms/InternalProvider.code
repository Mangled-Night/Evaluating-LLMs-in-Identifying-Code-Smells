/** 
 * An IQProvider for WebForm packets.
 * @author Derek DeMoro
 */
public static class InternalProvider extends IqProvider<WorkgroupForm> {
  @Override public WorkgroupForm parse(  XmlPullParser parser,  int initialDepth,  IqData iqData,  XmlEnvironment xmlEnvironment) throws XmlPullParserException, IOException, SmackParsingException {
    WorkgroupForm answer=new WorkgroupForm();
    boolean done=false;
    while (!done) {
      XmlPullParser.Event eventType=parser.next();
      if (eventType == XmlPullParser.Event.START_ELEMENT) {
        PacketParserUtils.addExtensionElement(answer,parser,xmlEnvironment);
      }
 else       if (eventType == XmlPullParser.Event.END_ELEMENT) {
        if (parser.getName().equals(ELEMENT_NAME)) {
          done=true;
        }
      }
    }
    return answer;
  }
}
