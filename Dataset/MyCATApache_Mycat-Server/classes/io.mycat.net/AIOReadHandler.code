class AIOReadHandler implements CompletionHandler<Integer,AIOSocketWR> {
  @Override public void completed(  final Integer i,  final AIOSocketWR wr){
    if (i > 0) {
      try {
        wr.con.onReadData(i);
        wr.con.asynRead();
      }
 catch (      IOException e) {
        wr.con.close("handle err:" + e);
      }
    }
 else     if (i == -1) {
      wr.con.close("client closed");
    }
  }
  @Override public void failed(  Throwable exc,  AIOSocketWR wr){
    wr.con.close(exc.toString());
  }
}
