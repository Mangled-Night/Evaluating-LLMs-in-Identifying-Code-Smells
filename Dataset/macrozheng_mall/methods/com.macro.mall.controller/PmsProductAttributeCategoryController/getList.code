@ApiOperation("分页获取所有商品属性分类") @RequestMapping(value="/list",method=RequestMethod.GET) @ResponseBody public CommonResult<CommonPage<PmsProductAttributeCategory>> getList(@RequestParam(defaultValue="5") Integer pageSize,@RequestParam(defaultValue="1") Integer pageNum){
  List<PmsProductAttributeCategory> productAttributeCategoryList=productAttributeCategoryService.getList(pageSize,pageNum);
  return CommonResult.success(CommonPage.restPage(productAttributeCategoryList));
}
