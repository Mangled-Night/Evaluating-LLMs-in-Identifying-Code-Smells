@ApiOperation("批量发货") @RequestMapping(value="/update/delivery",method=RequestMethod.POST) @ResponseBody public CommonResult delivery(@RequestBody List<OmsOrderDeliveryParam> deliveryParamList){
  int count=orderService.delivery(deliveryParamList);
  if (count > 0) {
    return CommonResult.success(count);
  }
  return CommonResult.failed();
}
