@Override public void set(long integer){
  if (member == null) {
    member=(V)(Long)integer;
    return;
  }
  if (withHash) {
    geohash=integer;
  }
}
@Override public void set(ByteBuffer bytes){
  if (member == null) {
    member=codec.decodeValue(bytes);
    return;
  }
  double value=(bytes == null) ? 0 : parseDouble(decodeAscii(bytes));
  set(value);
}
@Override public void set(double number){
  if (withDistance) {
    if (distance == null) {
      distance=number;
      return;
    }
  }
  if (withCoordinates) {
    if (x == null) {
      x=number;
      return;
    }
    coordinates=new GeoCoordinates(x,number);
  }
}
