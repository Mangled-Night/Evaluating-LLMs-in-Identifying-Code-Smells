private static void runAsyncConnectionPoolBenchmark() throws RunnerException {
  new Runner(prepareOptions().mode(Mode.AverageTime).timeUnit(TimeUnit.NANOSECONDS).include(".*AsyncConnectionPoolBenchmark.*").build()).run();
  new Runner(prepareOptions().mode(Mode.Throughput).timeUnit(TimeUnit.SECONDS).include(".*AsyncConnectionPoolBenchmark.*").build()).run();
}
