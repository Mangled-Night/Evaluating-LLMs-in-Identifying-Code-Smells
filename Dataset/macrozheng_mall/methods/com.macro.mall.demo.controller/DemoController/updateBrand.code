@ApiOperation(value="更新品牌") @RequestMapping(value="/brand/update/{id}",method=RequestMethod.POST) @ResponseBody public CommonResult updateBrand(@PathVariable("id") Long id,@Validated @RequestBody PmsBrandDto pmsBrandDto){
  CommonResult commonResult;
  int count=demoService.updateBrand(id,pmsBrandDto);
  if (count == 1) {
    commonResult=CommonResult.success(pmsBrandDto);
    LOGGER.debug("updateBrand success:{}",pmsBrandDto);
  }
 else {
    commonResult=CommonResult.failed("操作失败");
    LOGGER.debug("updateBrand failed:{}",pmsBrandDto);
  }
  return commonResult;
}
