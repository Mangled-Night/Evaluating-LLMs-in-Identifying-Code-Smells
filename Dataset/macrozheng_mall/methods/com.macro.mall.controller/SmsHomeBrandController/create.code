@ApiOperation("添加首页推荐品牌") @RequestMapping(value="/create",method=RequestMethod.POST) @ResponseBody public CommonResult create(@RequestBody List<SmsHomeBrand> homeBrandList){
  int count=homeBrandService.create(homeBrandList);
  if (count > 0) {
    return CommonResult.success(count);
  }
  return CommonResult.failed();
}
