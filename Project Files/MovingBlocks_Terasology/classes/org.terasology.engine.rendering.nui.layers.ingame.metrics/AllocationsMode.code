final class AllocationsMode extends TimeMetricsMode {
  AllocationsMode(){
    super("\n- Memory Allocations -",10,"bytes");
  }
  @Override protected TObjectDoubleMap<String> gatherMetrics(){
    return PerformanceMonitor.getAllocationMean();
  }
  @Override public boolean isAvailable(){
    return true;
  }
  @Override public boolean isPerformanceManagerMode(){
    return true;
  }
}
