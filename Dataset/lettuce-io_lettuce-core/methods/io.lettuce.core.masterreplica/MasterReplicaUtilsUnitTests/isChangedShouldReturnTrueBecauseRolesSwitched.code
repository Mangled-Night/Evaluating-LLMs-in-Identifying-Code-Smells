@Test void isChangedShouldReturnTrueBecauseRolesSwitched(){
  RedisMasterReplicaNode upstream=new RedisMasterReplicaNode("host",1234,RedisURI.create("host",111),RedisInstance.Role.UPSTREAM);
  RedisMasterReplicaNode replica=new RedisMasterReplicaNode("host",234,RedisURI.create("host",234),RedisInstance.Role.UPSTREAM);
  RedisMasterReplicaNode newslave=new RedisMasterReplicaNode("host",1234,RedisURI.create("host",111),RedisInstance.Role.REPLICA);
  RedisMasterReplicaNode newupstream=new RedisMasterReplicaNode("host",234,RedisURI.create("host",234),RedisInstance.Role.UPSTREAM);
  assertThat(ReplicaUtils.isChanged(Arrays.asList(upstream,replica),Arrays.asList(newupstream,newslave))).isTrue();
  assertThat(ReplicaUtils.isChanged(Arrays.asList(upstream,replica),Arrays.asList(newslave,newupstream))).isTrue();
}
