@Override protected long acquireExecutionMemory(long numBytes,long taskAttemptId,MemoryMode memoryMode){
  if (oomOnce) {
    oomOnce=false;
    return 0;
  }
 else   if (available >= numBytes) {
    available-=numBytes;
    return numBytes;
  }
 else {
    long grant=available;
    available=0;
    return grant;
  }
}
