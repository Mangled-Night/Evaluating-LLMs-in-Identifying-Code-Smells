@Test public void testAddSide(){
  Assertions.assertEquals(SideBitFlag.addSide(SideBitFlag.getSides(Side.BACK,Side.BOTTOM),Side.TOP),SideBitFlag.getSides(Side.BACK,Side.BOTTOM,Side.TOP));
  Assertions.assertEquals(SideBitFlag.addSide(SideBitFlag.getSides(Side.BACK,Side.BOTTOM),Side.RIGHT),SideBitFlag.getSides(Side.BACK,Side.BOTTOM,Side.RIGHT));
  Assertions.assertEquals(SideBitFlag.addSide(SideBitFlag.getSides(Side.BACK,Side.BOTTOM,Side.TOP),Side.TOP),SideBitFlag.getSides(Side.BACK,Side.BOTTOM,Side.TOP));
}
