public static int parse(String stmt,int offset){
  int i=offset;
  for (; i < stmt.length(); i++) {
switch (stmt.charAt(i)) {
case ' ':
      continue;
case '/':
case '#':
    i=ParseUtil.comment(stmt,i);
  continue;
case 'T':
case 't':
return transactionCheck(stmt,i);
default :
return OTHER;
}
}
return OTHER;
}
