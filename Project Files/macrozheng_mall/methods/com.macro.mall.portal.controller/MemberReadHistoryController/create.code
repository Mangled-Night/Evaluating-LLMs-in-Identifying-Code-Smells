@ApiOperation("创建浏览记录") @RequestMapping(value="/create",method=RequestMethod.POST) @ResponseBody public CommonResult create(@RequestBody MemberReadHistory memberReadHistory){
  int count=memberReadHistoryService.create(memberReadHistory);
  if (count > 0) {
    return CommonResult.success(count);
  }
 else {
    return CommonResult.failed();
  }
}
