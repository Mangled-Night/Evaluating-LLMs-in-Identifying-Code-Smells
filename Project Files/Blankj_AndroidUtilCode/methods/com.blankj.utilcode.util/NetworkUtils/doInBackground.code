@RequiresPermission(INTERNET) @Override public Boolean doInBackground(){
  return isAvailable();
}
@RequiresPermission(INTERNET) @Override public Boolean doInBackground(){
  return isAvailableByPing(ip);
}
@RequiresPermission(INTERNET) @Override public Boolean doInBackground(){
  return isAvailableByDns(domain);
}
@RequiresPermission(allOf={ACCESS_WIFI_STATE,INTERNET}) @Override public Boolean doInBackground(){
  return isWifiAvailable();
}
@RequiresPermission(INTERNET) @Override public String doInBackground(){
  return getIPAddress(useIPv4);
}
@RequiresPermission(INTERNET) @Override public String doInBackground(){
  return getDomainAddress(domain);
}
