@Test public void testSetXAOn(){
  Assert.assertEquals(ServerParseSet.XA_FLAG_ON,ServerParseSet.parse("set xa=on",3));
  Assert.assertEquals(ServerParseSet.XA_FLAG_ON,ServerParseSet.parse("set xa = on",3));
  Assert.assertEquals(ServerParseSet.XA_FLAG_ON,ServerParseSet.parse("set xa \t\n\r = \t\n\r on",3));
}
