/** 
 * 关联表
 * @author Ben
 */
public static class RelaTable {
  private String tableName;
  private int count;
  public RelaTable(  String tableName,  int count){
    super();
    this.tableName=tableName;
    this.count=count;
  }
  public String getTableName(){
    return this.tableName;
  }
  public int getCount(){
    return this.count;
  }
  public void incCount(){
    this.count++;
  }
}
