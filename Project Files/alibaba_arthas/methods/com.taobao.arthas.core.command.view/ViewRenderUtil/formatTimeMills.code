private static String formatTimeMills(long timeMills){
  long seconds=timeMills / 1000;
  long mills=timeMills % 1000;
  long min=seconds / 60;
  seconds=seconds % 60;
  String str;
  if (mills >= 100) {
    str=min + ":" + seconds+ "."+ mills;
  }
 else   if (mills >= 10) {
    str=min + ":" + seconds+ ".0"+ mills;
  }
 else {
    str=min + ":" + seconds+ ".00"+ mills;
  }
  return str;
}
