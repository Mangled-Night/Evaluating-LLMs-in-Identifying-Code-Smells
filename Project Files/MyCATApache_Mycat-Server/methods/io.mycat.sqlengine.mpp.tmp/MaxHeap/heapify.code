private void heapify(int i,int size){
  int max=0;
  int mid=size >> 1;
  while (i <= mid) {
    max=i;
    int left=i << 1;
    int right=left + 1;
    if (left < size && cmp.compare(data.get(left),data.get(i)) > 0) {
      max=left;
    }
    if (right < size && cmp.compare(data.get(right),data.get(max)) > 0) {
      max=right;
    }
    if (i == max) {
      break;
    }
    if (i != max) {
      RowDataPacket tmp=data.get(i);
      data.set(i,data.get(max));
      data.set(max,tmp);
      i=max;
    }
  }
}
