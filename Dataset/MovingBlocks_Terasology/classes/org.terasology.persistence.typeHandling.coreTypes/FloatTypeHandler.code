public class FloatTypeHandler extends TypeHandler<Float> {
  @Override public PersistedData serializeNonNull(  Float value,  PersistedDataSerializer serializer){
    return serializer.serialize(value);
  }
  @Override public Optional<Float> deserialize(  PersistedData data){
    if (data.isNumber()) {
      return Optional.of(data.getAsFloat());
    }
    return Optional.empty();
  }
}
