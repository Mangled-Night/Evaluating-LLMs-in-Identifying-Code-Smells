@Override public PGPPublicKeyRing getPublicKeyRing(BareJid owner,OpenPgpV4Fingerprint fingerprint) throws IOException, PGPException {
  PGPPublicKeyRingCollection publicKeyRings=getPublicKeysOf(owner);
  if (publicKeyRings != null) {
    return publicKeyRings.getPublicKeyRing(fingerprint.getKeyId());
  }
  return null;
}
