public static class CollisionPair {
  EntityRef a;
  EntityRef b;
  Vector3f pointA;
  Vector3f pointB;
  float distance;
  Vector3f normal;
  public CollisionPair(  EntityRef a,  EntityRef b,  Vector3f pointA,  Vector3f pointb,  float distance,  Vector3f normal){
    this.a=a;
    this.b=b;
    this.pointA=pointA;
    this.pointB=pointb;
    this.distance=distance;
    this.normal=normal;
  }
}
