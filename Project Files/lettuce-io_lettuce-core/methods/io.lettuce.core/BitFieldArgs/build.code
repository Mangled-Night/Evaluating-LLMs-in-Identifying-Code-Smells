@Override <K,V>void build(CommandArgs<K,V> args){
  args.add(CommandType.SET).add(bitFieldType.asString());
  if (bitOffset) {
    args.add("#" + offset);
  }
 else {
    args.add(offset);
  }
  args.add(value);
}
