@RequiresPermissions("admin:storage:update") @RequiresPermissionsDesc(menu={"系统管理","对象存储"},button="编辑") @PostMapping("/update") public Object update(@RequestBody LitemallStorage litemallStorage){
  if (litemallStorageService.update(litemallStorage) == 0) {
    return ResponseUtil.updatedDataFailed();
  }
  return ResponseUtil.ok(litemallStorage);
}
