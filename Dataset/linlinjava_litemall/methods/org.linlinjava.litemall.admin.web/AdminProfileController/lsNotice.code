@RequiresAuthentication @GetMapping("/lsnotice") public Object lsNotice(String title,String type,@RequestParam(defaultValue="1") Integer page,@RequestParam(defaultValue="10") Integer limit,@Sort @RequestParam(defaultValue="add_time") String sort,@Order @RequestParam(defaultValue="desc") String order){
  List<LitemallNoticeAdmin> noticeList=noticeAdminService.querySelective(title,type,getAdminId(),page,limit,sort,order);
  return ResponseUtil.okList(noticeList);
}
