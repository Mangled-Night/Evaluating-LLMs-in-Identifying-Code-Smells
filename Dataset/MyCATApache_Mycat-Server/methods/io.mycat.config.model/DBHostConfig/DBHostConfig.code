public DBHostConfig(String hostName,String ip,int port,String url,String user,String password,String encryptPassword,boolean checkAlive){
  super();
  this.hostName=hostName;
  this.ip=ip;
  this.port=port;
  this.url=url;
  this.user=user;
  this.password=password;
  this.encryptPassword=encryptPassword;
  this.checkAlive=checkAlive;
}
