@ApiOperation("修改场次") @RequestMapping(value="/update/{id}",method=RequestMethod.POST) @ResponseBody public CommonResult update(@PathVariable Long id,@RequestBody SmsFlashPromotionSession promotionSession){
  int count=flashPromotionSessionService.update(id,promotionSession);
  if (count > 0) {
    return CommonResult.success(count);
  }
  return CommonResult.failed();
}
