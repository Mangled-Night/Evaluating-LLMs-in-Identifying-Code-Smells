@Override public boolean accept(Stanza packet){
  if (!iqAndIdFilter.accept(packet))   return false;
  if (fromFilter.accept(packet)) {
    return true;
  }
 else {
    String msg=String.format("Rejected potentially spoofed reply to IQ-packet. Filter settings: " + "packetId=%s, to=%s, local=%s, server=%s. Received packet with from=%s",packetId,to,local,server,packet.getFrom());
    LOGGER.log(Level.WARNING,msg,packet);
    return false;
  }
}
