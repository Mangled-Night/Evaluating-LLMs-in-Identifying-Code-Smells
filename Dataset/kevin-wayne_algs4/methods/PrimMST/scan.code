private void scan(EdgeWeightedGraph G,int v){
  marked[v]=true;
  for (  Edge e : G.adj(v)) {
    int w=e.other(v);
    if (marked[w])     continue;
    if (e.weight() < distTo[w]) {
      distTo[w]=e.weight();
      edgeTo[w]=e;
      if (pq.contains(w))       pq.decreaseKey(w,distTo[w]);
 else       pq.insert(w,distTo[w]);
    }
  }
}
