static int commitOrCallCheckOrCreate(String stmt,int offset){
  int sqlType=OTHER;
switch (stmt.charAt((offset + 1))) {
case 'O':
case 'o':
    sqlType=commitCheck(stmt,offset);
  break;
case 'A':
case 'a':
sqlType=callCheck(stmt,offset);
break;
case 'R':
case 'r':
sqlType=createCheck(stmt,offset);
break;
default :
sqlType=OTHER;
}
return sqlType;
}
