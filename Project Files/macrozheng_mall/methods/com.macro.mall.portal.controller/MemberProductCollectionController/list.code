@ApiOperation("显示当前用户商品收藏列表") @RequestMapping(value="/list",method=RequestMethod.GET) @ResponseBody public CommonResult<CommonPage<MemberProductCollection>> list(@RequestParam(value="pageNum",defaultValue="1") Integer pageNum,@RequestParam(value="pageSize",defaultValue="5") Integer pageSize){
  Page<MemberProductCollection> page=memberCollectionService.list(pageNum,pageSize);
  return CommonResult.success(CommonPage.restPage(page));
}
