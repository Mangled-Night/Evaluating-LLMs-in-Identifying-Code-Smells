@RequiresPermissions("admin:storage:delete") @RequiresPermissionsDesc(menu={"系统管理","对象存储"},button="删除") @PostMapping("/delete") public Object delete(@RequestBody LitemallStorage litemallStorage){
  String key=litemallStorage.getKey();
  if (StringUtils.isEmpty(key)) {
    return ResponseUtil.badArgument();
  }
  litemallStorageService.deleteByKey(key);
  storageService.delete(key);
  return ResponseUtil.ok();
}
