@ApiOperation(value="更新品牌") @RequestMapping(value="/update/{id}",method=RequestMethod.POST) @ResponseBody public CommonResult update(@PathVariable("id") Long id,@Validated @RequestBody PmsBrandParam pmsBrandParam){
  CommonResult commonResult;
  int count=brandService.updateBrand(id,pmsBrandParam);
  if (count == 1) {
    commonResult=CommonResult.success(count);
  }
 else {
    commonResult=CommonResult.failed();
  }
  return commonResult;
}
