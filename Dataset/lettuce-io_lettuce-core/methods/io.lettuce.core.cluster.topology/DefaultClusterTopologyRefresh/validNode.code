private static boolean validNode(RedisClusterNode redisClusterNode){
  if (redisClusterNode.is(RedisClusterNode.NodeFlag.NOADDR)) {
    return false;
  }
  if (redisClusterNode.getUri() == null || redisClusterNode.getUri().getPort() == 0 || LettuceStrings.isEmpty(redisClusterNode.getUri().getHost())) {
    return false;
  }
  return true;
}
