@Test void testMasterSlaveSentinelClosesSentinelConnections(){
  ChannelGroup channels=(ChannelGroup)ReflectionTestUtils.getField(redisClient,"channels");
  int count=channels.size();
  StatefulRedisMasterSlaveConnection<String,String> connection=MasterSlave.connect(redisClient,StringCodec.UTF8,SentinelTestSettings.SENTINEL_URI);
  connection.sync().ping();
  connection.setReadFrom(ReadFrom.REPLICA);
  slaveCall(connection);
  connection.close();
  assertThat(channels.size()).isEqualTo(count);
}
