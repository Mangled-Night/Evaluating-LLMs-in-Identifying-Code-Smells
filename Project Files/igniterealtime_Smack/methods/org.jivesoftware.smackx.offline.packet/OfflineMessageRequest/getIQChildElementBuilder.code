@Override protected IQChildElementXmlStringBuilder getIQChildElementBuilder(IQChildElementXmlStringBuilder buf){
  buf.rightAngleBracket();
synchronized (items) {
    for (    Item item : items) {
      buf.append(item.toXML());
    }
  }
  if (purge) {
    buf.append("<purge/>");
  }
  if (fetch) {
    buf.append("<fetch/>");
  }
  return buf;
}
