/** 
 * Computes the strong components of the digraph  {@code G}.
 * @param G the digraph
 */
public GabowSCC(Digraph G){
  marked=new boolean[G.V()];
  stack1=new Stack<Integer>();
  stack2=new Stack<Integer>();
  id=new int[G.V()];
  preorder=new int[G.V()];
  for (int v=0; v < G.V(); v++)   id[v]=-1;
  for (int v=0; v < G.V(); v++) {
    if (!marked[v])     dfs(G,v);
  }
  assert check(G);
}
