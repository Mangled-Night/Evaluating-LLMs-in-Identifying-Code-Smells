public String genXATXID(){
  long seq=this.xaIDInc.incrementAndGet();
  if (seq < 0) {
synchronized (xaIDInc) {
      if (xaIDInc.get() < 0) {
        xaIDInc.set(0);
      }
      seq=xaIDInc.incrementAndGet();
    }
  }
  return "'Mycat." + this.getConfig().getSystem().getMycatNodeId() + "."+ seq+ "'";
}
