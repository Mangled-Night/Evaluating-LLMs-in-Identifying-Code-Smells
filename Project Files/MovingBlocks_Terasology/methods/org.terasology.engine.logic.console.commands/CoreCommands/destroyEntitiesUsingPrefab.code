/** 
 * Removes all entities of the given prefab
 * @param prefabName String containing prefab name
 */
@Command(shortDescription="Removes all entities of the given prefab",runOnServer=true) public void destroyEntitiesUsingPrefab(@CommandParam("prefabName") String prefabName){
  Prefab prefab=entityManager.getPrefabManager().getPrefab(prefabName);
  if (prefab != null) {
    for (    EntityRef entity : entityManager.getAllEntities()) {
      if (prefab.equals(entity.getParentPrefab())) {
        entity.destroy();
      }
    }
  }
}
