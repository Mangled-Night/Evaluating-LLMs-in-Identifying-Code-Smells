@Test void shouldCompleteCommandExceptionallyOnOutputFailure() throws Exception {
  sut.channelRegistered(context);
  sut.channelActive(context);
  stack.add(command);
  sut.channelRead(context,responseBytes(":1000\r\n"));
  assertThat((Object)ReflectionTestUtils.getField(command,"exception")).isInstanceOf(UnsupportedOperationException.class);
}
