/** 
 * Get the attributes string.
 * @return the attributes.
 */
public String getAttributes(){
  StringBuilder str=new StringBuilder();
  if (getSid() != null)   str.append(" sid='").append(getSid()).append('\'');
  if (getPass() != null)   str.append(" pass='").append(getPass()).append('\'');
  if (getPortA() != -1)   str.append(" porta='").append(getPortA()).append('\'');
  if (getPortB() != -1)   str.append(" portb='").append(getPortB()).append('\'');
  if (getHostA() != null)   str.append(" hosta='").append(getHostA()).append('\'');
  if (getHostB() != null)   str.append(" hostb='").append(getHostB()).append('\'');
  return str.toString();
}
