/** 
 * Writes the 8-bit byte to standard output.
 */
private static void writeByte(int x){
  if (!isInitialized)   initialize();
  assert x >= 0 && x < 256;
  if (n == 0) {
    try {
      out.write(x);
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
    return;
  }
  for (int i=0; i < 8; i++) {
    boolean bit=((x >>> (8 - i - 1)) & 1) == 1;
    writeBit(bit);
  }
}
