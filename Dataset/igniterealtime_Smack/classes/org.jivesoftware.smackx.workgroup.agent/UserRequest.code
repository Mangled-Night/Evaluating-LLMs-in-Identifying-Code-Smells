/** 
 * Requests made by users to get support by some agent.
 * @author Gaston Dombiak
 */
public class UserRequest extends OfferContent {
  private static final UserRequest instance=new UserRequest();
  public static OfferContent getInstance(){
    return instance;
  }
  @Override boolean isUserRequest(){
    return true;
  }
  @Override boolean isInvitation(){
    return false;
  }
  @Override boolean isTransfer(){
    return false;
  }
}
