@Test public void testDelimitedListToStringArray(){
  Assert.assertArrayEquals(new String[]{},StringUtils.delimitedListToStringArray("",">",""));
  Assert.assertArrayEquals(new String[]{"r662"},StringUtils.delimitedListToStringArray("r662",">>>>",""));
  Assert.assertArrayEquals(new String[]{},StringUtils.delimitedListToStringArray("","",""));
  Assert.assertArrayEquals(new String[]{"foo","br","bz"},StringUtils.delimitedListToStringArray("foo>bar>baz",">","a"));
}
