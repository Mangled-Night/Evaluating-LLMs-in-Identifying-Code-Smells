public static long getLong(final JSONObject jsonObject,final String key){
  return getLong(jsonObject,key,-1);
}
public static long getLong(final JSONObject jsonObject,final String key,final long defaultValue){
  return getValueByType(jsonObject,key,defaultValue,TYPE_LONG);
}
public static long getLong(final String json,final String key){
  return getLong(json,key,-1);
}
public static long getLong(final String json,final String key,final long defaultValue){
  return getValueByType(json,key,defaultValue,TYPE_LONG);
}
