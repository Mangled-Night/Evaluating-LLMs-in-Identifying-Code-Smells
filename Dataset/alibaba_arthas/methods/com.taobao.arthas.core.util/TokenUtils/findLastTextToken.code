/** 
 * find the last text token
 */
public static CliToken findLastTextToken(List<CliToken> tokens){
  if (tokens == null || tokens.isEmpty()) {
    return null;
  }
  for (int i=tokens.size() - 1; i >= 0; i--) {
    CliToken token=tokens.get(i);
    if (token != null && token.isText()) {
      return token;
    }
  }
  return null;
}
