private boolean isScreenValid(){
  if (Stream.of(gameModules,gameWorlds,blocks,description,descriptionTitle,openModuleDetails,previewSlideshow,tabs,showErrors,close,slideLeft,slideRight,slideStop,title).anyMatch(Objects::isNull)) {
    logger.error("Can't initialize screen correctly. At least one widget was missed!");
    return false;
  }
  return true;
}
