/** 
 * find the first text token
 */
public static CliToken findFirstTextToken(List<CliToken> tokens){
  if (tokens == null || tokens.isEmpty()) {
    return null;
  }
  CliToken first=null;
  for (  CliToken token : tokens) {
    if (token != null && token.isText()) {
      first=token;
      break;
    }
  }
  return first;
}
