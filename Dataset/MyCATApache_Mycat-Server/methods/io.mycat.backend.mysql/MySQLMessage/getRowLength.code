public int getRowLength(int fileldCount){
  int size=0;
  int bak_position=position;
  position+=4;
  for (int i=0; i < fileldCount; i++) {
    int length=(int)readLength();
    if (length == NULL_LENGTH || length <= 0) {
      continue;
    }
    position+=length;
    size+=length;
  }
  position=bak_position;
  return size;
}
