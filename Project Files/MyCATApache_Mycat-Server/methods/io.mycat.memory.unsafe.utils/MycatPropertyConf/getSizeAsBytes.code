public long getSizeAsBytes(String s,long i){
  String value=(String)settings.get(s);
  if (value != null) {
    return byteStringAsBytes(value);
  }
  return i;
}
public long getSizeAsBytes(String s,String defaultValue){
  String value=(String)settings.get(s);
  if (value != null) {
    return byteStringAsBytes(value);
  }
  return byteStringAsBytes(defaultValue);
}
