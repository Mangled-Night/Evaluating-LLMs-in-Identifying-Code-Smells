@ApiOperation("修改指定用户信息") @RequestMapping(value="/update/{id}",method=RequestMethod.POST) @ResponseBody public CommonResult update(@PathVariable Long id,@RequestBody UmsAdmin admin){
  int count=adminService.update(id,admin);
  if (count > 0) {
    return CommonResult.success(count);
  }
  return CommonResult.failed();
}
