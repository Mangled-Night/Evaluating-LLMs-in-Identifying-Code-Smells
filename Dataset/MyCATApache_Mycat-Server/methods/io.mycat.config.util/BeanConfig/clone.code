@Override public Object clone(){
  try {
    super.clone();
  }
 catch (  CloneNotSupportedException e) {
    throw new ConfigException(e);
  }
  BeanConfig bc=null;
  try {
    bc=getClass().newInstance();
  }
 catch (  InstantiationException e) {
    throw new ConfigException(e);
  }
catch (  IllegalAccessException e) {
    throw new ConfigException(e);
  }
  bc.className=className;
  bc.name=name;
  return bc;
}
