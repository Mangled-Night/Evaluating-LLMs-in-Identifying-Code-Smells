@Test void bitfield(){
  BitFieldArgs bitFieldArgs=BitFieldArgs.Builder.set(signed(8),0,1).set(5,1).incrBy(2,3).get().get(2);
  StepVerifier.create(reactive.bitfield(key,bitFieldArgs)).expectNext(Value.just(0L),Value.just(32L),Value.just(3L),Value.just(0L),Value.just(3L)).verifyComplete();
  assertThat(bitstring.get(key)).isEqualTo("0000000000010011");
}
