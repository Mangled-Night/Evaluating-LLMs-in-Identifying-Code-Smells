public Connection getConnection() throws SQLException {
  Connection con=null;
  if (cons.isEmpty()) {
    System.out.println("warn no connection in pool,create new one");
    con=getCon();
    return con;
  }
 else {
    con=cons.remove(0);
  }
  if (con.isClosed()) {
    System.out.println("warn connection closed ,create new one");
    con=getCon();
  }
  return con;
}
