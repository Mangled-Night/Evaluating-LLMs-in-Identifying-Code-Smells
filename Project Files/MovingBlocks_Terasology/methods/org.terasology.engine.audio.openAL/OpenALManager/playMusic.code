@Override public void playMusic(StreamingSound music){
  playMusic(music,1.0f,null);
}
@Override public void playMusic(StreamingSound music,AudioEndListener endListener){
  playMusic(music,1.0f,endListener);
}
@Override public void playMusic(StreamingSound music,float volume){
  playMusic(music,volume,null);
}
@Override public void playMusic(StreamingSound music,float volume,AudioEndListener endListener){
  SoundPool<StreamingSound,?> pool=(SoundPool<StreamingSound,?>)pools.get("music");
  pool.stopAll();
  if (music == null) {
    return;
  }
  SoundSource<?> source=pool.getSource(music);
  if (source != null) {
    source.setGain(volume).play();
    if (endListener != null) {
      endListeners.put(source,endListener);
    }
  }
}
