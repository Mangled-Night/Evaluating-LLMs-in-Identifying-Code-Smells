protected void heapifyRecursive(int i,int size){
  int l=left(i);
  int r=right(i);
  int max=i;
  if (l < size && cmp.compare(data.get(l),data.get(i)) > 0) {
    max=l;
  }
  if (r < size && cmp.compare(data.get(r),data.get(max)) > 0) {
    max=r;
  }
  if (i == max) {
    return;
  }
  swap(i,max);
  heapifyRecursive(max,size);
}
