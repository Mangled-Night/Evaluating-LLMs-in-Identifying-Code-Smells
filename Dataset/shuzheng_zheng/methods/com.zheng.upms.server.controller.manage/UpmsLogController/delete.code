@ApiOperation(value="删除日志") @RequiresPermissions("upms:log:delete") @RequestMapping(value="/delete/{ids}",method=RequestMethod.GET) @ResponseBody public Object delete(@PathVariable("ids") String ids){
  int count=upmsLogService.deleteByPrimaryKeys(ids);
  return new UpmsResult(UpmsResultConstant.SUCCESS,count);
}
