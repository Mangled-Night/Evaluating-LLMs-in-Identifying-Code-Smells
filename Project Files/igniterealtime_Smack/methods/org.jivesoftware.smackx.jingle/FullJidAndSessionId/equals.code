@Override public boolean equals(Object other){
  if (!(other instanceof FullJidAndSessionId)) {
    return false;
  }
  FullJidAndSessionId otherFullJidAndSessionId=(FullJidAndSessionId)other;
  return fullJid.equals(otherFullJidAndSessionId.fullJid) && sessionId.equals(otherFullJidAndSessionId.sessionId);
}
