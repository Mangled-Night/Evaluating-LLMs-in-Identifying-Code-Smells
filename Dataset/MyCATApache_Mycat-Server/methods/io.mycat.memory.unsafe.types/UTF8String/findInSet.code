public int findInSet(UTF8String match){
  if (match.contains(COMMA_UTF8)) {
    return 0;
  }
  int n=1, lastComma=-1;
  for (int i=0; i < numBytes; i++) {
    if (getByte(i) == (byte)',') {
      if (i - (lastComma + 1) == match.numBytes && ByteArrayMethods.arrayEquals(base,offset + (lastComma + 1),match.base,match.offset,match.numBytes)) {
        return n;
      }
      lastComma=i;
      n++;
    }
  }
  if (numBytes - (lastComma + 1) == match.numBytes && ByteArrayMethods.arrayEquals(base,offset + (lastComma + 1),match.base,match.offset,match.numBytes)) {
    return n;
  }
  return 0;
}
