private String parseFileName(String sql){
  if (sql.contains("'")) {
    int beginIndex=sql.indexOf("'");
    return sql.substring(beginIndex + 1,sql.indexOf("'",beginIndex + 1));
  }
 else   if (sql.contains("\"")) {
    int beginIndex=sql.indexOf("\"");
    return sql.substring(beginIndex + 1,sql.indexOf("\"",beginIndex + 1));
  }
  return null;
}
