public static double getDouble(final JSONObject jsonObject,final String key){
  return getDouble(jsonObject,key,-1);
}
public static double getDouble(final JSONObject jsonObject,final String key,final double defaultValue){
  return getValueByType(jsonObject,key,defaultValue,TYPE_DOUBLE);
}
public static double getDouble(final String json,final String key){
  return getDouble(json,key,-1);
}
public static double getDouble(final String json,final String key,final double defaultValue){
  return getValueByType(json,key,defaultValue,TYPE_DOUBLE);
}
