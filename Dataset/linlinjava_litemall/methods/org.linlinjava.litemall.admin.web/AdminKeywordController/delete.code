@RequiresPermissions("admin:keyword:delete") @RequiresPermissionsDesc(menu={"商场管理","关键词"},button="删除") @PostMapping("/delete") public Object delete(@RequestBody LitemallKeyword keyword){
  Integer id=keyword.getId();
  if (id == null) {
    return ResponseUtil.badArgument();
  }
  keywordService.deleteById(id);
  return ResponseUtil.ok();
}
