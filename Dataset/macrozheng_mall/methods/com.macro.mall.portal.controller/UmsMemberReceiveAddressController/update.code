@ApiOperation("修改收货地址") @RequestMapping(value="/update/{id}",method=RequestMethod.POST) @ResponseBody public CommonResult update(@PathVariable Long id,@RequestBody UmsMemberReceiveAddress address){
  int count=memberReceiveAddressService.update(id,address);
  if (count > 0) {
    return CommonResult.success(count);
  }
  return CommonResult.failed();
}
