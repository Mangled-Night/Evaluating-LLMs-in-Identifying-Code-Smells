@Override public String toString(){
  if (stringCache == null) {
    StringBuilder sb=new StringBuilder(128);
    sb.append("Stanza Collector filter='").append(packetFilter).append('\'');
    if (request != null) {
      sb.append(" request='").append(request).append('\'');
    }
    stringCache=sb.toString();
  }
  return stringCache;
}
