/** 
 * 清除SpyAPI里的引用
 */
private void cleanUpSpyReference(){
  try {
    SpyAPI.setNopSpy();
    SpyAPI.destroy();
  }
 catch (  Throwable e) {
  }
  try {
    Class<?> clazz=ClassLoader.getSystemClassLoader().loadClass("com.taobao.arthas.agent334.AgentBootstrap");
    Method method=clazz.getDeclaredMethod("resetArthasClassLoader");
    method.invoke(null);
  }
 catch (  Throwable e) {
  }
}
