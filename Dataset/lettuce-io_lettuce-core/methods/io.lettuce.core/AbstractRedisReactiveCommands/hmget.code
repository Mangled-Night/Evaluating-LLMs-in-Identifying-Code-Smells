@Override public Flux<KeyValue<K,V>> hmget(K key,K... fields){
  return createDissolvingFlux(() -> commandBuilder.hmgetKeyValue(key,fields));
}
@Override public Mono<Long> hmget(KeyValueStreamingChannel<K,V> channel,K key,K... fields){
  return createMono(() -> commandBuilder.hmget(channel,key,fields));
}
