@Override public DefaultConnectionFuture<Void> acceptEitherAsync(CompletionStage<? extends T> other,Consumer<? super T> action){
  return adopt(delegate.acceptEitherAsync(other,action));
}
@Override public DefaultConnectionFuture<Void> acceptEitherAsync(CompletionStage<? extends T> other,Consumer<? super T> action,Executor executor){
  return adopt(delegate.acceptEitherAsync(other,action,executor));
}
