/** 
 * Find the root of a module build directory. <p> If  {@code path} matches a known build directory sub-path, return the base directory.<p> Example: <ul> <li>findModuleRoot("foo/bar.txt", "/some/base/foo/bar.txt") == "/some/base" <li>findModuleRoot("foo/bar.txt", "/some/base/x/foo/bar.txt") == "/some/base/x" <li>findModuleRoot("foo/bar.txt", "/some/base/foo/x/bar.txt") == <i>Empty</i> <li>findModuleRoot("foo/bar.txt", "/some/base/foo/baz.txt") == <i>Empty</i> </ul>
 */
private static Optional<Path> findModuleRoot(Path relativePathFromModuleRoot,Path path){
  if (path.endsWith(relativePathFromModuleRoot)) {
    int relativeDepth=relativePathFromModuleRoot.getNameCount();
    Path parentPath=path.subpath(0,path.getNameCount() - relativeDepth);
    if (path.getRoot() != null) {
      parentPath=path.getRoot().resolve(parentPath);
    }
    return Optional.of(parentPath);
  }
 else {
    logger.warn("does not seem to be in a build directory {}",path);
    return Optional.empty();
  }
}
