public static boolean isEOF(char c){
  return (c == ' ' || c == '\t' || c == '\n' || c == '\r' || c == ';');
}
public static boolean isEOF(String stmt,int offset){
  for (; offset < stmt.length(); offset++) {
    if (!ParseUtil.isEOF(stmt.charAt(offset))) {
      return false;
    }
  }
  return true;
}
