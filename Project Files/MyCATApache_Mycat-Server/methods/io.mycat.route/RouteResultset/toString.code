@Override public String toString(){
  StringBuilder s=new StringBuilder();
  s.append(statement).append(", route={");
  if (nodes != null) {
    for (int i=0; i < nodes.length; ++i) {
      s.append("\n ").append(FormatUtil.format(i + 1,3));
      s.append(" -> ").append(nodes[i]);
    }
  }
  s.append("\n}");
  return s.toString();
}
