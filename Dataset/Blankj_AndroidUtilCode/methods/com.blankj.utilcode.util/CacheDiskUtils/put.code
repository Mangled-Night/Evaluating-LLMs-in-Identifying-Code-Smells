private void put(final File file){
  cacheCount.addAndGet(1);
  cacheSize.addAndGet(file.length());
  while (cacheCount.get() > countLimit || cacheSize.get() > sizeLimit) {
    cacheSize.addAndGet(-removeOldest());
    cacheCount.addAndGet(-1);
  }
}
