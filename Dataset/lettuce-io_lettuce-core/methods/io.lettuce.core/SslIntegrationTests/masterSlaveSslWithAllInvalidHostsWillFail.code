@Test void masterSlaveSslWithAllInvalidHostsWillFail(){
  SslOptions sslOptions=SslOptions.builder().jdkSslProvider().truststore(TRUSTSTORE_FILE).build();
  setOptions(sslOptions);
  assertThatThrownBy(() -> verifyMasterSlaveConnection(MASTER_SLAVE_URIS_WITH_ALL_INVALID)).isInstanceOf(RedisConnectionException.class);
}
