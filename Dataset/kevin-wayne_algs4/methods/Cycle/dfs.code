private void dfs(Graph G,int u,int v){
  marked[v]=true;
  for (  int w : G.adj(v)) {
    if (cycle != null)     return;
    if (!marked[w]) {
      edgeTo[w]=v;
      dfs(G,v,w);
    }
 else     if (w != u) {
      cycle=new Stack<Integer>();
      for (int x=v; x != w; x=edgeTo[x]) {
        cycle.push(x);
      }
      cycle.push(w);
      cycle.push(v);
    }
  }
}
