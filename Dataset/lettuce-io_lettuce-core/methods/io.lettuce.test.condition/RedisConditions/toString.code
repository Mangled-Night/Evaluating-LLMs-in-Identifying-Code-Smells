@Override public String toString(){
  List<Integer> digits=new ArrayList<>();
  digits.add(major);
  digits.add(minor);
  if (build != 0 || bugfix != 0) {
    digits.add(bugfix);
  }
  if (build != 0) {
    digits.add(build);
  }
  return digits.stream().map(Object::toString).collect(Collectors.joining("."));
}
