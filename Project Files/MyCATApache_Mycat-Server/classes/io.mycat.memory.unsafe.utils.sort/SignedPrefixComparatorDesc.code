public static final class SignedPrefixComparatorDesc extends RadixSortSupport {
  @Override public boolean sortDescending(){
    return true;
  }
  @Override public boolean sortSigned(){
    return true;
  }
  @Override public int compare(  long b,  long a){
    return (a < b) ? -1 : (a > b) ? 1 : 0;
  }
}
