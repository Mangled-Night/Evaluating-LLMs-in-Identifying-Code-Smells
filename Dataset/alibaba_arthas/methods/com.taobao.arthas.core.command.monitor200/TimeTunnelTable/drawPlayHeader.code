public static TableElement drawPlayHeader(String className,String methodName,String objectAddress,int index,TableElement table){
  SimpleDateFormat sdf=new SimpleDateFormat("yyyy-MM-dd HH:mm:ss");
  return table.row("RE-INDEX","" + index).row("GMT-REPLAY",sdf.format(new Date())).row("OBJECT",objectAddress).row("CLASS",className).row("METHOD",methodName);
}
