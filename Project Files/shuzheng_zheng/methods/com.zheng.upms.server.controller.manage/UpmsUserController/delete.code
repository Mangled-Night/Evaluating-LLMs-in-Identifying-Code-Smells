@ApiOperation(value="删除用户") @RequiresPermissions("upms:user:delete") @RequestMapping(value="/delete/{ids}",method=RequestMethod.GET) @ResponseBody public Object delete(@PathVariable("ids") String ids){
  int count=upmsUserService.deleteByPrimaryKeys(ids);
  return new UpmsResult(UpmsResultConstant.SUCCESS,count);
}
