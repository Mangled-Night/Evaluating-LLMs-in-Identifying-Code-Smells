@Test public void parseShowTableTest(){
  String stmt="SHOW FULL TABLES FROM schema001 WHERE Tables_in_schema001 LIKE 'tab_company'; ";
  String[] fields=SchemaUtil.parseShowTable(stmt);
  assertArrayEquals(new String[]{"1","FULL","FROM","schema001","WHERE","Tables_in_schema001","LIKE","'tab_company'","tab_company"},fields);
}
