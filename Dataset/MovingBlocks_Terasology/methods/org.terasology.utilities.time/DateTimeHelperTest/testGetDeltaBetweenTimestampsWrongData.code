@Test public void testGetDeltaBetweenTimestampsWrongData(){
  Integer[][] wrongData={{-1,50000},{123,-1000},{-667,-1000},{123,0},{12321321,1231}};
  assertAll(Arrays.stream(wrongData).map((data) -> () -> assertEquals("Wrong timestamp values: " + data[0] + " or "+ data[1],assertThrows(IllegalArgumentException.class,() -> DateTimeHelper.getDeltaBetweenTimestamps(data[0],data[1])).getMessage())));
}
