@ApiOperation("修改购物车中商品的规格") @RequestMapping(value="/update/attr",method=RequestMethod.POST) @ResponseBody public CommonResult updateAttr(@RequestBody OmsCartItem cartItem){
  int count=cartItemService.updateAttr(cartItem);
  if (count > 0) {
    return CommonResult.success(count);
  }
  return CommonResult.failed();
}
