@ApiOperation(value="删除权限") @RequiresPermissions("upms:permission:delete") @RequestMapping(value="/delete/{ids}",method=RequestMethod.GET) @ResponseBody public Object delete(@PathVariable("ids") String ids){
  int count=upmsPermissionService.deleteByPrimaryKeys(ids);
  return new UpmsResult(UpmsResultConstant.SUCCESS,count);
}
