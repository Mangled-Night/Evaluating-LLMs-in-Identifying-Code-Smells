@Override protected void addXml(XmlStringBuilder xml){
  if ((items == null) || (items.size() == 0)) {
    xml.closeEmptyElement();
    return;
  }
  if (notify != null) {
    xml.attribute(type.getElementAttribute(),notify);
    xml.rightAngleBracket();
  }
 else {
    xml.rightAngleBracket();
    xml.append(items);
  }
  xml.closeElement(this);
}
