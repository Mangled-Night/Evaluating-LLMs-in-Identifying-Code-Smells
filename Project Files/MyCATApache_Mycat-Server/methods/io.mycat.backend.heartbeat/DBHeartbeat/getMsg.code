protected String getMsg(int status){
switch (status) {
case DBHeartbeat.INIT_STATUS:
    return "init status";
case DBHeartbeat.TIMEOUT_STATUS:
  return "timeout status";
case DBHeartbeat.OK_STATUS:
return "ok status";
case DBHeartbeat.ERROR_STATUS:
return "error status";
default :
return "unknown status";
}
}
