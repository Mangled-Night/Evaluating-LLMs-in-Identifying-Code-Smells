private boolean isPerfectMatching(){
  boolean[] perm=new boolean[n];
  for (int i=0; i < n; i++) {
    if (perm[xy[i]]) {
      StdOut.println("Not a perfect matching");
      return false;
    }
    perm[xy[i]]=true;
  }
  for (int j=0; j < n; j++) {
    if (xy[yx[j]] != j) {
      StdOut.println("xy[] and yx[] are not inverses");
      return false;
    }
  }
  for (int i=0; i < n; i++) {
    if (yx[xy[i]] != i) {
      StdOut.println("xy[] and yx[] are not inverses");
      return false;
    }
  }
  return true;
}
