@Produces(Facet1.class) @Requires(@Facet(value=Facet2.class,border=@FacetBorder(sides=2))) public static class Facet1Provider implements FacetProvider {
  @Override public void process(  GeneratingRegion region){
    Facet1 facet=new Facet1(region.getRegion(),region.getBorderForFacet(Facet1.class));
    region.setRegionFacet(Facet1.class,facet);
  }
}
