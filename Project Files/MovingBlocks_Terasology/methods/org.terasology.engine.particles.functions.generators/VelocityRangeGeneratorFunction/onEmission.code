@Override public void onEmission(final VelocityRangeGeneratorComponent component,final ParticleData particleData,final Random random){
  particleData.velocity.set(random.nextFloat(component.minVelocity.x(),component.maxVelocity.x()),random.nextFloat(component.minVelocity.y(),component.maxVelocity.y()),random.nextFloat(component.minVelocity.z(),component.maxVelocity.z()));
}
