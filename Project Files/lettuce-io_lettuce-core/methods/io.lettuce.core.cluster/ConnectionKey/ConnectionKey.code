public ConnectionKey(ConnectionIntent connectionIntent,String nodeId){
  this.connectionIntent=connectionIntent;
  this.nodeId=nodeId;
  this.host=null;
  this.port=0;
}
public ConnectionKey(ConnectionIntent connectionIntent,String host,int port){
  this.connectionIntent=connectionIntent;
  this.host=host;
  this.port=port;
  this.nodeId=null;
}
