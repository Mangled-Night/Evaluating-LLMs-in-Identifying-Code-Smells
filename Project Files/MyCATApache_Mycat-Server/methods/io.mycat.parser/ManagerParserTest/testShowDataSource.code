@Test public void testShowDataSource(){
  Assert.assertEquals(ManagerParseShow.DATASOURCE,ManagerParseShow.parse("show @@datasource",5));
  Assert.assertEquals(ManagerParseShow.DATASOURCE,ManagerParseShow.parse("SHOW @@DATASOURCE",5));
  Assert.assertEquals(ManagerParseShow.DATASOURCE,ManagerParseShow.parse(" show  @@DATASOURCE ",5));
  Assert.assertEquals(ManagerParseShow.DATASOURCE,ManagerParseShow.parse(" show  @@DATASOURCE   ",5));
  Assert.assertEquals(ManagerParseShow.DATASOURCE_WHERE,0xff & ManagerParseShow.parse(" show  @@DATASOURCE where datanode = 1",5));
  Assert.assertEquals(ManagerParseShow.DATASOURCE_WHERE,0xff & ManagerParseShow.parse(" show  @@DATASOURCE where datanode=1",5));
  Assert.assertEquals(ManagerParseShow.DATASOURCE_WHERE,0xff & ManagerParseShow.parse(" show  @@DATASOURCE WHERE datanode = 1",5));
  Assert.assertEquals(ManagerParseShow.DATASOURCE_WHERE,0xff & ManagerParseShow.parse(" show  @@DATASOURCE where DATAnode= 1 ",5));
}
