public class BoBInfo {
  private final Set<ContentId> hashes;
  private final BoBData data;
  BoBInfo(  Set<ContentId> hashes,  BoBData data){
    this.hashes=hashes;
    this.data=data;
  }
  public Set<ContentId> getHashes(){
    return hashes;
  }
  public BoBData getData(){
    return data;
  }
}
