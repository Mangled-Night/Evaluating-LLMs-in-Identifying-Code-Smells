@Override public void remove(){
  if (!next) {
    throw new IllegalStateException();
  }
switch (size) {
case 0:
    throw new IllegalStateException();
case 1:
  size=i=0;
single=null;
if (list != null && !list.isEmpty()) {
list.remove(0);
}
break;
default :
list.remove(--i);
if (--size == 1) {
single=list.get(0);
}
break;
}
next=false;
}
