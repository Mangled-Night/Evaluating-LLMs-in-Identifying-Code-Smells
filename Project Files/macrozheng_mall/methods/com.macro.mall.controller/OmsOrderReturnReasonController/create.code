@ApiOperation("添加退货原因") @RequestMapping(value="/create",method=RequestMethod.POST) @ResponseBody public CommonResult create(@RequestBody OmsOrderReturnReason returnReason){
  int count=orderReturnReasonService.create(returnReason);
  if (count > 0) {
    return CommonResult.success(count);
  }
  return CommonResult.failed();
}
