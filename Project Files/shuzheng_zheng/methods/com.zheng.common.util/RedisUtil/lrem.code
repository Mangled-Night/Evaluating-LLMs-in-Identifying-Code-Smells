/** 
 * lrem
 * @param key
 * @param count
 * @param value
 */
public synchronized static void lrem(String key,long count,String value){
  try {
    Jedis jedis=RedisUtil.getJedis();
    jedis.lrem(key,count,value);
    jedis.close();
  }
 catch (  Exception e) {
    LOGGER.error("lpush error : " + e);
  }
}
