@ApiOperation("修改商品分类") @RequestMapping(value="/update/{id}",method=RequestMethod.POST) @ResponseBody public CommonResult update(@PathVariable Long id,@Validated @RequestBody PmsProductCategoryParam productCategoryParam){
  int count=productCategoryService.update(id,productCategoryParam);
  if (count > 0) {
    return CommonResult.success(count);
  }
 else {
    return CommonResult.failed();
  }
}
