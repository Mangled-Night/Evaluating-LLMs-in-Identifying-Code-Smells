private void show(){
  StdOut.println("m = " + m);
  StdOut.println("n = " + n);
  for (int i=0; i <= m; i++) {
    for (int j=0; j <= m + n; j++) {
      StdOut.printf("%7.2f ",a[i][j]);
    }
    StdOut.println();
  }
  StdOut.println("value = " + value());
  for (int i=0; i < m; i++)   if (basis[i] < n)   StdOut.println("x_" + basis[i] + " = "+ a[i][m + n]);
  StdOut.println();
}
