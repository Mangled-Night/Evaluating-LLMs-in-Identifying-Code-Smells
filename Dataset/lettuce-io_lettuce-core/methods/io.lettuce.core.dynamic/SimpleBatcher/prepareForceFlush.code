private List<RedisCommand<Object,Object,Object>> prepareForceFlush(){
  List<RedisCommand<Object,Object,Object>> batch=new ArrayList<>(Math.max(batchSize,10));
  do {
    RedisCommand<Object,Object,Object> poll=queue.poll();
    assert poll != null;
    batch.add(poll);
  }
 while (!queue.isEmpty());
  return batch;
}
