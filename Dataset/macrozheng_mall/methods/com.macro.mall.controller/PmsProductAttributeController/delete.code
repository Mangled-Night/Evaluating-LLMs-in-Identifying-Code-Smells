@ApiOperation("批量删除商品属性") @RequestMapping(value="/delete",method=RequestMethod.POST) @ResponseBody public CommonResult delete(@RequestParam("ids") List<Long> ids){
  int count=productAttributeService.delete(ids);
  if (count > 0) {
    return CommonResult.success(count);
  }
 else {
    return CommonResult.failed();
  }
}
