/** 
 * Convert  {@link String} to {@code double}. If  {@code s} is {@literal +inf}/ {@literal -inf}, returns positive/negative infinity. If  {@code s} is {@literal +nan}/ {@literal -nan}, returns NaN.
 * @param s string representation of the number
 * @return the {@code double} value.
 * @since 4.3.3
 */
public static double toDouble(String s){
  if ("+inf".equals(s) || "inf".equals(s)) {
    return Double.POSITIVE_INFINITY;
  }
  if ("-inf".equals(s)) {
    return Double.NEGATIVE_INFINITY;
  }
  if ("-nan".equals(s) || "nan".equals(s) || "+nan".equals(s)) {
    return Double.NaN;
  }
  return Double.parseDouble(s);
}
