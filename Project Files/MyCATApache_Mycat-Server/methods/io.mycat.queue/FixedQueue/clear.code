public void clear(){
  final E[] items=this.items;
  final ReentrantLock lock=this.lock;
  lock.lock();
  try {
    int i=takeIndex;
    int j=count;
    while (j-- > 0) {
      items[i]=null;
      i=inc(i);
    }
    count=0;
    putIndex=0;
    takeIndex=0;
  }
  finally {
    lock.unlock();
  }
}
