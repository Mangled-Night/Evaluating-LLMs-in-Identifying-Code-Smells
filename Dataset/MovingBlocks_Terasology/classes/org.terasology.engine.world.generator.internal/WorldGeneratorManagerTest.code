class WorldGeneratorManagerTest {
  Context context;
  WorldGeneratorManager manager;
  @BeforeEach void provideContext() throws Exception {
    context=new ContextImpl();
    context.put(ModuleManager.class,ModuleManagerFactory.create());
  }
  @Test void hasUnittestWorldGenerator(){
    manager=new WorldGeneratorManager(context);
    assertThat(manager.getWorldGenerators()).comparingElementsUsing(Correspondence.transforming(WorldGeneratorInfo::getUri,"info")).contains(new SimpleUri("unittest:stub"));
  }
}
