@CallSuper @OnLifecycleEvent(Lifecycle.Event.ON_DESTROY) public void onDestroy(){
  Log.i(TAG,"destroy view: " + getClass().getSimpleName());
  removeLifecycle(this);
  for (  BasePresenter<V> presenter : mPresenterMap.values()) {
    if (presenter != null) {
      presenter.onDestroy();
    }
  }
  mPresenterMap.clear();
}
