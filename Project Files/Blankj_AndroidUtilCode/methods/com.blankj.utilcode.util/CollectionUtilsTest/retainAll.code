@Test public void retainAll(){
  ArrayList<Integer> list=CollectionUtils.newArrayList(0,1,2,3);
  Assert.assertEquals(0,CollectionUtils.retainAll(null,null).size());
  Assert.assertEquals(0,CollectionUtils.retainAll(list,null).size());
  Assert.assertEquals(0,CollectionUtils.retainAll(null,list).size());
  Assert.assertTrue(CollectionUtils.isEqualCollection(CollectionUtils.newArrayList(0,1,2,3),CollectionUtils.retainAll(list,list)));
  Assert.assertTrue(CollectionUtils.isEqualCollection(CollectionUtils.newArrayList(0,1),CollectionUtils.retainAll(list,CollectionUtils.newArrayList(0,1,6))));
}
