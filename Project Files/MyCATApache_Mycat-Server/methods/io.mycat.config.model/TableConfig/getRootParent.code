/** 
 * get root parent
 * @return
 */
public TableConfig getRootParent(){
  if (parentTC == null) {
    return null;
  }
  TableConfig preParent=parentTC;
  TableConfig parent=preParent.getParentTC();
  while (parent != null) {
    preParent=parent;
    parent=parent.getParentTC();
  }
  return preParent;
}
