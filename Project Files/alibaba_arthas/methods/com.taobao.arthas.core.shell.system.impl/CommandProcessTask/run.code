@Override public void run(){
  try {
    handler.handle(process);
  }
 catch (  Throwable t) {
    logger.error("Error during processing the command:",t);
    process.end(1,"Error during processing the command: " + t.getClass().getName() + ", message:"+ t.getMessage()+ ", please check $HOME/logs/arthas/arthas.log for more details.");
  }
}
