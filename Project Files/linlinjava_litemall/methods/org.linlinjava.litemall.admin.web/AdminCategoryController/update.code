@RequiresPermissions("admin:category:update") @RequiresPermissionsDesc(menu={"商场管理","类目管理"},button="编辑") @PostMapping("/update") public Object update(@RequestBody LitemallCategory category){
  Object error=validate(category);
  if (error != null) {
    return error;
  }
  if (categoryService.updateById(category) == 0) {
    return ResponseUtil.updatedDataFailed();
  }
  return ResponseUtil.ok();
}
