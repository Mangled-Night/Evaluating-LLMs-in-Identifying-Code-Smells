@Test void shouldDisposeIdleObjects(){
  mockCreation();
  BoundedAsyncPool<String> pool=new BoundedAsyncPool<>(factory,BoundedPoolConfig.builder().maxTotal(20).maxIdle(5).minIdle(5).build());
  assertThat(pool.getIdle()).isEqualTo(5);
  String object=TestFutures.getOrTimeout(pool.acquire());
  pool.release(object);
  assertThat(pool.getIdle()).isEqualTo(5);
  verify(factory).destroy(object);
}
