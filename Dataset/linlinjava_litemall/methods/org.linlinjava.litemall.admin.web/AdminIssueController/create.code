@RequiresPermissions("admin:issue:create") @RequiresPermissionsDesc(menu={"商场管理","通用问题"},button="添加") @PostMapping("/create") public Object create(@RequestBody LitemallIssue issue){
  Object error=validate(issue);
  if (error != null) {
    return error;
  }
  issueService.add(issue);
  return ResponseUtil.ok(issue);
}
