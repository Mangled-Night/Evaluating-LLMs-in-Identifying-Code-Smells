public void judgeIsLeader(boolean hashLeaderShip){
  if (hashLeaderShip) {
    isLeaderCount++;
    isSlaveCount=0;
  }
 else {
    isLeaderCount=0;
    isSlaveCount++;
  }
  if (isLeaderCount > 3 && !isLeader) {
    LOGGER.info(myId + " Currently run as leader");
    isLeader=true;
    isLeaderRunnable();
  }
  if (isSlaveCount > 3 && isLeader) {
    LOGGER.info(myId + " Currently run as slave");
    isLeader=false;
    notLeaderRunnable();
  }
}
