@Override public void set(ByteBuffer bytes){
  if (id == null) {
    id=decodeAscii(bytes);
    return;
  }
  if (key == null) {
    key=codec.decodeKey(bytes);
    return;
  }
  if (body == null) {
    body=new LinkedHashMap<>();
  }
  body.put(key,bytes == null ? null : codec.decodeValue(bytes));
  key=null;
}
