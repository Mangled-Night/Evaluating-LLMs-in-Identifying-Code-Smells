@ApiOperation("根据分类获取专题") @RequestMapping(value="/subjectList",method=RequestMethod.GET) @ResponseBody public CommonResult<List<CmsSubject>> getSubjectList(@RequestParam(required=false) Long cateId,@RequestParam(value="pageSize",defaultValue="4") Integer pageSize,@RequestParam(value="pageNum",defaultValue="1") Integer pageNum){
  List<CmsSubject> subjectList=homeService.getSubjectList(cateId,pageSize,pageNum);
  return CommonResult.success(subjectList);
}
