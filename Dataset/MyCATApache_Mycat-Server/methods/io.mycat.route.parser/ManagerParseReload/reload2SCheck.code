static int reload2SCheck(String stmt,int offset){
  if (stmt.length() > offset + 4) {
    char c1=stmt.charAt(++offset);
    char c2=stmt.charAt(++offset);
    char c3=stmt.charAt(++offset);
    char c4=stmt.charAt(++offset);
    char c5=stmt.charAt(++offset);
    char c6=stmt.charAt(++offset);
    if ((c1 == 'Q' || c1 == 'q') && (c2 == 'L' || c2 == 'l') && (c3 == 's' || c3 == 'S')&& (c4 == 'L' || c4 == 'l')&& (c5 == 'O' || c5 == 'o')&& (c6 == 'W' || c6 == 'w')&& stmt.length() > ++offset && stmt.charAt(offset) != ' ') {
      return SQL_SLOW;
    }
    if ((c1 == 'Q' || c1 == 'q') && (c2 == 'L' || c2 == 'l') && (c3 == 's' || c3 == 'S')&& (c4 == 'T' || c4 == 't')&& (c5 == 'A' || c5 == 'a')&& (c6 == 'T' || c6 == 't')&& stmt.length() > ++offset && stmt.charAt(offset) != ' ') {
      return SQL_STAT;
    }
    return OTHER;
  }
  return OTHER;
}
