public class BooleansUtils {
  public static boolean contains(  boolean[] array,  boolean target){
    for (    boolean b : array) {
      if (b == target) {
        return true;
      }
    }
    return false;
  }
  public static int numberOf(  boolean[] array,  boolean target){
    int res=0;
    for (    boolean b : array) {
      if (b == target) {
        res++;
      }
    }
    return res;
  }
}
