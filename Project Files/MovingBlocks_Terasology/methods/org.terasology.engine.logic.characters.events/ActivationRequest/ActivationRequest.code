protected ActivationRequest(){
}
public ActivationRequest(EntityRef instigator,boolean ownedEntityUsage,EntityRef usedOwnedEntity,boolean eventWithTarget,EntityRef target,Vector3f origin,Vector3f direction,Vector3f hitPosition,Vector3f hitNormal,int activationId){
  super(instigator);
  this.ownedEntityUsage=ownedEntityUsage;
  this.usedOwnedEntity=usedOwnedEntity;
  this.eventWithTarget=eventWithTarget;
  this.target=target;
  this.direction=direction;
  this.hitPosition=hitPosition;
  this.hitNormal=hitNormal;
  this.origin=origin;
  this.activationId=activationId;
}
