@ApiOperation("批量修改审核状态") @RequestMapping(value="/update/verifyStatus",method=RequestMethod.POST) @ResponseBody public CommonResult updateVerifyStatus(@RequestParam("ids") List<Long> ids,@RequestParam("verifyStatus") Integer verifyStatus,@RequestParam("detail") String detail){
  int count=productService.updateVerifyStatus(ids,verifyStatus,detail);
  if (count > 0) {
    return CommonResult.success(count);
  }
 else {
    return CommonResult.failed();
  }
}
