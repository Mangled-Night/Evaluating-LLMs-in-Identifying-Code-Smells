/** 
 * A collection of constants for different color treeView.
 */
public final class ColorModels {
  /** 
 * The default color model for OpenGL
 */
  public static final DirectColorModel RGBA=new DirectColorModel(32,0xFF000000,0xFF0000,0xFF00,0xFF);
  /** 
 * The default Swing color model for BufferedImage.TYPE_INT_ARGB
 */
  public static final DirectColorModel ARGB=new DirectColorModel(32,0x00ff0000,0x0000ff00,0x000000ff,0xff000000);
  private ColorModels(){
  }
}
