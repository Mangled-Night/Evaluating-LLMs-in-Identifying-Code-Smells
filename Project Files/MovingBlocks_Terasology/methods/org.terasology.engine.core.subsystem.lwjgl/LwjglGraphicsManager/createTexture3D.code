public void createTexture3D(ByteBuffer alignedBuffer,Texture.WrapMode wrapMode,Texture.FilterMode filterMode,int size,Consumer<Integer> idConsumer){
  asynchToDisplayThread(() -> {
    int id=glGenTextures();
    reloadTexture3D(id,alignedBuffer,wrapMode,filterMode,size);
    idConsumer.accept(id);
  }
);
}
