public String getFullStateName(boolean breakStateName){
  String reference=getReference();
  if (reference != null) {
    char sep;
    if (breakStateName) {
      sep='\n';
    }
 else {
      sep=' ';
    }
    return getStateName() + sep + '('+ reference+ ')';
  }
 else {
    return getStateName();
  }
}
