@Override public boolean supportsParameter(ParameterContext parameterContext,ExtensionContext extensionContext) throws ParameterResolutionException {
  if (SUPPORTED_INJECTABLE_TYPES.contains(parameterContext.getParameter().getType())) {
    if (parameterContext.isAnnotated(Inject.class) || parameterContext.getDeclaringExecutable().isAnnotationPresent(Inject.class)) {
      return true;
    }
    LOGGER.warn("Parameter type " + parameterContext.getParameter().getType() + " supported but injection target is not annotated with @Inject");
  }
  return false;
}
