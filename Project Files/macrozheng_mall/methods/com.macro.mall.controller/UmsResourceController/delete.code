@ApiOperation("根据ID删除后台资源") @RequestMapping(value="/delete/{id}",method=RequestMethod.POST) @ResponseBody public CommonResult delete(@PathVariable Long id){
  int count=resourceService.delete(id);
  dynamicSecurityMetadataSource.clearDataSource();
  if (count > 0) {
    return CommonResult.success(count);
  }
 else {
    return CommonResult.failed();
  }
}
