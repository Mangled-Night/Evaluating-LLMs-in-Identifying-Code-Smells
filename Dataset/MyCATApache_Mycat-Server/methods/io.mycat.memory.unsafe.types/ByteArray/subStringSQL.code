public static byte[] subStringSQL(byte[] bytes,int pos,int len){
  if (pos > bytes.length) {
    return EMPTY_BYTE;
  }
  int start=0;
  int end;
  if (pos > 0) {
    start=pos - 1;
  }
 else   if (pos < 0) {
    start=bytes.length + pos;
  }
  if ((bytes.length - start) < len) {
    end=bytes.length;
  }
 else {
    end=start + len;
  }
  start=Math.max(start,0);
  if (start >= end) {
    return EMPTY_BYTE;
  }
  return Arrays.copyOfRange(bytes,start,end);
}
