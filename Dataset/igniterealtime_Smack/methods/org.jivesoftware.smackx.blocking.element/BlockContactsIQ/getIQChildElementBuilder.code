@Override protected IQChildElementXmlStringBuilder getIQChildElementBuilder(IQChildElementXmlStringBuilder xml){
  xml.rightAngleBracket();
  if (jids != null) {
    for (    Jid jid : jids) {
      xml.halfOpenElement("item");
      xml.attribute("jid",jid);
      xml.closeEmptyElement();
    }
  }
  return xml;
}
