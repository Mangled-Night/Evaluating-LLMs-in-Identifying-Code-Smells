@Override public boolean equals(Object other){
  if (this == other) {
    return true;
  }
  if (!(other instanceof ResolvableType)) {
    return false;
  }
  ResolvableType otherType=(ResolvableType)other;
  if (!nullSafeEquals(this.type,otherType.type)) {
    return false;
  }
  if (this.typeProvider != otherType.typeProvider && (this.typeProvider == null || otherType.typeProvider == null || !nullSafeEquals(this.typeProvider.getSource(),otherType.typeProvider.getSource()))) {
    return false;
  }
  if (this.variableResolver != otherType.variableResolver && (this.variableResolver == null || otherType.variableResolver == null || !nullSafeEquals(this.variableResolver.getSource(),otherType.variableResolver.getSource()))) {
    return false;
  }
  if (!nullSafeEquals(this.componentType,otherType.componentType)) {
    return false;
  }
  return true;
}
@Override public boolean equals(Object other){
  if (this == other) {
    return true;
  }
  if (!(other instanceof ParameterizedType)) {
    return false;
  }
  ParameterizedType otherType=(ParameterizedType)other;
  return (otherType.getOwnerType() == null && this.rawType.equals(otherType.getRawType()) && Arrays.equals(this.typeArguments,otherType.getActualTypeArguments()));
}
