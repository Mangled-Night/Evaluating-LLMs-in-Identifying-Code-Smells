public static class AckAnswer implements Nonza {
  public static final String ELEMENT="a";
  private final long handledCount;
  public AckAnswer(  long handledCount){
    this.handledCount=handledCount;
  }
  public long getHandledCount(){
    return handledCount;
  }
  @Override public CharSequence toXML(  org.jivesoftware.smack.packet.XmlEnvironment enclosingNamespace){
    XmlStringBuilder xml=new XmlStringBuilder(this,enclosingNamespace);
    xml.attribute("h",Long.toString(handledCount));
    xml.closeEmptyElement();
    return xml;
  }
  @Override public String getNamespace(){
    return NAMESPACE;
  }
  @Override public String getElementName(){
    return ELEMENT;
  }
}
