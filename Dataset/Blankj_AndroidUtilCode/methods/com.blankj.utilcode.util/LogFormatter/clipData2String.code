@RequiresApi(api=Build.VERSION_CODES.JELLY_BEAN) private static void clipData2String(ClipData clipData,StringBuilder sb){
  ClipData.Item item=clipData.getItemAt(0);
  if (item == null) {
    sb.append("ClipData.Item {}");
    return;
  }
  sb.append("ClipData.Item { ");
  String mHtmlText=item.getHtmlText();
  if (mHtmlText != null) {
    sb.append("H:");
    sb.append(mHtmlText);
    sb.append("}");
    return;
  }
  CharSequence mText=item.getText();
  if (mText != null) {
    sb.append("T:");
    sb.append(mText);
    sb.append("}");
    return;
  }
  Uri uri=item.getUri();
  if (uri != null) {
    sb.append("U:").append(uri);
    sb.append("}");
    return;
  }
  Intent intent=item.getIntent();
  if (intent != null) {
    sb.append("I:");
    sb.append(intent2String(intent));
    sb.append("}");
    return;
  }
  sb.append("NULL");
  sb.append("}");
}
