@RequiresPermissions("admin:brand:update") @RequiresPermissionsDesc(menu={"商场管理","品牌管理"},button="编辑") @PostMapping("/update") public Object update(@RequestBody LitemallBrand brand){
  Object error=validate(brand);
  if (error != null) {
    return error;
  }
  if (brandService.updateById(brand) == 0) {
    return ResponseUtil.updatedDataFailed();
  }
  return ResponseUtil.ok(brand);
}
