private void dfs(Graph G,int v){
  marked[v]=true;
  id[v]=count;
  size[count]++;
  for (  int w : G.adj(v)) {
    if (!marked[w]) {
      dfs(G,w);
    }
  }
}
private void dfs(EdgeWeightedGraph G,int v){
  marked[v]=true;
  id[v]=count;
  size[count]++;
  for (  Edge e : G.adj(v)) {
    int w=e.other(v);
    if (!marked[w]) {
      dfs(G,w);
    }
  }
}
