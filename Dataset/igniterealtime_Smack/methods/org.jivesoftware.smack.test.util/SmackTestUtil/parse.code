public static <E extends Element>E parse(CharSequence xml,AbstractProvider<E> provider,XmlPullParserKind parserKind) throws XmlPullParserException, IOException, SmackParsingException {
  String xmlString=xml.toString();
  Reader reader=new StringReader(xmlString);
  return parse(reader,provider,parserKind);
