/** 
 * All Paths known to this PathManager. 
 */
private List<Path> getAllPaths(){
  List<Path> allPaths=Arrays.stream(PathManager.class.getDeclaredFields()).filter(field -> Path.class.isAssignableFrom(field.getType())).map(this::getField).collect(Collectors.toList());
  allPaths.addAll(modPaths);
  return allPaths;
}
