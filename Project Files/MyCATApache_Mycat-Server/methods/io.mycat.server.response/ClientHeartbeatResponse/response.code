public static void response(ServerConnection c){
  if (RESPONSE_DATA == null) {
synchronized (ClientHeartbeatResponse.class) {
      if (RESPONSE_DATA == null) {
        RESPONSE_DATA=buildPacket(c);
      }
    }
  }
  ByteBuffer buffer=c.allocate();
  buffer.put(RESPONSE_DATA);
  c.write(buffer);
}
