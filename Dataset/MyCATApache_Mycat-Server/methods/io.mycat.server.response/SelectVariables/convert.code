private static List<String> convert(List<String> in){
  List<String> out=new ArrayList<>();
  for (  String s : in) {
    int asIndex=s.toUpperCase().indexOf(" AS ");
    if (asIndex != -1) {
      out.add(s.substring(asIndex + 4));
    }
  }
  if (out.isEmpty()) {
    return in;
  }
 else {
    return out;
  }
}
