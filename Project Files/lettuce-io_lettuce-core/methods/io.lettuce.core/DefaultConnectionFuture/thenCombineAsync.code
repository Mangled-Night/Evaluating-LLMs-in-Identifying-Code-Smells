@Override public <U,V>DefaultConnectionFuture<V> thenCombineAsync(CompletionStage<? extends U> other,BiFunction<? super T,? super U,? extends V> fn){
  return adopt(delegate.thenCombineAsync(other,fn));
}
@Override public <U,V>DefaultConnectionFuture<V> thenCombineAsync(CompletionStage<? extends U> other,BiFunction<? super T,? super U,? extends V> fn,Executor executor){
  return adopt(delegate.thenCombineAsync(other,fn,executor));
}
