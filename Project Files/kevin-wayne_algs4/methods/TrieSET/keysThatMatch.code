/** 
 * Returns all of the keys in the set that match  {@code pattern}, where the character '.' is interpreted as a wildcard character.
 * @param pattern the pattern
 * @return all of the keys in the set that match {@code pattern}, as an iterable, where . is treated as a wildcard character.
 */
public Iterable<String> keysThatMatch(String pattern){
  Queue<String> results=new Queue<String>();
  StringBuilder prefix=new StringBuilder();
  collect(root,prefix,pattern,results);
  return results;
}
