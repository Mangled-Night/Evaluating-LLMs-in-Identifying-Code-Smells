/** 
 * Rearranges an array of objects in uniformly random order (under the assumption that  {@code Math.random()} generates independentand uniformly distributed numbers between 0 and 1).
 * @param a the array to be shuffled
 */
public static void shuffleAlternate(Object[] a){
  int n=a.length;
  for (int i=0; i < n; i++) {
    int r=i + (int)(Math.random() * (n - i));
    Object swap=a[r];
    a[r]=a[i];
    a[i]=swap;
  }
}
