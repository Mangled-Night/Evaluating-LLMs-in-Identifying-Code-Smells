/** 
 * 检查文件是否存在 方法描述
 * @param path
 * @return
 * @创建日期 2016年9月21日
 */
protected boolean checkPathExists(String path){
  try {
    Stat state=this.curator.checkExists().forPath(path);
    if (null != state) {
      return true;
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  return false;
}
