@Test public void fromMinAndSizeInvalid(){
  assertThrows(IllegalArgumentException.class,() -> new BlockRegion(new Vector3i()).setSize(new Vector3i(-1,1,1)));
  assertThrows(IllegalArgumentException.class,() -> new BlockRegion(new Vector3i()).setSize(new Vector3i(1,-1,1)));
  assertThrows(IllegalArgumentException.class,() -> new BlockRegion(new Vector3i()).setSize(new Vector3i(1,1,-1)));
}
