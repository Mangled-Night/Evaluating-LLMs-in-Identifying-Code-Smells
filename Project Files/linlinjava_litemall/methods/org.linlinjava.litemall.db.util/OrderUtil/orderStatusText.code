public static String orderStatusText(LitemallOrder order){
  int status=order.getOrderStatus().intValue();
  if (status == 101) {
    return "未付款";
  }
  if (status == 102) {
    return "已取消";
  }
  if (status == 103) {
    return "已取消(系统)";
  }
  if (status == 201) {
    return "已付款";
  }
  if (status == 202) {
    return "订单取消，退款中";
  }
  if (status == 203) {
    return "已退款";
  }
  if (status == 204) {
    return "已超时团购";
  }
  if (status == 301) {
    return "已发货";
  }
  if (status == 401) {
    return "已收货";
  }
  if (status == 402) {
    return "已收货(系统)";
  }
  throw new IllegalStateException("orderStatus不支持");
}
