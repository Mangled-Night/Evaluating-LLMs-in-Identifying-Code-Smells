private void pivot(int p,int q){
  for (int i=0; i < n; i++) {
    double alpha=a[i][q] / a[p][q];
    for (int j=0; j <= n + n; j++) {
      if (i != p && j != q)       a[i][j]-=alpha * a[p][j];
    }
  }
  for (int i=0; i < n; i++)   if (i != p)   a[i][q]=0.0;
  for (int j=0; j <= n + n; j++)   if (j != q)   a[p][j]/=a[p][q];
  a[p][q]=1.0;
}
