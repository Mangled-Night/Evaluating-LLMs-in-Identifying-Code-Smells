@Produces(Facet4.class) @Requires(@Facet(value=Facet2.class,border=@FacetBorder(sides=3))) public static class Facet4Provider implements FacetProvider {
  @Override public void process(  GeneratingRegion region){
    Facet4 facet=new Facet4(region.getRegion(),region.getBorderForFacet(Facet4.class));
    region.setRegionFacet(Facet4.class,facet);
  }
}
