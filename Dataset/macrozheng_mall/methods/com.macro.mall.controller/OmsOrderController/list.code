@ApiOperation("查询订单") @RequestMapping(value="/list",method=RequestMethod.GET) @ResponseBody public CommonResult<CommonPage<OmsOrder>> list(OmsOrderQueryParam queryParam,@RequestParam(value="pageSize",defaultValue="5") Integer pageSize,@RequestParam(value="pageNum",defaultValue="1") Integer pageNum){
  List<OmsOrder> orderList=orderService.list(queryParam,pageSize,pageNum);
  return CommonResult.success(CommonPage.restPage(orderList));
}
