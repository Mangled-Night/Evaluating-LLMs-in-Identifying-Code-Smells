/** 
 * Make a phone call. <p>Must hold  {@code <uses-permission android:name="android.permission.CALL_PHONE" />}</p>
 * @param phoneNumber The phone number.
 */
@RequiresPermission(CALL_PHONE) public static void call(@NonNull final String phoneNumber){
  Utils.getApp().startActivity(UtilsBridge.getCallIntent(phoneNumber));
}
