@RegisterSystem(RegisterMode.REMOTE_CLIENT) public class BehaviorTreeClientSystem extends BaseComponentSystem {
  @In private Console console;
  /** 
 * Called when a remote client presses F5 to open the Behavior Editor
 * @param event F5 press
 * @param entity the character entity reference
 */
  @ReceiveEvent(components=ClientComponent.class) public void onToggleConsole(  BTEditorButton event,  EntityRef entity){
    if (event.getState() == ButtonState.DOWN) {
      console.addMessage("Behavior Editor is only available to the Host or on Singleplayer",CoreMessageType.CHAT);
    }
  }
}
