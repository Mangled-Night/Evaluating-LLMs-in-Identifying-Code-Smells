@Service @Transactional @BaseService public class UpmsUserServiceImpl extends BaseServiceImpl<UpmsUserMapper,UpmsUser,UpmsUserExample> implements UpmsUserService {
  private static final Logger LOGGER=LoggerFactory.getLogger(UpmsUserServiceImpl.class);
  @Autowired UpmsUserMapper upmsUserMapper;
  @Override public UpmsUser createUser(  UpmsUser upmsUser){
    UpmsUserExample upmsUserExample=new UpmsUserExample();
    upmsUserExample.createCriteria().andUsernameEqualTo(upmsUser.getUsername());
    long count=upmsUserMapper.countByExample(upmsUserExample);
    if (count > 0) {
      return null;
    }
    upmsUserMapper.insert(upmsUser);
    return upmsUser;
  }
}
