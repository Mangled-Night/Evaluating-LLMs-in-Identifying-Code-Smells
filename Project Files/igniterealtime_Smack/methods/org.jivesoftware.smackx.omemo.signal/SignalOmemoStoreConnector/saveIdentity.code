@Override public boolean saveIdentity(SignalProtocolAddress signalProtocolAddress,IdentityKey identityKey){
  OmemoDevice device;
  try {
    device=asOmemoDevice(signalProtocolAddress);
  }
 catch (  XmppStringprepException e) {
    throw new AssertionError(e);
  }
  try {
    omemoStore.storeOmemoIdentityKey(getOurDevice(),device,identityKey);
  }
 catch (  IOException e) {
    throw new IllegalStateException(e);
  }
  return true;
}
