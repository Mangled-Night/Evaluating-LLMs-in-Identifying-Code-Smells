@ApiOperation("更新商品") @RequestMapping(value="/update/{id}",method=RequestMethod.POST) @ResponseBody public CommonResult update(@PathVariable Long id,@RequestBody PmsProductParam productParam){
  int count=productService.update(id,productParam);
  if (count > 0) {
    return CommonResult.success(count);
  }
 else {
    return CommonResult.failed();
  }
}
