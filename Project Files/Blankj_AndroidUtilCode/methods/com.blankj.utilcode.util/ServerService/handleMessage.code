@Override public void handleMessage(Message msg){
switch (msg.what) {
case WHAT_SUBSCRIBE:
    mClientMap.put(msg.arg1,msg.replyTo);
  break;
case WHAT_SEND:
sendMsg2Client(msg);
consumeServerProcessCallback(msg);
break;
case WHAT_UNSUBSCRIBE:
mClientMap.remove(msg.arg1);
break;
default :
super.handleMessage(msg);
}
}
