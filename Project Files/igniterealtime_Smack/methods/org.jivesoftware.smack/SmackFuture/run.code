@Override public void run(){
  successCallback.onSuccess(result);
}
@Override public void run(){
  exceptionCallback.processException(exception);
}
@Override public void run(){
  try {
    socket.connect(socketAddress,timeout);
  }
 catch (  IOException e) {
    setException(e);
    return;
  }
synchronized (wasInterruptedLock) {
    if (wasInterrupted) {
      closeSocket();
      return;
    }
  }
  setResult(socket);
}
