protected void rejectCommands(Collection<? extends RedisCommand<?,?,?>> redisCommands){
  for (  RedisCommand<?,?,?> command : redisCommands) {
    command.completeExceptionally(new RedisException(String.format("Command %s not allowed while subscribed. Allowed commands are: %s",command.getType().name(),ALLOWED_COMMANDS_SUBSCRIBED)));
  }
}
