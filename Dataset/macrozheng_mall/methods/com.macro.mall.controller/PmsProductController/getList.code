@ApiOperation("查询商品") @RequestMapping(value="/list",method=RequestMethod.GET) @ResponseBody public CommonResult<CommonPage<PmsProduct>> getList(PmsProductQueryParam productQueryParam,@RequestParam(value="pageSize",defaultValue="5") Integer pageSize,@RequestParam(value="pageNum",defaultValue="1") Integer pageNum){
  List<PmsProduct> productList=productService.list(productQueryParam,pageSize,pageNum);
  return CommonResult.success(CommonPage.restPage(productList));
}
@ApiOperation("根据商品名称或货号模糊查询") @RequestMapping(value="/simpleList",method=RequestMethod.GET) @ResponseBody public CommonResult<List<PmsProduct>> getList(String keyword){
  List<PmsProduct> productList=productService.list(keyword);
  return CommonResult.success(productList);
}
