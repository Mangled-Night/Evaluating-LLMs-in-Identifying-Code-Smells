private static SchemaInfo parseTables(SQLStatement stmt,MycatSchemaStatVisitor schemaStatVisitor){
  stmt.accept(schemaStatVisitor);
  String key=schemaStatVisitor.getCurrentTable();
  if (key != null && key.contains("`")) {
    key=key.replaceAll("`","");
  }
  if (key != null) {
    SchemaInfo schemaInfo=new SchemaInfo();
    int pos=key.indexOf(".");
    if (pos > 0) {
      schemaInfo.schema=key.substring(0,pos);
      schemaInfo.table=key.substring(pos + 1);
    }
 else {
      schemaInfo.table=key;
    }
    return schemaInfo;
  }
  return null;
}
