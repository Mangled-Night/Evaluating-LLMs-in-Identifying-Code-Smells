private static class ContextImplementation implements Context {
  private final Map<Class<?>,Object> map=Maps.newConcurrentMap();
  @Override public <T>T get(  Class<T> type){
    T result=type.cast(map.get(type));
    if (result != null) {
      return result;
    }
    return null;
  }
  @Override public <T,U extends T>void put(  Class<T> type,  U object){
    map.put(type,object);
  }
}
