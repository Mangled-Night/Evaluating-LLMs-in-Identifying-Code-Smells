private void sendMsg2Client(final Message msg){
  final Message obtain=Message.obtain(msg);
  for (  Messenger client : mClientMap.values()) {
    try {
      if (client != null) {
        client.send(Message.obtain(obtain));
      }
    }
 catch (    RemoteException e) {
      e.printStackTrace();
    }
  }
  obtain.recycle();
}
