static int show2SqlBlankCheck(String stmt,int offset){
  for (++offset; stmt.length() > offset; ) {
switch (stmt.charAt(offset)) {
case ' ':
      return SQL;
case 'W':
case 'w':
    if (isWhere(stmt,offset)) {
      return SQL;
    }
 else {
      return OTHER;
    }
default :
  return (offset << 8) | SQL;
}
}
return OTHER;
}
