public Engines(List<String> dependencies,String worldGeneratorUri,NetworkMode networkMode,List<Class<? extends EngineSubsystem>> subsystems){
  this.networkMode=networkMode;
  this.dependencies.addAll(dependencies);
  this.subsystems.addAll(subsystems);
  if (worldGeneratorUri != null) {
    this.worldGeneratorUri=worldGeneratorUri;
  }
}
