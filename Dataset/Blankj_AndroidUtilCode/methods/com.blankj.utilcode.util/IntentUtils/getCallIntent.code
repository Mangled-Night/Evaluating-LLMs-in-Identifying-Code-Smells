/** 
 * Return the intent of call. <p>Must hold  {@code <uses-permission android:name="android.permission.CALL_PHONE" />}</p>
 * @param phoneNumber The phone number.
 * @return the intent of call
 */
@RequiresPermission(CALL_PHONE) public static Intent getCallIntent(@NonNull final String phoneNumber){
  Intent intent=new Intent("android.intent.action.CALL",Uri.parse("tel:" + Uri.encode(phoneNumber)));
  return getIntent(intent,true);
}
