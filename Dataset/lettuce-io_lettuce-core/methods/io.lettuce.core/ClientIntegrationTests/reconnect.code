@Test void reconnect(){
  redis.set(key,value);
  redis.quit();
  Delay.delay(Duration.ofMillis(100));
  assertThat(redis.get(key)).isEqualTo(value);
  redis.quit();
  Delay.delay(Duration.ofMillis(100));
  assertThat(redis.get(key)).isEqualTo(value);
  redis.quit();
  Delay.delay(Duration.ofMillis(100));
  assertThat(redis.get(key)).isEqualTo(value);
}
