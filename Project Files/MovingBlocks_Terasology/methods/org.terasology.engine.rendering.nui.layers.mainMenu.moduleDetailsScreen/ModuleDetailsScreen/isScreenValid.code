private boolean isScreenValid(){
  if (Stream.of(moduleName,installedVersion,minSupportedVersion,maxSupportedVersion,close,onlineVersion,required,dependencies,modules,openInBrowser,updateModuleButton,description).anyMatch(Objects::isNull)) {
    logger.error("Can't initialize screen correctly. At least one widget was missed!");
    return false;
  }
  return true;
}
