/** 
 * Returns true if this picture is equal to the argument picture.
 * @param other the other picture
 * @return {@code true} if this picture is the same dimension as {@code other}and if all pixels have the same color;  {@code false} otherwise
 */
public boolean equals(Object other){
  if (other == this)   return true;
  if (other == null)   return false;
  if (other.getClass() != this.getClass())   return false;
  GrayscalePicture that=(GrayscalePicture)other;
  if (this.width() != that.width())   return false;
  if (this.height() != that.height())   return false;
  for (int col=0; col < width(); col++)   for (int row=0; row < height(); row++)   if (this.getGrayscale(col,row) != that.getGrayscale(col,row))   return false;
  return true;
}
