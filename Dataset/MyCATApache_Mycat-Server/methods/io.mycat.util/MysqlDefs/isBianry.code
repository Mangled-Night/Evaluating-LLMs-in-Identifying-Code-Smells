public static boolean isBianry(byte mysqlType){
  int type=mysqlType;
  if (type < 0) {
    type+=256;
  }
  if (type == MysqlDefs.FIELD_TYPE_BLOB || type == MysqlDefs.FIELD_TYPE_TINY_BLOB || type == MysqlDefs.FIELD_TYPE_MEDIUM_BLOB || type == MysqlDefs.FIELD_TYPE_LONG_BLOB) {
    return true;
  }
  return false;
}
