@ApiOperation("修改启用状态") @RequestMapping(value="/update/status/{id}",method=RequestMethod.POST) @ResponseBody public CommonResult updateStatus(@PathVariable Long id,Integer status){
  int count=flashPromotionSessionService.updateStatus(id,status);
  if (count > 0) {
    return CommonResult.success(count);
  }
  return CommonResult.failed();
}
