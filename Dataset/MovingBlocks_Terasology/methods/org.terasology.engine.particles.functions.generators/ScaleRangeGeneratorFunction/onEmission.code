@Override public void onEmission(final ScaleRangeGeneratorComponent component,final ParticleData particleData,final Random random){
  particleData.scale.set(random.nextFloat(component.minScale.x(),component.maxScale.x()),random.nextFloat(component.minScale.y(),component.maxScale.y()),random.nextFloat(component.minScale.z(),component.maxScale.z()));
}
