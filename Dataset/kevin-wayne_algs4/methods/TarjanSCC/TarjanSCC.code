/** 
 * Computes the strong components of the digraph  {@code G}.
 * @param G the digraph
 */
public TarjanSCC(Digraph G){
  marked=new boolean[G.V()];
  stack=new Stack<Integer>();
  id=new int[G.V()];
  low=new int[G.V()];
  for (int v=0; v < G.V(); v++) {
    if (!marked[v])     dfs(G,v);
  }
  assert check(G);
}
