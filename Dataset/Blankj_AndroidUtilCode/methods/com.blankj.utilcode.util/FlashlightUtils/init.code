private static boolean init(){
  if (mCamera == null) {
    try {
      mCamera=Camera.open(0);
      mSurfaceTexture=new SurfaceTexture(0);
    }
 catch (    Throwable t) {
      Log.e("FlashlightUtils","init failed: ",t);
      return false;
    }
  }
  if (mCamera == null) {
    Log.e("FlashlightUtils","init failed.");
    return false;
  }
  return true;
}
