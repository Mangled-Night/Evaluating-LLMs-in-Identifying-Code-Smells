/** 
 * Shut down a previously started testing environment. <p> Used to properly shut down and clean up a testing environment set up and started with  {@link #setup()}.
 */
public void tearDown(){
  engines.forEach(TerasologyEngine::shutdown);
  engines.forEach(TerasologyEngine::cleanup);
  engines.clear();
  try {
    pathManagerCleaner.close();
  }
 catch (  RuntimeException e) {
    throw e;
  }
catch (  Throwable e) {
    throw new RuntimeException(e);
  }
  host=null;
  hostContext=null;
}
