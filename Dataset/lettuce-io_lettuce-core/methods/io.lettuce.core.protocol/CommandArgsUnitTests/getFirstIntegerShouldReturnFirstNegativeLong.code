@Test void getFirstIntegerShouldReturnFirstNegativeLong(){
  assertThat(CommandArgsAccessor.getFirstInteger(new CommandArgs<>(StringCodec.UTF8).add(Long.MIN_VALUE))).isEqualTo(Long.MIN_VALUE);
  assertThat(CommandArgsAccessor.getFirstInteger(new CommandArgs<>(StringCodec.UTF8).add(Long.MIN_VALUE + 2))).isEqualTo(Long.MIN_VALUE + 2);
  assertThat(CommandArgsAccessor.getFirstInteger(new CommandArgs<>(StringCodec.UTF8).add(Integer.MIN_VALUE))).isEqualTo(Integer.MIN_VALUE);
}
