@ApiOperation("删除单个商品属性分类") @RequestMapping(value="/delete/{id}",method=RequestMethod.GET) @ResponseBody public CommonResult delete(@PathVariable Long id){
  int count=productAttributeCategoryService.delete(id);
  if (count > 0) {
    return CommonResult.success(count);
  }
 else {
    return CommonResult.failed();
  }
}
