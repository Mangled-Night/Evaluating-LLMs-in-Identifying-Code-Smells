private List<String> findBlockMatches(String searchLowercase){
  return assetManager.getAvailableAssets(BlockFamilyDefinition.class).stream().<Optional<BlockFamilyDefinition>>map(urn -> assetManager.getAsset(urn,BlockFamilyDefinition.class)).filter(def -> def.isPresent() && def.get().isLoadable() && matchesSearch(searchLowercase,def.get())).map(r -> new BlockUri(r.get().getUrn()).toString()).collect(Collectors.toList());
}
