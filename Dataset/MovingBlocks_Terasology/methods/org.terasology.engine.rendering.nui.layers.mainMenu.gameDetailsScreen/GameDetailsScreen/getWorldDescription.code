private String getWorldDescription(final WorldInfo worldInfo){
  String gameTitle;
  if (worldInfo.getCustomTitle().isEmpty()) {
    gameTitle=worldInfo.getTitle();
  }
 else {
    gameTitle=worldInfo.getCustomTitle();
  }
  return translationSystem.translate("${engine:menu#game-details-game-title} ") + gameTitle + '\n'+ '\n'+ translationSystem.translate("${engine:menu#game-details-game-seed} ")+ worldInfo.getSeed()+ '\n'+ '\n'+ translationSystem.translate("${engine:menu#game-details-world-generator}: ")+ worldGeneratorManager.getWorldGeneratorInfo(worldInfo.getWorldGenerator()).getDisplayName()+ '\n'+ '\n'+ translationSystem.translate("${engine:menu#game-details-game-duration} ")+ DateTimeHelper.getDeltaBetweenTimestamps(new Date(0).getTime(),worldInfo.getTime());
}
