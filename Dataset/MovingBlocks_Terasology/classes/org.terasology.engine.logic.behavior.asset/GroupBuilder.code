/** 
 * This class is responsible for implementing the serialization functionality required by the Group asset class to load group information from .group asset files.
 * @see Group
 */
@API public class GroupBuilder {
  private GroupData groupData;
  @In private Gson gson;
  public GroupData loadFromJson(  InputStream json){
    GroupData groupFromFile=new GroupData();
    try {
      gson=new Gson();
      groupFromFile=gson.fromJson(new InputStreamReader(json),GroupData.class);
    }
 catch (    JsonSyntaxException e) {
      groupFromFile.setGroupLabel("ERROR - Syntax");
      e.printStackTrace();
    }
catch (    JsonIOException e) {
      groupFromFile.setGroupLabel("ERROR - IO");
      e.printStackTrace();
    }
    return groupFromFile;
  }
}
