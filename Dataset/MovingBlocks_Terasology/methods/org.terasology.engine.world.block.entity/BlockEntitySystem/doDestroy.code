@Priority(EventPriority.PRIORITY_LOW) @ReceiveEvent public void doDestroy(DoDestroyEvent event,EntityRef entity,ActAsBlockComponent blockComponent){
  if (blockComponent.block != null) {
    commonDestroyed(event,entity,blockComponent.block.getArchetypeBlock());
  }
}
@Priority(EventPriority.PRIORITY_LOW) @ReceiveEvent public void doDestroy(DoDestroyEvent event,EntityRef entity,BlockComponent blockComponent){
  commonDestroyed(event,entity,blockComponent.getBlock());
  worldProvider.setBlock(blockComponent.getPosition(),blockManager.getBlock(BlockManager.AIR_ID));
}
