@Controller @Api(tags="CmsPrefrenceAreaController") @Tag(name="CmsPrefrenceAreaController",description="商品优选管理") @RequestMapping("/prefrenceArea") public class CmsPrefrenceAreaController {
  @Autowired private CmsPrefrenceAreaService prefrenceAreaService;
  @ApiOperation("获取所有商品优选") @RequestMapping(value="/listAll",method=RequestMethod.GET) @ResponseBody public CommonResult<List<CmsPrefrenceArea>> listAll(){
    List<CmsPrefrenceArea> prefrenceAreaList=prefrenceAreaService.listAll();
    return CommonResult.success(prefrenceAreaList);
  }
}
