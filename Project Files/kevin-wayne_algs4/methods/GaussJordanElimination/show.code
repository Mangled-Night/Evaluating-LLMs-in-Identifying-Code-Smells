private void show(){
  for (int i=0; i < n; i++) {
    for (int j=0; j < n; j++) {
      StdOut.printf("%8.3f ",a[i][j]);
    }
    StdOut.printf("| ");
    for (int j=n; j < n + n; j++) {
      StdOut.printf("%8.3f ",a[i][j]);
    }
    StdOut.printf("| %8.3f\n",a[i][n + n]);
  }
  StdOut.println();
}
