@Override public void run(){
  Drawable topDrawable=swipePanel.getTopDrawable();
  topDrawable.setLevel(topDrawable.getLevel() + 500);
  swipePanel.invalidate();
  startRotate();
}
@Override public void run(){
  swipePanel.close(SwipePanel.TOP);
}
@Override public void run(){
  if (getParent() != null) {
    wrapWindow();
  }
}
