@Test public void join(){
  assertEquals(PathUtils.join("",""),"");
  assertEquals(PathUtils.join("","data"),"/data");
  assertEquals(PathUtils.join("","//data"),"/data");
  assertEquals(PathUtils.join("","data//"),"/data");
  assertEquals(PathUtils.join("","//data//"),"/data");
  assertEquals(PathUtils.join("/sdcard","data"),"/sdcard/data");
  assertEquals(PathUtils.join("/sdcard/","data"),"/sdcard/data");
}
