@Override public Flux<StreamMessage<K,V>> xclaim(K key,Consumer<K> consumer,long minIdleTime,String... messageIds){
  return createDissolvingFlux(() -> commandBuilder.xclaim(key,consumer,XClaimArgs.Builder.minIdleTime(minIdleTime),messageIds));
}
@Override public Flux<StreamMessage<K,V>> xclaim(K key,Consumer<K> consumer,XClaimArgs args,String... messageIds){
  return createDissolvingFlux(() -> commandBuilder.xclaim(key,consumer,args,messageIds));
}
