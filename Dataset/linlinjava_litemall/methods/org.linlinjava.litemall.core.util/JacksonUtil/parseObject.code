public static <T>T parseObject(String body,String field,Class<T> clazz){
  ObjectMapper mapper=new ObjectMapper();
  JsonNode node;
  try {
    node=mapper.readTree(body);
    node=node.get(field);
    return mapper.treeToValue(node,clazz);
  }
 catch (  IOException e) {
    logger.error(e.getMessage(),e);
  }
  return null;
}
