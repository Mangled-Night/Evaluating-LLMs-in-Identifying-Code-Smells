@Override public <U>DefaultConnectionFuture<U> applyToEitherAsync(CompletionStage<? extends T> other,Function<? super T,U> fn){
  return adopt(delegate.applyToEitherAsync(other,fn));
}
@Override public <U>DefaultConnectionFuture<U> applyToEitherAsync(CompletionStage<? extends T> other,Function<? super T,U> fn,Executor executor){
  return adopt(delegate.applyToEitherAsync(other,fn,executor));
}
