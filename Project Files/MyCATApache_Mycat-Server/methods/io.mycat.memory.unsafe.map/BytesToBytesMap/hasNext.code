@Override public boolean hasNext(){
  if (numRecords == 0) {
    if (reader != null) {
      File file=spillWriters.removeFirst().getFile();
      if (file != null && file.exists()) {
        if (!file.delete()) {
          logger.error("Was unable to delete spill file {}",file.getAbsolutePath());
        }
      }
    }
  }
  return numRecords > 0;
}
