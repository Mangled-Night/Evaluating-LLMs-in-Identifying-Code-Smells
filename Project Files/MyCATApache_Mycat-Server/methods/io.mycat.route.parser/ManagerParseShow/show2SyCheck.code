static int show2SyCheck(String stmt,int offset){
  if (stmt.length() > offset + "YS".length()) {
    char c1=stmt.charAt(++offset);
    char c2=stmt.charAt(++offset);
    if (c1 == 'S' || c1 == 's') {
switch (c2) {
case 'L':
case 'l':
        return show2syslog(stmt,offset);
case 'P':
case 'p':
      return show2sysparam(stmt,offset);
default :
    return OTHER;
}
}
}
return OTHER;
}
