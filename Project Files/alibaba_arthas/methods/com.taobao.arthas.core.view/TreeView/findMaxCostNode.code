/** 
 * 查找耗时最大的节点，便于后续高亮展示
 * @param node
 */
private void findMaxCostNode(Node node){
  if (!node.isRoot() && !node.parent.isRoot()) {
    if (maxCost == null) {
      maxCost=node;
    }
 else     if (maxCost.totalCost < node.totalCost) {
      maxCost=node;
    }
  }
  if (!node.isLeaf()) {
    for (    Node n : node.children) {
      findMaxCostNode(n);
    }
  }
}
