private <RES,REQ>RES requestEndpoint(String endpoint,String urlArgument,HttpMethod method,REQ data,Class<RES> responseClass) throws IOException, StorageServiceException {
  URL url=new URL(serviceURL,endpoint + (urlArgument == null ? "" : ("/" + urlArgument)));
  return ServiceApiRequest.request(url,method,sessionToken,data,responseClass);
}
