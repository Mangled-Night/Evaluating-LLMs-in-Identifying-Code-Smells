public DirectedCycleX(Digraph G){
  int[] indegree=new int[G.V()];
  for (int v=0; v < G.V(); v++) {
    indegree[v]=G.indegree(v);
  }
  Queue<Integer> queue=new Queue<Integer>();
  for (int v=0; v < G.V(); v++)   if (indegree[v] == 0)   queue.enqueue(v);
  while (!queue.isEmpty()) {
    int v=queue.dequeue();
    for (    int w : G.adj(v)) {
      indegree[w]--;
      if (indegree[w] == 0)       queue.enqueue(w);
    }
  }
  int[] edgeTo=new int[G.V()];
  int root=-1;
  for (int v=0; v < G.V(); v++) {
    if (indegree[v] == 0)     continue;
 else     root=v;
    for (    int w : G.adj(v)) {
      if (indegree[w] > 0) {
        edgeTo[w]=v;
      }
    }
  }
  if (root != -1) {
    boolean[] visited=new boolean[G.V()];
    while (!visited[root]) {
      visited[root]=true;
      root=edgeTo[root];
    }
    cycle=new Stack<Integer>();
    int v=root;
    do {
      cycle.push(v);
      v=edgeTo[v];
    }
 while (v != root);
    cycle.push(root);
  }
  assert check();
}
