public final Config setFileExtension(final String fileExtension){
  if (UtilsBridge.isSpace(fileExtension)) {
    mFileExtension=".txt";
  }
 else {
    if (fileExtension.startsWith(".")) {
      mFileExtension=fileExtension;
    }
 else {
      mFileExtension="." + fileExtension;
    }
  }
  return this;
}
