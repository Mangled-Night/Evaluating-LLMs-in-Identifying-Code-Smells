@SuppressWarnings("UndefinedEquals") @Test public void loadStoreDateOfLastMessageReceived() throws IOException {
  assertNull("The date of last message received must be null at this stage.",store.getDateOfLastReceivedMessage(alice,bob));
  Date before=new Date();
  store.setDateOfLastReceivedMessage(alice,bob,before);
  Date after=store.getDateOfLastReceivedMessage(alice,bob);
  assertEquals("Dates must equal.",after,before);
}
