private static int countChar(String sql,int end){
  int count=0;
  boolean skipChar=false;
  for (int i=0; i < end; i++) {
    if (sql.charAt(i) == '\'' && !skipChar) {
      count++;
      skipChar=false;
    }
 else     if (sql.charAt(i) == '\\') {
      skipChar=true;
    }
 else {
      skipChar=false;
    }
  }
  return count;
}
