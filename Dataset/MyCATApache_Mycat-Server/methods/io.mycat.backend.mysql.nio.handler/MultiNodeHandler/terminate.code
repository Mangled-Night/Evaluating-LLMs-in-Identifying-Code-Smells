@Override public void terminate(Runnable terminateCallBack){
  boolean zeroReached=false;
  lock.lock();
  try {
    if (nodeCount > 0) {
      this.terminateCallBack=terminateCallBack;
    }
 else {
      zeroReached=true;
    }
  }
  finally {
    lock.unlock();
  }
  if (zeroReached) {
    terminateCallBack.run();
  }
}
