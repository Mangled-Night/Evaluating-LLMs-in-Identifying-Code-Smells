@Override public void toggleScreen(String screenUri){
  toggleScreen(new ResourceUrn(screenUri));
}
@Override public void toggleScreen(ResourceUrn screenUri){
  if (isOpen(screenUri)) {
    closeScreen(screenUri);
  }
 else {
    pushScreen(screenUri);
  }
}
@Override public void toggleScreen(UIElement element){
  toggleScreen(element.getUrn());
}
