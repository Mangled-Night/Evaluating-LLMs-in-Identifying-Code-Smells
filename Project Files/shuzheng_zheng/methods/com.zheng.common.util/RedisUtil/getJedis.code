/** 
 * 同步获取Jedis实例
 * @return Jedis
 */
public synchronized static Jedis getJedis(){
  poolInit();
  Jedis jedis=null;
  try {
    if (null != jedisPool) {
      jedis=jedisPool.getResource();
      try {
        jedis.auth(PASSWORD);
      }
 catch (      Exception e) {
      }
    }
  }
 catch (  Exception e) {
    LOGGER.error("Get jedis error : " + e);
  }
  return jedis;
}
