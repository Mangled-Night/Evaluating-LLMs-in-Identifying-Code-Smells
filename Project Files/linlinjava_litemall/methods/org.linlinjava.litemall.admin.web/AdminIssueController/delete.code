@RequiresPermissions("admin:issue:delete") @RequiresPermissionsDesc(menu={"商场管理","通用问题"},button="删除") @PostMapping("/delete") public Object delete(@RequestBody LitemallIssue issue){
  Integer id=issue.getId();
  if (id == null) {
    return ResponseUtil.badArgument();
  }
  issueService.deleteById(id);
  return ResponseUtil.ok();
}
