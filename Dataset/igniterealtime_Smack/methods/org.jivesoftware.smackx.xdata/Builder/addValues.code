public final B addValues(Collection<? extends CharSequence> values){
  ensureValuesAreInitialized();
  for (  CharSequence value : values) {
    addValueVerbatim(value);
  }
  return getThis();
}
public Builder addValues(Collection<? extends Jid> jids){
  for (  Jid jid : jids) {
    addValue(jid);
  }
  return this;
}
