@Override public boolean equals(Object obj){
  if (this == obj) {
    return true;
  }
  if (!super.equals(obj)) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  final Audio other=(Audio)obj;
  if (getClockRate() != other.getClockRate()) {
    return false;
  }
  return true;
}
