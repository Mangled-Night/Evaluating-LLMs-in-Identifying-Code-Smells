/** 
 * Bytes to output stream.
 */
public static OutputStream bytes2OutputStream(final byte[] bytes){
  if (bytes == null || bytes.length <= 0)   return null;
  ByteArrayOutputStream os=null;
  try {
    os=new ByteArrayOutputStream();
    os.write(bytes);
    return os;
  }
 catch (  IOException e) {
    e.printStackTrace();
    return null;
  }
 finally {
    try {
      if (os != null) {
        os.close();
      }
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
  }
}
