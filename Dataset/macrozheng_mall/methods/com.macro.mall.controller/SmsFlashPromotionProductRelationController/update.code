@ApiOperation("修改关联信息") @RequestMapping(value="/update/{id}",method=RequestMethod.POST) @ResponseBody public CommonResult update(@PathVariable Long id,@RequestBody SmsFlashPromotionProductRelation relation){
  int count=relationService.update(id,relation);
  if (count > 0) {
    return CommonResult.success(count);
  }
  return CommonResult.failed();
}
