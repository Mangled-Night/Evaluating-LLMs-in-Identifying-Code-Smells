/** 
 * SELECT USER()
 */
static int currentUserCheck(String stmt,int offset){
  int length=offset + _CURRENT_USER.length;
  if (stmt.length() >= length && ParseUtil.compare(stmt,offset,_CURRENT_USER)) {
    if (stmt.length() > length && stmt.charAt(length) != ' ') {
      return OTHER;
    }
    return USER;
  }
  return OTHER;
}
