@Override public void offer(float locX,float locY,T object){
  float dx=cursor.x() - locX;
  float dy=cursor.y() - locY;
  float distSq=dx * dx + dy * dy;
  float rad=radiusFunc.apply(object).floatValue();
  if (distSq < rad * rad) {
    if (distSq < minDistSq) {
      minDistSq=distSq;
      closest=object;
    }
  }
}
