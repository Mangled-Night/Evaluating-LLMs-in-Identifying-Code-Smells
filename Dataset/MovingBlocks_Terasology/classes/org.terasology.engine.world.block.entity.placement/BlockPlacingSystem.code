@RegisterSystem(RegisterMode.AUTHORITY) public class BlockPlacingSystem extends BaseComponentSystem {
  @In private WorldProvider worldProvider;
  @Priority(EventPriority.PRIORITY_TRIVIAL) @ReceiveEvent(components=WorldComponent.class) public void placeBlockInWorld(  PlaceBlocks event,  EntityRef world){
    worldProvider.setBlocks(event.getBlocks());
  }
}
