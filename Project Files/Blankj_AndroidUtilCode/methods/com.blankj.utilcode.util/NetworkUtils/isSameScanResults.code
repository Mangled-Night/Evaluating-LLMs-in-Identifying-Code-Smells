private static boolean isSameScanResults(List<ScanResult> l1,List<ScanResult> l2){
  if (l1 == null && l2 == null) {
    return true;
  }
  if (l1 == null || l2 == null) {
    return false;
  }
  if (l1.size() != l2.size()) {
    return false;
  }
  for (int i=0; i < l1.size(); i++) {
    ScanResult r1=l1.get(i);
    ScanResult r2=l2.get(i);
    if (!isSameScanResultContent(r1,r2)) {
      return false;
    }
  }
  return true;
}
