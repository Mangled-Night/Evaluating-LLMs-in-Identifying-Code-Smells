@Test public void testShowSlowSchema(){
  Assert.assertEquals(ManagerParseShow.SLOW_SCHEMA,0xff & ManagerParseShow.parse("show @@slow where schema=a",5));
  Assert.assertEquals(ManagerParseShow.SLOW_SCHEMA,0xff & ManagerParseShow.parse("  SHOW @@SLOW   WHERE SCHEMA=B",6));
  Assert.assertEquals(ManagerParseShow.SLOW_SCHEMA,0xff & ManagerParseShow.parse(" show @@slow  WHERE  SCHEMA  = a ",5));
}
