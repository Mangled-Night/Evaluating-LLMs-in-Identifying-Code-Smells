@ApiOperation(value="新增评论") @RequiresPermissions("cms:comment:create") @RequestMapping(value="/create",method=RequestMethod.GET) public String create(){
  return "/manage/comment/create.jsp";
}
@ApiOperation(value="新增评论") @RequiresPermissions("cms:comment:create") @RequestMapping(value="/create",method=RequestMethod.POST) @ResponseBody public Object create(CmsComment cmsComment){
  long time=System.currentTimeMillis();
  cmsComment.setCtime(time);
  int count=cmsCommentService.insertSelective(cmsComment);
  return new CmsResult(CmsResultConstant.SUCCESS,count);
}
