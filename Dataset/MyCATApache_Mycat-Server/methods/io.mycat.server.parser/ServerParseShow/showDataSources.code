static int showDataSources(String stmt,int offset){
  if (stmt.length() > offset + "ources".length()) {
    char c1=stmt.charAt(++offset);
    char c2=stmt.charAt(++offset);
    char c3=stmt.charAt(++offset);
    char c4=stmt.charAt(++offset);
    char c5=stmt.charAt(++offset);
    char c6=stmt.charAt(++offset);
    if ((c1 == 'O' || c1 == 'o') && (c2 == 'U' || c2 == 'u') && (c3 == 'R' || c3 == 'r')&& (c4 == 'C' || c4 == 'c')&& (c5 == 'E' || c5 == 'e')&& (c6 == 'S' || c6 == 's')&& (stmt.length() == ++offset || ParseUtil.isEOF(stmt.charAt(offset)))) {
      return DATASOURCES;
    }
  }
  return OTHER;
}
