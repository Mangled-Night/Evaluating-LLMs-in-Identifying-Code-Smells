@ApiOperation("分页获取品牌相关商品") @RequestMapping(value="/productList",method=RequestMethod.GET) @ResponseBody public CommonResult<CommonPage<PmsProduct>> productList(@RequestParam Long brandId,@RequestParam(value="pageNum",defaultValue="1") Integer pageNum,@RequestParam(value="pageSize",defaultValue="6") Integer pageSize){
  CommonPage<PmsProduct> result=portalBrandService.productList(brandId,pageNum,pageSize);
  return CommonResult.success(result);
}
