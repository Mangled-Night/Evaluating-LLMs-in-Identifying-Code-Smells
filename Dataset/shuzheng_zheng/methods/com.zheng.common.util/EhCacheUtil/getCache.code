/** 
 * 获取缓存
 * @param cacheName
 * @return
 */
private static Cache getCache(String cacheName){
  CacheManager cacheManager=CacheManager.getInstance();
  if (null == cacheManager) {
    return null;
  }
  Cache cache=cacheManager.getCache(cacheName);
  if (null == cache) {
    return null;
  }
  return cache;
}
