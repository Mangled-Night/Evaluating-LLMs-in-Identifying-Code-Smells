@ApiOperation("删除购物车中的指定商品") @RequestMapping(value="/delete",method=RequestMethod.POST) @ResponseBody public CommonResult delete(@RequestParam("ids") List<Long> ids){
  int count=cartItemService.delete(memberService.getCurrentMember().getId(),ids);
  if (count > 0) {
    return CommonResult.success(count);
  }
  return CommonResult.failed();
}
