/** 
 * 输出版本
 * @author vlinux
 */
@Name("version") @Summary("Display Arthas version") public class VersionCommand extends AnnotatedCommand {
  @Override public void process(  CommandProcess process){
    VersionModel result=new VersionModel();
    result.setVersion(ArthasBanner.version());
    process.appendResult(result);
    process.end();
  }
}
