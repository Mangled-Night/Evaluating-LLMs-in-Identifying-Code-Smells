@ParameterizedTest @EnumSource(SmackTestUtil.XmlPullParserKind.class) public void isChunkedBase64DataParsedCorrectly(SmackTestUtil.XmlPullParserKind parserKind) throws XmlPullParserException, IOException, SmackParsingException {
  String streamId="Stream0001";
  String chunkBase64Data="  <chunkedBase64 streamId='" + streamId + "'/>";
  String string="<resp xmlns='urn:xmpp:http' version='1.1' statusCode='200' statusMessage='OK'>" + "<headers xmlns='http://jabber.org/protocol/shim'><header name='Server'>Clayster</header></headers>" + "<data>" + chunkBase64Data + "</data></resp>";
  AbstractHttpOverXmpp.ChunkedBase64 chunkedBase64Value=(AbstractHttpOverXmpp.ChunkedBase64)parseAbstractBody(string,"resp",parserKind).getData().getChild();
  assertEquals(streamId,chunkedBase64Value.getStreamId());
}
