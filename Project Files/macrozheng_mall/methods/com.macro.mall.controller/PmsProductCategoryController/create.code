@ApiOperation("添加商品分类") @RequestMapping(value="/create",method=RequestMethod.POST) @ResponseBody public CommonResult create(@Validated @RequestBody PmsProductCategoryParam productCategoryParam){
  int count=productCategoryService.create(productCategoryParam);
  if (count > 0) {
    return CommonResult.success(count);
  }
 else {
    return CommonResult.failed();
  }
}
