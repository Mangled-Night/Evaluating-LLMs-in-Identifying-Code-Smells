public void testPurgeItems() throws XMPPException {
  LeafNode node=getRandomPubnode(getManager(),true,false);
  node.send(new Item());
  node.send(new Item());
  node.send(new Item());
  node.send(new Item());
  node.send(new Item());
  Collection<? extends Item> items=node.getItems();
  assertTrue(items.size() == 5);
  node.deleteAllItems();
  items=node.getItems();
  assertTrue(items.size() < 2);
}
