@Test void hdel(){
  assertThat(redis.hdel(key,"one")).isEqualTo(0);
  redis.hset(key,"two","2");
  assertThat(redis.hdel(key,"one")).isEqualTo(0);
  redis.hset(key,"one","1");
  assertThat(redis.hdel(key,"one")).isEqualTo(1);
  redis.hset(key,"one","1");
  assertThat(redis.hdel(key,"one","two")).isEqualTo(2);
}
