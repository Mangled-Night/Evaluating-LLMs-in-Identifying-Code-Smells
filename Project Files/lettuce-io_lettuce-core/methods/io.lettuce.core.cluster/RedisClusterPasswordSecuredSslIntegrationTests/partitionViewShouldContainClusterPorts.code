@Test void partitionViewShouldContainClusterPorts(){
  StatefulRedisClusterConnection<String,String> connection=redisClient.connect();
  List<Integer> ports=connection.getPartitions().stream().map(redisClusterNode -> redisClusterNode.getUri().getPort()).collect(Collectors.toList());
  connection.close();
  assertThat(ports).contains(CLUSTER_PORT_SSL_1,CLUSTER_PORT_SSL_3);
}
