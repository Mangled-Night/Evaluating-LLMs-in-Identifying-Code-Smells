@ApiOperation("删除浏览记录") @RequestMapping(value="/delete",method=RequestMethod.POST) @ResponseBody public CommonResult delete(@RequestParam("ids") List<String> ids){
  int count=memberReadHistoryService.delete(ids);
  if (count > 0) {
    return CommonResult.success(count);
  }
 else {
    return CommonResult.failed();
  }
}
