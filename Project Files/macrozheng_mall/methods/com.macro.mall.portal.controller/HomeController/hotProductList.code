@ApiOperation("分页获取人气推荐商品") @RequestMapping(value="/hotProductList",method=RequestMethod.GET) @ResponseBody public CommonResult<List<PmsProduct>> hotProductList(@RequestParam(value="pageNum",defaultValue="1") Integer pageNum,@RequestParam(value="pageSize",defaultValue="6") Integer pageSize){
  List<PmsProduct> productList=homeService.hotProductList(pageNum,pageSize);
  return CommonResult.success(productList);
}
