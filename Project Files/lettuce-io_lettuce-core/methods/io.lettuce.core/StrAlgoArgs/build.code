@Override public <K,V>void build(CommandArgs<K,V> args){
  args.add("LCS");
  args.add(by.name());
  for (  String key : keys) {
    if (by == By.STRINGS) {
      args.add(key.getBytes(charset));
    }
 else {
      args.add(key);
    }
  }
  if (justLen) {
    args.add("LEN");
  }
  if (withIdx) {
    args.add("IDX");
  }
  if (minMatchLen > 0) {
    args.add("MINMATCHLEN");
    args.add(minMatchLen);
  }
  if (withMatchLen) {
    args.add("WITHMATCHLEN");
  }
}
