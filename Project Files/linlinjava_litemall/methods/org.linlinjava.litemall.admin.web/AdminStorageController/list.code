@RequiresPermissions("admin:storage:list") @RequiresPermissionsDesc(menu={"系统管理","对象存储"},button="查询") @GetMapping("/list") public Object list(String key,String name,@RequestParam(defaultValue="1") Integer page,@RequestParam(defaultValue="10") Integer limit,@Sort @RequestParam(defaultValue="add_time") String sort,@Order @RequestParam(defaultValue="desc") String order){
  List<LitemallStorage> storageList=litemallStorageService.querySelective(key,name,page,limit,sort,order);
  return ResponseUtil.okList(storageList);
}
