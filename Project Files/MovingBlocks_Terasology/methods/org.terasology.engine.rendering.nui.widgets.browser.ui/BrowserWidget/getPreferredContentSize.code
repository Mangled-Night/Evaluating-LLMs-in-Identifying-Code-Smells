@Override public Vector2i getPreferredContentSize(Canvas canvas,Vector2i sizeHint){
  if (displayedPage != null) {
    return DocumentRenderer.getDocumentPreferredSize(displayedPage,canvas.getCurrentStyle().getFont(),canvas.getCurrentStyle().getTextColor(),canvas.getRegion().lengthX());
  }
 else {
    return new Vector2i();
  }
}
