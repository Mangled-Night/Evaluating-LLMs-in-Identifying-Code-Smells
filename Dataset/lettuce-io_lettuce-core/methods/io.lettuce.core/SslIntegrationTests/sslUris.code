private static List<RedisURI> sslUris(IntStream masterSlaveOffsets,Function<RedisURI.Builder,RedisURI.Builder> builderCustomizer){
  return masterSlaveOffsets.map(it -> it + MASTER_SLAVE_BASE_PORT_OFFSET).mapToObj(offset -> RedisURI.Builder.redis(TestSettings.host(),sslPort(offset)).withSsl(true)).map(builderCustomizer).map(RedisURI.Builder::build).collect(Collectors.toList());
}
