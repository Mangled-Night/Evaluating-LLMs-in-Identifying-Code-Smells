@Test void shouldDecryptValue(){
  RedisCodec<String,String> crypto=CipherCodec.forValues(StringCodec.UTF8,encrypt,decrypt);
  ByteBuffer encrypted=ByteBuffer.wrap(new byte[]{36,43,48,36,-99,-39,126,-106,-7,-88,118,-74,42,98,117,81,37,-124,26,-88});
  String result=crypto.decodeValue(encrypted);
  assertThat(result).isEqualTo("foobar");
}
