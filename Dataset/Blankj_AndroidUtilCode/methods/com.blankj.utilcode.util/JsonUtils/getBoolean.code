public static boolean getBoolean(final JSONObject jsonObject,final String key){
  return getBoolean(jsonObject,key,false);
}
public static boolean getBoolean(final JSONObject jsonObject,final String key,final boolean defaultValue){
  return getValueByType(jsonObject,key,defaultValue,TYPE_BOOLEAN);
}
public static boolean getBoolean(final String json,final String key){
  return getBoolean(json,key,false);
}
public static boolean getBoolean(final String json,final String key,final boolean defaultValue){
  return getValueByType(json,key,defaultValue,TYPE_BOOLEAN);
}
