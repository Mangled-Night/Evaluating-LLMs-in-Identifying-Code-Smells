@Test public void testParseFailed() throws Exception {
  String failedStanza=XMLBuilder.create("failed").a("xmlns","urn:xmpp:sm:3").asString(outputProperties);
  StreamManagement.Failed failedPacket=ParseStreamManagement.failed(PacketParserUtils.getParserFor(failedStanza));
  assertNotNull(failedPacket);
  assertTrue(failedPacket.getStanzaErrorCondition() == null);
}
