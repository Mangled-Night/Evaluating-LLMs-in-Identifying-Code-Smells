@RegisterWorldGenerator(id="dummy",displayName="dummy") public class DummyWorldGenerator extends BaseFacetedWorldGenerator {
  public static final int SURFACE_HEIGHT=40;
  @In private WorldGeneratorPluginLibrary worldGeneratorPluginLibrary;
  public DummyWorldGenerator(  SimpleUri uri){
    super(uri);
  }
  @Override protected WorldBuilder createWorld(){
    return new WorldBuilder(worldGeneratorPluginLibrary).addProvider(new FlatSurfaceHeightProvider(SURFACE_HEIGHT)).addPlugins();
  }
}
