/** 
 * Creates a  {@link Value} from a {@code score} and {@code value}. The resulting value contains the value if the {@code value} is not null.
 * @param score the score.
 * @param value the value. May be {@code null}.
 * @return the {@link Value}.
 */
public static <T extends V,V>Value<V> fromNullable(double score,T value){
  if (value == null) {
    return empty();
  }
  return new ScoredValue<>(score,value);
}
