public T getValue(){
  if (!mFlag.get()) {
    try {
      mLatch.await();
    }
 catch (    InterruptedException e) {
      e.printStackTrace();
    }
  }
  return mValue;
}
public T getValue(long timeout,TimeUnit unit,T defaultValue){
  if (!mFlag.get()) {
    try {
      mLatch.await(timeout,unit);
    }
 catch (    InterruptedException e) {
      e.printStackTrace();
      return defaultValue;
    }
  }
  return mValue;
}
