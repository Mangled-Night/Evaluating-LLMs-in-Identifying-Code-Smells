static int show2SeCheck(String stmt,int offset){
  if (stmt.length() > offset + "SSION".length()) {
    char c1=stmt.charAt(++offset);
    char c2=stmt.charAt(++offset);
    char c3=stmt.charAt(++offset);
    char c4=stmt.charAt(++offset);
    char c5=stmt.charAt(++offset);
    if ((c1 == 'S' || c1 == 's') && (c2 == 'S' || c2 == 's') && (c3 == 'I' || c3 == 'i')&& (c4 == 'O' || c4 == 'o')&& (c5 == 'N' || c5 == 'n')) {
      if (stmt.length() > ++offset && stmt.charAt(offset) != ' ') {
        return OTHER;
      }
      return SESSION;
    }
  }
 else   if (stmt.length() > offset + "RVER".length()) {
    char c1=stmt.charAt(++offset);
    char c2=stmt.charAt(++offset);
    char c3=stmt.charAt(++offset);
    char c4=stmt.charAt(++offset);
    if ((c1 == 'R' || c1 == 'r') && (c2 == 'V' || c2 == 'v') && (c3 == 'E' || c3 == 'e')&& (c4 == 'R' || c4 == 'r')) {
      if (stmt.length() > ++offset && stmt.charAt(offset) != ' ') {
        return OTHER;
      }
      return SERVER;
    }
  }
  return OTHER;
}
