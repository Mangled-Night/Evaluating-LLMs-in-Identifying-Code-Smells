public static final class Feature implements ExtensionElement {
  public static final QName QNAME=new QName(NAMESPACE,ELEMENT);
  public static final Feature INSTANCE=new Feature();
  private Feature(){
  }
  @Override public String getElementName(){
    return ELEMENT;
  }
  @Override public String getNamespace(){
    return NAMESPACE;
  }
  @Override public String toXML(  org.jivesoftware.smack.packet.XmlEnvironment enclosingNamespace){
    return '<' + ELEMENT + " xmlns='"+ NAMESPACE+ "'/>";
  }
}
public static class Feature implements ExtensionElement {
  public static final QName QNAME=new QName(NAMESPACE,ELEMENT);
  public static final String OPTIONAL_ELEMENT="optional";
  private final boolean optional;
  public Feature(  boolean optional){
    this.optional=optional;
  }
  public boolean isOptional(){
    return optional;
  }
  @Override public String getElementName(){
    return ELEMENT;
  }
  @Override public String getNamespace(){
    return NAMESPACE;
  }
  @Override public XmlStringBuilder toXML(  org.jivesoftware.smack.packet.XmlEnvironment enclosingNamespace){
    XmlStringBuilder xml=new XmlStringBuilder(this);
    if (optional) {
      xml.rightAngleBracket();
      xml.emptyElement(OPTIONAL_ELEMENT);
      xml.closeElement(this);
    }
 else {
      xml.closeEmptyElement();
    }
    return xml;
  }
}
