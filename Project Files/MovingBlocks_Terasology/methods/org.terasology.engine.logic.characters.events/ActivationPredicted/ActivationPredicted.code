public ActivationPredicted(EntityRef instigator,EntityRef target,Vector3f origin,Vector3f direction,Vector3f hitPosition,Vector3f hitNormal,int activationId){
  this.instigator=instigator;
  this.target=target;
  this.direction=direction;
  this.hitPosition=hitPosition;
  this.hitNormal=hitNormal;
  this.origin=origin;
  this.activationId=activationId;
}
