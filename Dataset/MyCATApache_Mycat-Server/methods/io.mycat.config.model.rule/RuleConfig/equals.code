@Override public boolean equals(Object obj){
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  RuleConfig other=(RuleConfig)obj;
  if (column == null) {
    if (other.column != null)     return false;
  }
 else   if (!column.equals(other.column))   return false;
  if (functionName == null) {
    if (other.functionName != null)     return false;
  }
 else   if (!functionName.equals(other.functionName))   return false;
  if (ruleAlgorithm == null) {
    if (other.ruleAlgorithm != null)     return false;
  }
 else   if (!ruleAlgorithm.equals(other.ruleAlgorithm))   return false;
  return true;
}
