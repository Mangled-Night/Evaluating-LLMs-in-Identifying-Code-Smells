private boolean certifySolution(double[][] A,double[] b){
  if (!isFeasible())   return true;
  double[] x=primal();
  for (int i=0; i < m; i++) {
    double sum=0.0;
    for (int j=0; j < n; j++) {
      sum+=A[i][j] * x[j];
    }
    if (Math.abs(sum - b[i]) > EPSILON) {
      StdOut.println("not feasible");
      StdOut.println("b[" + i + "] = "+ b[i]+ ", sum = "+ sum);
      return false;
    }
  }
  return true;
}
