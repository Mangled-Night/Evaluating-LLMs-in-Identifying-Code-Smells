private boolean isOptimal(double[] b,double[] c){
  double[] x=primal();
  double[] y=dual();
  double value=value();
  double value1=0.0;
  for (int j=0; j < x.length; j++)   value1+=c[j] * x[j];
  double value2=0.0;
  for (int i=0; i < y.length; i++)   value2+=y[i] * b[i];
  if (Math.abs(value - value1) > EPSILON || Math.abs(value - value2) > EPSILON) {
    StdOut.println("value = " + value + ", cx = "+ value1+ ", yb = "+ value2);
    return false;
  }
  return true;
}
