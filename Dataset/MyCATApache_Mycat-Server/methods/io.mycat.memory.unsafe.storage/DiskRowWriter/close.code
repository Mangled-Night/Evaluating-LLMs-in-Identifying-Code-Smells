@Override public void close(){
  if (initialized) {
    try {
      if (syncWrites) {
        objOut.flush();
        long start=System.nanoTime();
        fos.getFD().sync();
      }
    }
 catch (    IOException e) {
      LOG.error(e.getMessage());
    }
 finally {
      objOut.close();
    }
    channel=null;
    bs=null;
    fos=null;
    ts=null;
    objOut=null;
    initialized=false;
    hasBeenClosed=true;
  }
}
