/** 
 * Return the net mask by wifi.
 * @return the net mask by wifi
 */
@RequiresPermission(ACCESS_WIFI_STATE) public static String getNetMaskByWifi(){
  @SuppressLint("WifiManagerLeak") WifiManager wm=(WifiManager)Utils.getApp().getSystemService(Context.WIFI_SERVICE);
  if (wm == null)   return "";
  return Formatter.formatIpAddress(wm.getDhcpInfo().netmask);
}
