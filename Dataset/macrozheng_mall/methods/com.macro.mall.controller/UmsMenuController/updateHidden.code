@ApiOperation("修改菜单显示状态") @RequestMapping(value="/updateHidden/{id}",method=RequestMethod.POST) @ResponseBody public CommonResult updateHidden(@PathVariable Long id,@RequestParam("hidden") Integer hidden){
  int count=menuService.updateHidden(id,hidden);
  if (count > 0) {
    return CommonResult.success(count);
  }
 else {
    return CommonResult.failed();
  }
}
