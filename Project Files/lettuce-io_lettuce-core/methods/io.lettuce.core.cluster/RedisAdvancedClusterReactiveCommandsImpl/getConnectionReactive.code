private Mono<RedisClusterReactiveCommands<K,V>> getConnectionReactive(String nodeId){
  return getMono(getConnectionProvider().<K,V>getConnectionAsync(ConnectionIntent.WRITE,nodeId)).map(StatefulRedisConnection::reactive);
}
private Mono<RedisClusterReactiveCommands<K,V>> getConnectionReactive(String host,int port){
  return getMono(getConnectionProvider().<K,V>getConnectionAsync(ConnectionIntent.WRITE,host,port)).map(StatefulRedisConnection::reactive);
}
