@ApiOperation("显示当前用户品牌关注列表") @RequestMapping(value="/list",method=RequestMethod.GET) @ResponseBody public CommonResult<CommonPage<MemberBrandAttention>> list(@RequestParam(value="pageNum",defaultValue="1") Integer pageNum,@RequestParam(value="pageSize",defaultValue="5") Integer pageSize){
  Page<MemberBrandAttention> page=memberAttentionService.list(pageNum,pageSize);
  return CommonResult.success(CommonPage.restPage(page));
}
