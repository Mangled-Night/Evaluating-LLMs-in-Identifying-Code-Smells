static int select2Check(String stmt,int offset){
  if (stmt.length() > ++offset && stmt.charAt(offset) == '@' && stmt.length() > ++offset) {
switch (stmt.charAt(offset)) {
case 'S':
case 's':
      return select2SCheck(stmt,offset);
case 'V':
case 'v':
    return select2VCheck(stmt,offset);
default :
  return OTHER;
}
}
return OTHER;
}
