/** 
 * Gets the sunlight from the chunk. <p> Simply delegates to the provided chunk for values
 */
public class SunlightWorldView extends AbstractFullWorldView {
  public SunlightWorldView(  ChunkProvider chunkProvider){
    super(chunkProvider);
  }
  @Override protected byte getValueAt(  Chunk chunk,  Vector3ic pos){
    return chunk.getSunlight(pos);
  }
  @Override protected void setValueAt(  Chunk chunk,  Vector3ic pos,  byte value){
    chunk.setSunlight(pos,value);
  }
}
