@Override public void fireEvent(SqlExecuteStage stage){
switch (stage) {
case READ:
    onReadCompleted();
  break;
case PARSE:
onParseCompleted();
break;
case ROUTE:
onRouteCompleted();
break;
case EXECUTE:
onExecuteCompleted();
break;
case MERGE:
onMergeCompleted();
break;
case END:
onEndCompleted();
break;
}
}
