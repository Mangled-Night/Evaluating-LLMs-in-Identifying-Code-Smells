/** 
 * 品牌列表
 * @param page 分页页数
 * @param limit 分页大小
 * @return 品牌列表
 */
@GetMapping("list") public Object list(@RequestParam(defaultValue="1") Integer page,@RequestParam(defaultValue="10") Integer limit,@Sort @RequestParam(defaultValue="add_time") String sort,@Order @RequestParam(defaultValue="desc") String order){
  List<LitemallBrand> brandList=brandService.query(page,limit,sort,order);
  return ResponseUtil.okList(brandList);
}
