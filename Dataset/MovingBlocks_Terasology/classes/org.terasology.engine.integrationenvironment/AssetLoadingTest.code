@IntegrationEnvironment public class AssetLoadingTest {
  @In private BlockManager blockManager;
  @In private AssetManager assetManager;
  @Test public void blockPrefabLoadingTest(){
    Block block=blockManager.getBlock("engine:air");
    assertNotNull(block);
    assertEquals(0,block.getHardness());
    assertEquals("Air",block.getDisplayName());
  }
