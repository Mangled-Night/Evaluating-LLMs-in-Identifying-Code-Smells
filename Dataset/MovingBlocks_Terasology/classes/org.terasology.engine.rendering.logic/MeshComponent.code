@ForceBlockActive public final class MeshComponent implements VisualComponent<MeshComponent> {
  @Replicate public Mesh mesh;
  @Replicate public Material material;
  @Replicate public boolean translucent;
  @Replicate public float selfLuminance;
  public boolean hideFromOwner;
  @Replicate public Color color=Color.WHITE;
  @Override public void copyFrom(  MeshComponent other){
    this.mesh=other.mesh;
    this.material=other.material;
    this.translucent=other.translucent;
    this.selfLuminance=other.selfLuminance;
    this.hideFromOwner=other.hideFromOwner;
    this.color=new Color(other.color);
  }
}
