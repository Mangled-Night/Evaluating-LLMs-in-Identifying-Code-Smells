@ApiOperation("分页获取浏览记录") @RequestMapping(value="/list",method=RequestMethod.GET) @ResponseBody public CommonResult<CommonPage<MemberReadHistory>> list(@RequestParam(value="pageNum",defaultValue="1") Integer pageNum,@RequestParam(value="pageSize",defaultValue="5") Integer pageSize){
  Page<MemberReadHistory> page=memberReadHistoryService.list(pageNum,pageSize);
  return CommonResult.success(CommonPage.restPage(page));
}
