/** 
 * A simple IQ. <p> Simple IQs child elements do not contain further attributes besides 'xmlns'. They may contain additional packet extensions. </p>
 */
public abstract class SimpleIQ extends IQ {
  protected SimpleIQ(  String childElementName,  String childElementNamespace){
    super(childElementName,childElementNamespace);
  }
  protected SimpleIQ(  IqData iqBuilder,  String childElementName,  String childElementNamespace){
    super(iqBuilder,childElementName,childElementNamespace);
  }
  @Override protected IQChildElementXmlStringBuilder getIQChildElementBuilder(  IQChildElementXmlStringBuilder xml){
    xml.setEmptyElement();
    return xml;
  }
}
