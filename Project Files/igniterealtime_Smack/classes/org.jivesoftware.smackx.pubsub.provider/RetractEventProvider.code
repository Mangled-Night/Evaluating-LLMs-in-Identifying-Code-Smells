/** 
 * Parses the <b>retract</b> element out of the message event stanza from the server as specified in the <a href="http://xmpp.org/extensions/xep-0060.html#schemas-event">retract schema</a>. This element is a child of the <b>items</b> element.
 * @author Robin Collier
 */
public class RetractEventProvider extends EmbeddedExtensionProvider<RetractItem> {
  @Override protected RetractItem createReturnExtension(  String currentElement,  String currentNamespace,  Map<String,String> attributeMap,  List<? extends XmlElement> content){
    return new RetractItem(attributeMap.get("id"));
  }
}
