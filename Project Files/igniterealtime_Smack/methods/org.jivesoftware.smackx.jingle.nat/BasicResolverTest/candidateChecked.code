public void candidateChecked(TransportCandidate cand,boolean result){
  if (result == true) {
    System.out.println(cand.getIp() + " is reachable (as expected)");
    incCounter();
  }
}
public void candidateChecked(TransportCandidate cand,boolean result){
  if (result == false) {
    System.out.println(cand.getIp() + " is _not_ reachable (as expected)");
    incCounter();
  }
}
