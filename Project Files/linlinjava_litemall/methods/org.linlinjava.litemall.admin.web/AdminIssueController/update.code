@RequiresPermissions("admin:issue:update") @RequiresPermissionsDesc(menu={"商场管理","通用问题"},button="编辑") @PostMapping("/update") public Object update(@RequestBody LitemallIssue issue){
  Object error=validate(issue);
  if (error != null) {
    return error;
  }
  if (issueService.updateById(issue) == 0) {
    return ResponseUtil.updatedDataFailed();
  }
  return ResponseUtil.ok(issue);
}
