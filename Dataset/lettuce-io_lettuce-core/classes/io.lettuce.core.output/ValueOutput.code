/** 
 * Value output.
 * @param < K > Key type.
 * @param < V > Value type.
 * @author Will Glozer
 * @author Mark Paluch
 */
public class ValueOutput<K,V> extends CommandOutput<K,V,V> {
  public ValueOutput(  RedisCodec<K,V> codec){
    super(codec,null);
  }
  @Override public void set(  ByteBuffer bytes){
    output=(bytes == null) ? null : codec.decodeValue(bytes);
  }
}
