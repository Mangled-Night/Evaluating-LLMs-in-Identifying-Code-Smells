private void forwardElimination(){
  for (int p=0; p < Math.min(m,n); p++) {
    int max=p;
    for (int i=p + 1; i < m; i++) {
      if (Math.abs(a[i][p]) > Math.abs(a[max][p])) {
        max=i;
      }
    }
    swap(p,max);
    if (Math.abs(a[p][p]) <= EPSILON) {
      continue;
    }
    pivot(p);
  }
}
