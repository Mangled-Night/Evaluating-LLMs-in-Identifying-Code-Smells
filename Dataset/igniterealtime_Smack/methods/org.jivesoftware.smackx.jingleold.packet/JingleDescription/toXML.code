/** 
 * Convert a Jingle description to XML.
 * @return a string with the XML representation
 */
@Override public String toXML(org.jivesoftware.smack.packet.XmlEnvironment enclosingNamespace){
  StringBuilder buf=new StringBuilder();
synchronized (payloads) {
    if (payloads.size() > 0) {
      buf.append('<').append(getElementName());
      buf.append(" xmlns=\"").append(getNamespace()).append("\" >");
      for (      PayloadType payloadType : payloads) {
        if (payloadType != null) {
          buf.append(payloadType.toXML());
        }
      }
      buf.append("</").append(getElementName()).append('>');
    }
  }
  return buf.toString();
}
