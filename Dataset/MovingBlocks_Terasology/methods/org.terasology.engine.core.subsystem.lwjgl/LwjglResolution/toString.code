@Override public String toString(){
  int bpp=getRedBits() + getGreenBits() + getBlueBits();
  if (bpp < 15 || bpp >= 24) {
    bpp=32;
  }
  StringBuilder sb=new StringBuilder(32);
  sb.append(getWidth());
  sb.append("x");
  sb.append(getHeight());
  sb.append("x");
  sb.append(bpp);
  sb.append("@");
  sb.append(getRefreshRate());
  sb.append("Hz");
  return sb.toString();
}
