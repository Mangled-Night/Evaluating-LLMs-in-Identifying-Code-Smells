private static long[] hash(String src){
  long nr=1345345333L;
  long add=7;
  long nr2=0x12345671L;
  long tmp;
  for (int i=0; i < src.length(); ++i) {
switch (src.charAt(i)) {
case ' ':
case '\t':
      continue;
default :
    tmp=(0xff & src.charAt(i));
  nr^=((((nr & 63) + add) * tmp) + (nr << 8));
nr2+=((nr2 << 8) ^ nr);
add+=tmp;
}
}
long[] result=new long[2];
result[0]=nr & 0x7fffffffL;
result[1]=nr2 & 0x7fffffffL;
return result;
}
