private String generateResult(String suggestion,Name commandName,List<String> commandParameters,int suggestedIndex){
  if (suggestedIndex <= 0) {
    return suggestion + " ";
  }
 else {
    StringBuilder result=new StringBuilder();
    result.append(commandName.toString());
    for (int i=0; i < suggestedIndex - 1; i++) {
      result.append(" ");
      result.append(commandParameters.get(i));
    }
    result.append(" ");
    result.append(suggestion);
    result.append(" ");
    return result.toString();
  }
}
