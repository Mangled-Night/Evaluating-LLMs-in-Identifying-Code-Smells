/** 
 * Sends a read char on the spy stream.
 * @param ch - character read from the session
 */
void _spyRead(int ch){
  OutputStream spy=spyStream;
  if (spy != null) {
    try {
      if (ch != '\r') {
        if (ch == '\n') {
          spy.write('\r');
        }
        spy.write(ch);
        spy.flush();
      }
    }
 catch (    IOException e) {
      spyStream=null;
    }
  }
}
