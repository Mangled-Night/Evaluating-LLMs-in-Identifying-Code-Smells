@Test void bindsUnboundedValueRangeCorrectly(){
  CommandMethod commandMethod=DeclaredCommandMethod.create(ReflectionUtils.findMethod(MyCommands.class,"valueRange",Range.class));
  CommandArgs<String,String> args=bind(commandMethod,Range.unbounded());
  assertThat(args.toCommandString()).isEqualTo(String.format("%s %s",Base64.getEncoder().encodeToString("-".getBytes()),Base64.getEncoder().encodeToString("+".getBytes())));
}
