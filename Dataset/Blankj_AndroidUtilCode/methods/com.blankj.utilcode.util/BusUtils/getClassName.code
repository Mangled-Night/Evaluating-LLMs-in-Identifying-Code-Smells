private Class getClassName(String paramType) throws ClassNotFoundException {
switch (paramType) {
case "boolean":
    return boolean.class;
case "int":
  return int.class;
case "long":
return long.class;
case "short":
return short.class;
case "byte":
return byte.class;
case "double":
return double.class;
case "float":
return float.class;
case "char":
return char.class;
default :
return Class.forName(paramType);
}
}
