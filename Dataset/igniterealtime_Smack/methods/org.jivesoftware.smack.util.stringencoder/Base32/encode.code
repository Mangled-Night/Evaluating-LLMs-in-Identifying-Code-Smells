@Override public String encode(String string){
  return Base32.encode(string);
}
public static String encode(String str){
  byte[] b=str.getBytes(StandardCharsets.UTF_8);
  ByteArrayOutputStream os=new ByteArrayOutputStream();
  for (int i=0; i < (b.length + 4) / 5; i++) {
    short[] s=new short[5];
    int[] t=new int[8];
    int blocklen=5;
    for (int j=0; j < 5; j++) {
      if ((i * 5 + j) < b.length)       s[j]=(short)(b[i * 5 + j] & 0xFF);
 else {
        s[j]=0;
        blocklen--;
      }
    }
    int padlen=lenToPadding(blocklen);
    t[0]=(byte)((s[0] >> 3) & 0x1F);
    t[1]=(byte)(((s[0] & 0x07) << 2) | ((s[1] >> 6) & 0x03));
    t[2]=(byte)((s[1] >> 1) & 0x1F);
    t[3]=(byte)(((s[1] & 0x01) << 4) | ((s[2] >> 4) & 0x0F));
    t[4]=(byte)(((s[2] & 0x0F) << 1) | ((s[3] >> 7) & 0x01));
    t[5]=(byte)((s[3] >> 2) & 0x1F);
    t[6]=(byte)(((s[3] & 0x03) << 3) | ((s[4] >> 5) & 0x07));
    t[7]=(byte)(s[4] & 0x1F);
    for (int j=0; j < t.length - padlen; j++) {
      char c=ALPHABET.charAt(t[j]);
      os.write(c);
    }
  }
  String res=new String(os.toByteArray(),StandardCharsets.UTF_8);
  return res;
}
