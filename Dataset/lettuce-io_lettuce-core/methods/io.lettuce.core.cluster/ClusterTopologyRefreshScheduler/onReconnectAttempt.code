@Override public void onReconnectAttempt(int attempt){
  if (isEnabled(ClusterTopologyRefreshOptions.RefreshTrigger.PERSISTENT_RECONNECTS) && attempt >= getClusterTopologyRefreshOptions().getRefreshTriggersReconnectAttempts()) {
    if (indicateTopologyRefreshSignal()) {
      emitAdaptiveRefreshScheduledEvent();
    }
  }
}
