static int callCheck(String stmt,int offset){
  if (stmt.length() > offset + 3) {
    char c1=stmt.charAt(++offset);
    char c2=stmt.charAt(++offset);
    char c3=stmt.charAt(++offset);
    if ((c1 == 'A' || c1 == 'a') && (c2 == 'L' || c2 == 'l') && (c3 == 'L' || c3 == 'l')) {
      return CALL;
    }
  }
  return OTHER;
}
