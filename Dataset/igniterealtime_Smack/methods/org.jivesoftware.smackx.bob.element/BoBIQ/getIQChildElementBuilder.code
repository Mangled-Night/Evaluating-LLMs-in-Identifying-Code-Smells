@Override protected IQChildElementXmlStringBuilder getIQChildElementBuilder(IQChildElementXmlStringBuilder xml){
  xml.attribute("cid",cid.getCid());
  if (bobData != null) {
    xml.optIntAttribute("max_age",bobData.getMaxAge());
    xml.attribute("type",bobData.getType());
    xml.rightAngleBracket();
    xml.escape(bobData.getContentBase64Encoded());
  }
 else {
    xml.setEmptyElement();
  }
  return xml;
}
