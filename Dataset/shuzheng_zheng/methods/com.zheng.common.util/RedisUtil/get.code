/** 
 * 获取String值
 * @param key
 * @return value
 */
public synchronized static String get(String key){
  Jedis jedis=getJedis();
  if (null == jedis) {
    return null;
  }
  String value=jedis.get(key);
  jedis.close();
  return value;
}
/** 
 * 获取byte[]值
 * @param key
 * @return value
 */
public synchronized static byte[] get(byte[] key){
  Jedis jedis=getJedis();
  if (null == jedis) {
    return null;
  }
  byte[] value=jedis.get(key);
  jedis.close();
  return value;
}
