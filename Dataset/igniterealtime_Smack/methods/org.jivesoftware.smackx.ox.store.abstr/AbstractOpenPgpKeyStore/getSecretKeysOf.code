@Override public PGPSecretKeyRingCollection getSecretKeysOf(BareJid owner) throws IOException, PGPException {
  PGPSecretKeyRingCollection keys=secretKeyRingCollections.get(owner);
  if (keys == null) {
    keys=readSecretKeysOf(owner);
    if (keys != null) {
      secretKeyRingCollections.put(owner,keys);
    }
  }
  return keys;
}
