@ApiOperation("getForEntity params") @RequestMapping(value="/get2/{id}",method=RequestMethod.GET) @ResponseBody public Object getForEntity2(@PathVariable Long id){
  String url=HOST_MALL_ADMIN + "/brand/{id}";
  Map<String,String> params=new HashMap<>();
  params.put("id",String.valueOf(id));
  ResponseEntity<CommonResult> responseEntity=restTemplate.getForEntity(url,CommonResult.class,params);
  return responseEntity.getBody();
}
