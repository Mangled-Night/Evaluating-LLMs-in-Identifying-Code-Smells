@Test void mutateShouldConfigureNewOptions(){
  KeepAliveOptions sut=KeepAliveOptions.builder().enable().count(5).idle(Duration.ofSeconds(20)).interval(Duration.ofSeconds(40)).build();
  KeepAliveOptions reconfigured=sut.mutate().count(7).build();
  assertThat(sut.getCount()).isEqualTo(5);
  assertThat(reconfigured.getCount()).isEqualTo(7);
}
