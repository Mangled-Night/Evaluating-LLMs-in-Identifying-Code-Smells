public static String convertToKotlinDoc(String javaDoc){
  String res=javaDoc;
  res=replaceSurrounding(res,"{@code ","}","`","`");
  res=replaceSurrounding(res,"{@link ","}","[","]");
  return res.replace("java.lang.Object","Any").replace("Object","Any").replaceAll("\\bdouble\\b","Double").replaceAll("\\bint\\b","Integer");
}
