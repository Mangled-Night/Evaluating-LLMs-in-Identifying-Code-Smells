/** 
 * Initialises: network system, join status, and module manager.
 */
public ClientConnectionHandler(JoinStatusImpl joinStatus,Config config,Context parentContext,ModuleManager moduleManager,PlayerConfig playerConfig,EngineTime time){
  this.config=config;
  this.context=new ContextImpl(parentContext);
  this.moduleManager=moduleManager;
  this.playerConfig=playerConfig;
  this.time=time;
  this.joinStatus=joinStatus;
}
