public static boolean compare(String s,int offset,char[] keyword){
  if (s.length() >= offset + keyword.length) {
    for (int i=0; i < keyword.length; ++i, ++offset) {
      if (Character.toUpperCase(s.charAt(offset)) != keyword[i]) {
        return false;
      }
    }
    return true;
  }
  return false;
}
