/** 
 * Using Wang/Jenkins Hash
 * @param key
 * @return hash value
 */
protected int hash(int key){
  key=(~key) + (key << 21);
  key=key ^ (key >> 24);
  key=(key + (key << 3)) + (key << 8);
  key=key ^ (key >> 14);
  key=(key + (key << 2)) + (key << 4);
  key=key ^ (key >> 28);
  key=key + (key << 31);
  return key;
}
