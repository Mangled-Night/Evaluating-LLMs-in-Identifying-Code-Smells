@RequiresPermissions("admin:topic:update") @RequiresPermissionsDesc(menu={"推广管理","专题管理"},button="编辑") @PostMapping("/update") public Object update(@RequestBody LitemallTopic topic){
  Object error=validate(topic);
  if (error != null) {
    return error;
  }
  if (topicService.updateById(topic) == 0) {
    return ResponseUtil.updatedDataFailed();
  }
  return ResponseUtil.ok(topic);
}
