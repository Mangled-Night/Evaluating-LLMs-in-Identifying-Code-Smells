@RequiresPermissions("admin:storage:read") @RequiresPermissionsDesc(menu={"系统管理","对象存储"},button="详情") @PostMapping("/read") public Object read(@NotNull Integer id){
  LitemallStorage storageInfo=litemallStorageService.findById(id);
  if (storageInfo == null) {
    return ResponseUtil.badArgumentValue();
  }
  return ResponseUtil.ok(storageInfo);
}
