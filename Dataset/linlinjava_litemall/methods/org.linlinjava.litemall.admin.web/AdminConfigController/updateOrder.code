@RequiresPermissions("admin:config:order:updateConfigs") @RequiresPermissionsDesc(menu={"配置管理","订单配置"},button="编辑") @PostMapping("/order") public Object updateOrder(@RequestBody String body){
  Map<String,String> data=JacksonUtil.toMap(body);
  systemConfigService.updateConfig(data);
  return ResponseUtil.ok();
}
