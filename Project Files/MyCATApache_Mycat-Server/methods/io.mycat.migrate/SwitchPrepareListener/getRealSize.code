private int getRealSize(List<String> dataHosts){
  int size=dataHosts.size();
  Set<String> set=new HashSet(dataHosts);
  if (set.contains("_prepare")) {
    size=size - 1;
  }
  if (set.contains("_commit")) {
    size=size - 1;
  }
  if (set.contains("_clean")) {
    size=size - 1;
  }
  return size;
}
