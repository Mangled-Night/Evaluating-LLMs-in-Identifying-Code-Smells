public ChatSetting getChatSetting(String key){
  Collection<ChatSetting> col=getSettings();
  if (col != null) {
    Iterator<ChatSetting> iter=col.iterator();
    while (iter.hasNext()) {
      ChatSetting chatSetting=iter.next();
      if (chatSetting.getKey().equals(key)) {
        return chatSetting;
      }
    }
  }
  return null;
}
