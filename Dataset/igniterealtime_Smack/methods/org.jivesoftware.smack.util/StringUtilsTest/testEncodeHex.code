@Test public void testEncodeHex(){
  String input="";
  String output="";
  assertEquals(new String(StringUtils.encodeHex(input.getBytes(StandardCharsets.UTF_8))),output);
  input="foo bar 123";
  output="666f6f2062617220313233";
  assertEquals(new String(StringUtils.encodeHex(input.getBytes(StandardCharsets.UTF_8))),output);
}
