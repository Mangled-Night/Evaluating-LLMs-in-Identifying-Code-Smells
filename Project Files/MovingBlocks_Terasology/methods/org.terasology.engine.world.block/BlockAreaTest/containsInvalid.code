@Test public void containsInvalid(){
  BlockArea a=new BlockArea(BlockArea.INVALID);
  assertFalse(a.contains(1,0));
  assertFalse(a.contains(0,1));
  assertFalse(a.contains(1,1));
  assertFalse(a.contains(-1,0));
  assertFalse(a.contains(0,-1));
  assertFalse(a.contains(-1,1));
  assertFalse(a.contains(1,-1));
  assertFalse(a.contains(-1,-1));
}
