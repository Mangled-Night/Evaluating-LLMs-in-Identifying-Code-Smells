@Override public void beforeAll(ExtensionContext extContext){
  extContext.getTestInstance().ifPresentOrElse(o -> injectTestInstances(extContext),() -> {
    var lifecycle=extContext.getTestInstanceLifecycle().orElse(TestInstance.Lifecycle.PER_METHOD);
    if (!lifecycle.equals(TestInstance.Lifecycle.PER_METHOD)) {
      logger.warn("Unexpected: This {} test has no instance for {} in context {}",lifecycle,extContext.getUniqueId(),extContext);
    }
  }
);
}
