@Override public final XmlStringBuilder toXML(XmlEnvironment xmlEnvironment){
  XmlStringBuilder xml=new XmlStringBuilder(this,xmlEnvironment);
  xml.attribute(ATTR_CID,getCandidateId());
  xml.closeEmptyElement();
  return xml;
}
@Override public XmlStringBuilder toXML(org.jivesoftware.smack.packet.XmlEnvironment enclosingNamespace){
  XmlStringBuilder xml=new XmlStringBuilder();
  xml.halfOpenElement(this);
  xml.closeEmptyElement();
  return xml;
}
@Override public CharSequence toXML(org.jivesoftware.smack.packet.XmlEnvironment enclosingNamespace){
  XmlStringBuilder xml=new XmlStringBuilder();
  xml.halfOpenElement(this);
  xml.closeEmptyElement();
  return xml;
}
