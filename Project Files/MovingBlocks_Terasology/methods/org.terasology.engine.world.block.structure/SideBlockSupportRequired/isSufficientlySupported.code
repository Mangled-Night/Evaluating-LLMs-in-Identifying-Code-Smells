@Override public boolean isSufficientlySupported(Vector3ic location,Map<? extends Vector3ic,Block> blockOverrides){
  final SideBlockSupportRequiredComponent component=getComponent(location,blockOverrides);
  if (component != null) {
    return isSufficientlySupported(location,null,blockOverrides,component);
  }
  return true;
}
private boolean isSufficientlySupported(Vector3ic location,Side sideChanged,Map<? extends Vector3ic,Block> blockOverrides,SideBlockSupportRequiredComponent supportComponent){
  if (supportComponent != null) {
    if ((sideChanged == null || sideChanged.isHorizontal()) && supportComponent.sideAllowed && !hasSupport(location,supportComponent,blockOverrides)) {
      return false;
    }
 else     if ((sideChanged == null || !sideChanged.isHorizontal()) && (supportComponent.topAllowed || supportComponent.bottomAllowed) && !hasSupport(location,supportComponent,blockOverrides)) {
      return false;
    }
  }
  return true;
}
