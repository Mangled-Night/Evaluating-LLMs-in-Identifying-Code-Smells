private boolean check(AdjMatrixEdgeWeightedDigraph G){
  if (!hasNegativeCycle()) {
    for (int v=0; v < G.V(); v++) {
      for (      DirectedEdge e : G.adj(v)) {
        int w=e.to();
        for (int i=0; i < G.V(); i++) {
          if (distTo[i][w] > distTo[i][v] + e.weight()) {
            System.err.println("edge " + e + " is eligible");
            return false;
          }
        }
      }
    }
  }
  return true;
}
