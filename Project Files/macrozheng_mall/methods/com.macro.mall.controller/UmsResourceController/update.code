@ApiOperation("修改后台资源") @RequestMapping(value="/update/{id}",method=RequestMethod.POST) @ResponseBody public CommonResult update(@PathVariable Long id,@RequestBody UmsResource umsResource){
  int count=resourceService.update(id,umsResource);
  dynamicSecurityMetadataSource.clearDataSource();
  if (count > 0) {
    return CommonResult.success(count);
  }
 else {
    return CommonResult.failed();
  }
}
