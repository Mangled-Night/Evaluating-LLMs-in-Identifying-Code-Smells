@Test public void test5(){
  Assert.assertNull(SplitUtil.split(null,'\u0000'));
  Assert.assertArrayEquals(new String[]{},SplitUtil.split("",'/'));
  Assert.assertArrayEquals(new String[]{},SplitUtil.split("/",'/'));
