/** 
 * Adds a new value to the SpaceTree. If the root (start of the tree) is null, then a new node will be created. Otherwise, it'll call upon the addToNode() method to add the new value to the root node.
 * @param position   The position of the new node
 */
@Override public T add(float[] position,T value){
  validatePosition(position);
  if (value == null) {
    throw new IllegalArgumentException("Value cannot be null");
  }
  if (rootNode == null) {
    float[] min=new float[dimensions];
    float[] max=new float[dimensions];
    for (int i=0; i < dimensions; i++) {
      min[i]=Float.MIN_VALUE;
      max[i]=Float.MAX_VALUE;
    }
    rootNode=createNewNode(position,min,max,value);
    return null;
  }
 else {
    return addToNode(position,rootNode,value);
  }
}
