@Controller @Api(tags="UmsMemberLevelController") @Tag(name="UmsMemberLevelController",description="会员等级管理") @RequestMapping("/memberLevel") public class UmsMemberLevelController {
  @Autowired private UmsMemberLevelService memberLevelService;
  @ApiOperation("查询所有会员等级") @RequestMapping(value="/list",method=RequestMethod.GET) @ResponseBody public CommonResult<List<UmsMemberLevel>> list(  @RequestParam("defaultStatus") Integer defaultStatus){
    List<UmsMemberLevel> memberLevelList=memberLevelService.list(defaultStatus);
    return CommonResult.success(memberLevelList);
  }
}
