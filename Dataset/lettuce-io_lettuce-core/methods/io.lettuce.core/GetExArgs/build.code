@Override public <K,V>void build(CommandArgs<K,V> args){
  if (ex != null) {
    args.add("EX").add(ex);
  }
  if (exAt != null) {
    args.add("EXAT").add(exAt);
  }
  if (px != null) {
    args.add("PX").add(px);
  }
  if (pxAt != null) {
    args.add("PXAT").add(pxAt);
  }
  if (persist) {
    args.add(CommandType.PERSIST);
  }
}
