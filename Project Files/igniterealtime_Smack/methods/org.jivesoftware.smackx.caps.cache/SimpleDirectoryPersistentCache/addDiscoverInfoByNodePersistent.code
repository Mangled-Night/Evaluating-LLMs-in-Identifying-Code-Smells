@Override public void addDiscoverInfoByNodePersistent(String nodeVer,DiscoverInfo info){
  File nodeFile=getFileFor(nodeVer);
  try {
    if (nodeFile.createNewFile())     writeInfoToFile(nodeFile,info);
  }
 catch (  IOException e) {
    LOGGER.log(Level.SEVERE,"Failed to write disco info to file",e);
  }
}
