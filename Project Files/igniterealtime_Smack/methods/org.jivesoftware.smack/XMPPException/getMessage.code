@Override public String getMessage(){
  StringBuilder sb=new StringBuilder();
  if (stanza != null) {
    Jid from=stanza.getFrom();
    if (from != null) {
      sb.append("XMPP error reply received from " + from + ": ");
    }
  }
  sb.append(error);
  if (request != null) {
    sb.append(" as result of the following request: ");
    sb.append(request);
  }
  return sb.toString();
}
