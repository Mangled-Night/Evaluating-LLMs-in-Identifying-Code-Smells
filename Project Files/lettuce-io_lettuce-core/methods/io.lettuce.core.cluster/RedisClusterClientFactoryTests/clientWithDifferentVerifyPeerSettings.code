@Test void clientWithDifferentVerifyPeerSettings(){
  RedisURI redisURI=RedisURI.create("rediss://host1");
  redisURI.setVerifyPeer(false);
  assertThatThrownBy(() -> RedisClusterClient.create(Arrays.asList(redisURI,RedisURI.create("rediss://host1")))).isInstanceOf(IllegalArgumentException.class);
}
