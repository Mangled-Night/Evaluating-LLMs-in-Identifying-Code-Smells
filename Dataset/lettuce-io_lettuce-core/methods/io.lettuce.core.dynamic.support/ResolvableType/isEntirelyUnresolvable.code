/** 
 * Return  {@code true} if this type contains unresolvable generics only, that is, no substitute for any of its declared typevariables.
 */
boolean isEntirelyUnresolvable(){
  if (this == NONE) {
    return false;
  }
  ResolvableType[] generics=getGenerics();
  for (  ResolvableType generic : generics) {
    if (!generic.isUnresolvableTypeVariable() && !generic.isWildcardWithoutBounds()) {
      return false;
    }
  }
  return true;
}
