@ApiOperation("修改后台资源分类") @RequestMapping(value="/update/{id}",method=RequestMethod.POST) @ResponseBody public CommonResult update(@PathVariable Long id,@RequestBody UmsResourceCategory umsResourceCategory){
  int count=resourceCategoryService.update(id,umsResourceCategory);
  if (count > 0) {
    return CommonResult.success(count);
  }
 else {
    return CommonResult.failed();
  }
}
