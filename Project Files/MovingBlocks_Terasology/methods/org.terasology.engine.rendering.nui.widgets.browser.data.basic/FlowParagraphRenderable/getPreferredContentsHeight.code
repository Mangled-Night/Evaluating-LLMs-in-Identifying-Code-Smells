@Override public int getPreferredContentsHeight(ParagraphRenderStyle defaultStyle,int yStart,ContainerRenderSpace containerRenderSpace,int sideIndents){
  int height=0;
  for (  LaidFlowLine<FlowRenderable> element : updateCacheIfNeeded(defaultStyle,yStart,containerRenderSpace)) {
    height+=element.getHeight();
  }
  return height;
}
