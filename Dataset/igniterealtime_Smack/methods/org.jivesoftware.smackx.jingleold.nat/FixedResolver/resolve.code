/** 
 * Resolve the IP address.
 * @throws NotConnectedException if the XMPP connection is not connected.
 * @throws InterruptedException if the calling thread was interrupted.
 */
@Override public synchronized void resolve(JingleSession session) throws XMPPException, NotConnectedException, InterruptedException {
  if (!isResolving()) {
    setResolveInit();
    clearCandidates();
    if (fixedCandidate.getLocalIp() == null)     fixedCandidate.setLocalIp(fixedCandidate.getIp());
    if (fixedCandidate != null) {
      addCandidate(fixedCandidate);
    }
    setResolveEnd();
  }
}
