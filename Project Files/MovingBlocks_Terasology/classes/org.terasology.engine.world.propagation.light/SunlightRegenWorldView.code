/** 
 * Gets the sunlight regen values from the chunk. <p> Simply delegates to the provided chunk for values
 */
public class SunlightRegenWorldView extends AbstractFullWorldView {
  public SunlightRegenWorldView(  ChunkProvider chunkProvider){
    super(chunkProvider);
  }
  @Override protected byte getValueAt(  Chunk chunk,  Vector3ic pos){
    return chunk.getSunlightRegen(pos);
  }
  @Override protected void setValueAt(  Chunk chunk,  Vector3ic pos,  byte value){
    chunk.setSunlightRegen(pos,value);
  }
}
