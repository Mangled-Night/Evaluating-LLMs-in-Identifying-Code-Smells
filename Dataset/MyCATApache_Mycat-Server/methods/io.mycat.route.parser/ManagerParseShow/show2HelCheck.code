static int show2HelCheck(String stmt,int offset){
  if (stmt.length() > offset + "P".length()) {
    char c1=stmt.charAt(++offset);
    if ((c1 == 'P' || c1 == 'p')) {
      if (stmt.length() > ++offset && stmt.charAt(offset) != ' ') {
        return OTHER;
      }
      return HELP;
    }
  }
  return OTHER;
}
