@ParameterizedTest @EnumSource(SmackTestUtil.XmlPullParserKind.class) public void testAttributesNamespacedElementWithOneAttribute(SmackTestUtil.XmlPullParserKind parserKind) throws XmlPullParserException, IOException {
  String namespacedElementWithOneAttribute="<element xmlns='element-namespace' attribute-one='attribute-one-value'/>";
  XmlPullParser parser=SmackTestUtil.getParserFor(namespacedElementWithOneAttribute,parserKind);
  assertAttributeHolds(parser,0,"attribute-one","","");
  assertThrows(NullPointerException.class,() -> assertAttributeHolds(parser,1,"attribute-one","",""));
}
