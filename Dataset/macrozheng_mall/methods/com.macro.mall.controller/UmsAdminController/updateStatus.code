@ApiOperation("修改帐号状态") @RequestMapping(value="/updateStatus/{id}",method=RequestMethod.POST) @ResponseBody public CommonResult updateStatus(@PathVariable Long id,@RequestParam(value="status") Integer status){
  UmsAdmin umsAdmin=new UmsAdmin();
  umsAdmin.setStatus(status);
  int count=adminService.update(id,umsAdmin);
  if (count > 0) {
    return CommonResult.success(count);
  }
  return CommonResult.failed();
}
