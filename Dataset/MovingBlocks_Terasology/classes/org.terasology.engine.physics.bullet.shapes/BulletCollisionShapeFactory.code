public class BulletCollisionShapeFactory implements CollisionShapeFactory {
  @Override public BoxShape getNewBox(  Vector3f extents){
    return new BulletBoxShape(extents.mul(0.5f));
  }
  @Override public ConvexHullShape getNewConvexHull(  List<Vector3f> vertices){
    return new BulletConvexHullShape(vertices);
  }
  @Override public CompoundShape getNewCompoundShape(){
    return new BulletCompoundShape();
  }
  @Override public SphereShape getNewSphere(  float radius){
    return new BulletSphereShape(radius);
  }
}
