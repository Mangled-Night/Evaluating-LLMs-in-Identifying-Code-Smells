@ApiOperation("批量上下架商品") @RequestMapping(value="/update/publishStatus",method=RequestMethod.POST) @ResponseBody public CommonResult updatePublishStatus(@RequestParam("ids") List<Long> ids,@RequestParam("publishStatus") Integer publishStatus){
  int count=productService.updatePublishStatus(ids,publishStatus);
  if (count > 0) {
    return CommonResult.success(count);
  }
 else {
    return CommonResult.failed();
  }
}
