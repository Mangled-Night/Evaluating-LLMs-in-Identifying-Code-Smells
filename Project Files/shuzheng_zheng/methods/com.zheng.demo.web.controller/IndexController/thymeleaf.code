/** 
 * thymeleaf视图
 * @param model
 * @return
 */
@RequestMapping(value="/thymeleaf",method=RequestMethod.GET) public String thymeleaf(Model model){
  model.addAttribute("host",demoService.sayHello("http://www.zhangshuzheng.cn/"));
  List<User> users=new ArrayList<>();
  User user=new User();
  user.setId(1L);
  user.setAge(11);
  user.setName("zhangsan");
  users.add(user);
  user=new User();
  user.setId(2L);
  user.setAge(22);
  user.setName("lisi");
  users.add(user);
  model.addAttribute("users",users);
  return thymeleaf("/index");
}
