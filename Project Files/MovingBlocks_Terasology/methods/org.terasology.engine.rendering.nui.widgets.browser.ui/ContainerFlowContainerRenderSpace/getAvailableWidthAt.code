private int getAvailableWidthAt(int y){
  int width=containerWidth;
  Rectanglei lastRight=findLastAtYPosition(rightFloats,y);
  if (lastRight != null) {
    width=lastRight.minX;
  }
  Rectanglei lastLeft=findLastAtYPosition(leftFloats,y);
  if (lastLeft != null) {
    width-=lastLeft.maxX;
  }
  return width;
}
