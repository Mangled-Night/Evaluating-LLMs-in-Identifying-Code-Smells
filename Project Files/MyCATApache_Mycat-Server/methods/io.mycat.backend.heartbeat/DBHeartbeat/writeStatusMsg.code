protected void writeStatusMsg(String dataHost,String dataSourceName,int nextstatus){
  if (status != nextstatus) {
    StringBuilder msg=new StringBuilder("");
    msg.append("[dataHost=").append(dataHost).append(", dataSource=").append(dataSourceName).append(",statue=").append(getMsg(status)).append(" -> ").append(getMsg(nextstatus)).append("]");
    LogUtil.writeDataSourceLog(msg.toString());
  }
}
