/** 
 * Default setup order
 * @param moduleNames a list of module names
 */
Environment(Name... moduleNames){
  try {
    reset(Sets.newHashSet(moduleNames));
  }
 catch (  Exception e) {
    if (e instanceof RuntimeException) {
      throw (RuntimeException)e;
    }
 else {
      throw new RuntimeException(e);
    }
  }
}
