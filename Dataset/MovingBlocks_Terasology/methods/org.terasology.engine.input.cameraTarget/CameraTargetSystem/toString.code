@Override public String toString(){
  if (targetBlockPos != null) {
    Vector3f pos=localPlayer.getViewPosition(new Vector3f());
    Vector3f dir=localPlayer.getViewDirection(new Vector3f());
    return String.format("From: %f %f %f, Dir: %f %f %f, Hit %d %d %d %f %f %f",pos.x,pos.y,pos.z,dir.x,dir.y,dir.z,targetBlockPos.x,targetBlockPos.y,targetBlockPos.z,hitPosition.x,hitPosition.y,hitPosition.z);
  }
  return "";
}
