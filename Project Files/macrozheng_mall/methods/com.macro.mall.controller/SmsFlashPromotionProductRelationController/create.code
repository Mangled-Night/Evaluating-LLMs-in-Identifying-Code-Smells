@ApiOperation("批量选择商品添加关联") @RequestMapping(value="/create",method=RequestMethod.POST) @ResponseBody public CommonResult create(@RequestBody List<SmsFlashPromotionProductRelation> relationList){
  int count=relationService.create(relationList);
  if (count > 0) {
    return CommonResult.success(count);
  }
  return CommonResult.failed();
}
