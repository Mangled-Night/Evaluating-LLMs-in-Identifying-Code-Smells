@RequiresPermissions("admin:ad:delete") @RequiresPermissionsDesc(menu={"推广管理","广告管理"},button="删除") @PostMapping("/delete") public Object delete(@RequestBody LitemallAd ad){
  Integer id=ad.getId();
  if (id == null) {
    return ResponseUtil.badArgument();
  }
  adService.deleteById(id);
  return ResponseUtil.ok();
}
