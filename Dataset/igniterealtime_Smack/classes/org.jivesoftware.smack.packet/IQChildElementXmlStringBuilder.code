public static class IQChildElementXmlStringBuilder extends XmlStringBuilder {
  private final String element;
  private boolean isEmptyElement;
  public IQChildElementXmlStringBuilder(  ExtensionElement extensionElement,  XmlEnvironment enclosingXmlEnvironment){
    this(extensionElement.getElementName(),extensionElement.getNamespace(),extensionElement.getLanguage(),enclosingXmlEnvironment);
  }
  private IQChildElementXmlStringBuilder(  String elementName,  String xmlNs,  String xmlLang,  XmlEnvironment enclosingXmlEnvironment){
    super(elementName,xmlNs,xmlLang,enclosingXmlEnvironment);
    this.element=elementName;
  }
  public void setEmptyElement(){
    isEmptyElement=true;
  }
}
