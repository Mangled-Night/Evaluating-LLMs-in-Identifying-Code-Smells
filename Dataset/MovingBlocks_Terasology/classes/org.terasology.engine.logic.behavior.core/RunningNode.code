/** 
 * Runs always.
 */
public class RunningNode extends LeafNode {
  @Override public String getName(){
    return "running";
  }
  @Override public BehaviorNode deepCopy(){
    return new RunningNode();
  }
  @Override public void construct(  Actor actor){
  }
  @Override public BehaviorState execute(  Actor actor){
    return BehaviorState.RUNNING;
  }
  @Override public void destruct(  Actor actor){
  }
}
