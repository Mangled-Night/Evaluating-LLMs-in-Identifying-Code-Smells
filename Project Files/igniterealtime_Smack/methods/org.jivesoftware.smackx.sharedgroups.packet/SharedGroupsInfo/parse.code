@Override public SharedGroupsInfo parse(XmlPullParser parser,int initialDepth,IqData iqData,XmlEnvironment xmlEnvironment) throws XmlPullParserException, IOException {
  SharedGroupsInfo groupsInfo=new SharedGroupsInfo();
  boolean done=false;
  while (!done) {
    XmlPullParser.Event eventType=parser.next();
    if (eventType == XmlPullParser.Event.START_ELEMENT && parser.getName().equals("group")) {
      groupsInfo.getGroups().add(parser.nextText());
    }
 else     if (eventType == XmlPullParser.Event.END_ELEMENT) {
      if (parser.getName().equals("sharedgroup")) {
        done=true;
      }
    }
  }
  return groupsInfo;
}
