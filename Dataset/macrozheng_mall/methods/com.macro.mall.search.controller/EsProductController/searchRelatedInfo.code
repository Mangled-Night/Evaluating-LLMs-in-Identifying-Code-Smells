@ApiOperation(value="获取搜索的相关品牌、分类及筛选属性") @RequestMapping(value="/search/relate",method=RequestMethod.GET) @ResponseBody public CommonResult<EsProductRelatedInfo> searchRelatedInfo(@RequestParam(required=false) String keyword){
  EsProductRelatedInfo productRelatedInfo=esProductService.searchRelatedInfo(keyword);
  return CommonResult.success(productRelatedInfo);
}
