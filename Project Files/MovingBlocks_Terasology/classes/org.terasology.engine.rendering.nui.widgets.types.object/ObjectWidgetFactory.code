public class ObjectWidgetFactory implements TypeWidgetFactory {
  @In private ModuleManager moduleManager;
  @In private TypeRegistry typeRegistry;
  @Override public <T>Optional<TypeWidgetBuilder<T>> create(  TypeInfo<T> type,  TypeWidgetLibrary library){
    Class<T> rawType=type.getRawType();
    if (rawType.isLocalClass() || Object.class.equals(rawType) || (rawType.isMemberClass() && !Modifier.isStatic(rawType.getModifiers()))) {
      return Optional.empty();
    }
    ObjectLayoutBuilder<T> layoutBuilder=new ObjectLayoutBuilder<>(type,library);
    return Optional.of(layoutBuilder);
  }
}
