public static AsyncProfiler getInstance(){
  return getInstance(null);
}
public static synchronized AsyncProfiler getInstance(String libPath){
  if (instance != null) {
    return instance;
  }
  if (libPath == null) {
    System.loadLibrary("asyncProfiler");
  }
 else {
    System.load(libPath);
  }
  instance=new AsyncProfiler();
  return instance;
}
