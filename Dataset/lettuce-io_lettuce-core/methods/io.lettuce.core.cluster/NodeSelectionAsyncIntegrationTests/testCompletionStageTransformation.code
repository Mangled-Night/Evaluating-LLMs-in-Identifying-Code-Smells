@Test void testCompletionStageTransformation(){
  CompletableFuture<String> transformed=commands.masters().commands().ping().thenApply(it -> String.join(" ",it.toArray(new String[0]))).toCompletableFuture();
  TestFutures.awaitOrTimeout(transformed);
  assertThat(transformed.join()).isEqualTo("PONG PONG");
}
